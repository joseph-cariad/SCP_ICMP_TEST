<?xml version="1.0"?>
<xform xmlns="http://www.tresos.de/_projects/tresos/09/widgets.xsd"
       xmlns:pp="http://www.tresos.de/_projects/tresos/09/widget_pp.xsd">
<!-- TODO Should this page be split into two different pages?  Suggestion: Put the groups Bsw & Rte Interface into its own page. -->
  <Page id="PAGE_BSWM_GENERAL" title="BswM General">
    <Group widgetId="GROUP_CONFIGURATION_CLASS_PARAMETERS" frame="TITLE" label="Configuration Variant">
      <Label text="Implementation Configuration Variant"/>
      <Combo widgetId="Combo_General_ImplementationConfigVariant" mementoId="General/IMPLEMENTATION_CONFIG_VARIANT">
        <range>VariantPreCompile</range>
        <range>VariantPostBuild</range>
      </Combo>
      <Label text="Max Number Rules"/>
      <Text widgetId="Text_General_MaxNumRules" mementoId="General/MAX_NUM_RULES" format="INTEGER"/>
      <Label text="Max Number Action Lists"/>
      <Text widgetId="Text_General_MaxNumActionLists" mementoId="General/MAX_NUM_ACTION_LISTS" format="INTEGER"/>
      <Label text="Enable Relocatable Post-build Configuration"/>
      <CheckBox widgetId="CheckBox_General_RelocatablePbcfgEnable" mementoId="General/RELOCATABLE_PBCFG_ENABLE"/>
      <Label text="Enable Verification of Binary Generation Constraints"/>
      <CheckBox widgetId="CheckBox_General_BinaryGenerationVerified" mementoId="General/BINARY_GENERATION_VERIFIED"/>
    </Group>
    <Group widgetId="GROUP_BSW_INTERFACE" frame="TITLE" label="Bsw Interface">
      <Label text="CanSM Enabled"/>
      <CheckBox widgetId="CheckBox_General_CanSmEnabled" mementoId="General/CANSM_ENABLED"/>
      <Label text="ComM Enabled"/>
      <CheckBox widgetId="CheckBox_General_ComMEnabled" mementoId="General/COMM_ENABLED"/>
      <Label text="Dcm Enabled"/>
      <CheckBox widgetId="CheckBox_General_DcmEnabled" mementoId="General/DCM_ENABLED"/>
      <Label text="Legacy Dcm Enabled"/>
      <CheckBox widgetId="CheckBox_General_LegacyDcmEnabled" mementoId="General/LEGACY_DCM_ENABLED"/>
      <Label text="EthIf Enabled"/>
      <CheckBox widgetId="CheckBox_General_EthIfEnabled" mementoId="General/ETHIF_ENABLED"/>
      <Label text="EthSM Enabled"/>
      <CheckBox widgetId="CheckBox_General_EthSmEnabled" mementoId="General/ETHSM_ENABLED"/>
      <Label text="EcuM Enabled"/>
      <CheckBox widgetId="CheckBox_General_EcuMEnabled" mementoId="General/ECUM_ENABLED"/>
      <Label text="FrSM Enabled"/>
      <CheckBox widgetId="CheckBox_General_FrSmEnabled" mementoId="General/FRSM_ENABLED"/>
      <Label text="LinSM Enabled"/>
      <CheckBox widgetId="CheckBox_General_LinSmEnabled" mementoId="General/LINSM_ENABLED"/>
      <Label text="LinTP Enabled"/>
      <CheckBox widgetId="CheckBox_General_LinTPEnabled" mementoId="General/LINTP_ENABLED"/>
      <Label text="NM Enabled"/>
      <CheckBox widgetId="CheckBox_General_NMEnabled" mementoId="General/NM_ENABLED"/>
      <Label text="NvM Enabled"/>
      <CheckBox widgetId="CheckBox_General_NvMEnabled" mementoId="General/NVM_ENABLED"/>
      <Label text="SD Enabled"/>
      <CheckBox widgetId="CheckBox_General_SdEnabled" mementoId="General/SD_ENABLED"/>
      <Label text="WdgM Enabled"/>
      <CheckBox widgetId="CheckBox_General_WdgMEnabled" mementoId="General/WDGM_ENABLED"/>
      <Label text="Generic Request Enabled"/>
      <CheckBox widgetId="CheckBox_General_GenericRequestEnabled" mementoId="General/GENERIC_REQUEST_ENABLED"/>
    </Group>
    <Group widgetId="GROUP_RTE_INTERFACE" frame="TITLE" label="Rte Interface" pp:features="EB">
      <Label text="Rte Enabled"/>
      <CheckBox widgetId="CheckBox_General_RteEnabled" mementoId="General/RTE_ENABLED"/>
      <Spacer horizontalSpan="2"/>
      <Label text="BswM Mainfunction Period"/>
      <Text widgetId="Text_MainFunctionPeriod" mementoId="General/MAIN_FUNCTION_PERIOD" format="FLOAT"/>
    </Group>
    <Group widgetId="GROUP_GENERAL_PARAMETERS" frame="TITLE" label="General Parameters">
      <Label text="Version Info Api Enabled"/>
      <CheckBox widgetId="CheckBox_General_VersionInfoApiEnabled" mementoId="General/VERSION_INFO_API_ENABLED"/>
      <Spacer horizontalSpan="2"/>
      <Label text="Developmental Error Detection Enabled"/>
      <CheckBox widgetId="CheckBox_General_DetEnabled" mementoId="General/DET_ENABLED"/>
      <Spacer horizontalSpan="2"/>
    </Group>
    <Group widgetId="GROUP_GENERAL_PARAMETERS" frame="TITLE" label="Defensive Programming">
      <Label text="Enable Defensive Programming"/>
      <CheckBox widgetId="CheckBox_General_EnableDefensiveProgramming" mementoId="General/ENABLE_DEFENSIVE_PROGRAMMING"/>
      <Label text="Enable Precondition Assertions"/>
      <CheckBox widgetId="CheckBox_General_EnablePreconditionAssertions" mementoId="General/ENABLE_PRECONDITION_ASSERTIONS"/>
      <Label text="Enable Postcondition Assertions"/>
      <CheckBox widgetId="CheckBox_General_EnablePostConditionAssertions" mementoId="General/ENABLE_POSTCONDITION_ASSERTIONS"/>
      <Label text="Enable Unreachable Code Assertions"/>
      <CheckBox widgetId="CheckBox_General_EnableUnreachableCodeAssertions" mementoId="General/ENABLE_UNREACHABLE_CODE_ASSERTIONS"/>
      <Label text="Enable Static Assertions"/>
      <CheckBox widgetId="CheckBox_General_EnableStaticAssertions" mementoId="General/ENABLE_STATIC_ASSERTIONS"/>
      <Label text="Enable Invariant Assertions"/>
      <CheckBox widgetId="CheckBox_General_EnableInvariantAssertions" mementoId="General/ENABLE_INVARIANT_ASSERTIONS"/>
    </Group>
  </Page>
</xform>
