<?xml version="1.0" encoding="UTF-8"?>
<dem:events xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.tresos.de/_project/AutoCore/dem.events_1.xsd"
    xmlns:dem="http://www.tresos.de/_project/AutoCore/dem.events_1.xsd">

    <event name="NVM_E_INTEGRITY_FAILED">
        <enable type="XPATH"
            value="as:modconf('NvM')[1]/ReportToDem/NvMIntegrityFailedReportToDem='DEM'" />
        <updateReference
            value="as:modconf('NvM')[1]/NvmDemEventParameterRefs/NVM_E_INTEGRITY_FAILED" />
        <diagEventDebounceAlgorithm value="DiagEventDebounceMonitorInternal" />

    </event>

    <event name="NVM_E_REQ_FAILED">
        <enable type="XPATH"
            value="as:modconf('NvM')[1]/ReportToDem/NvMRequestFailedReportToDem='DEM'" />
