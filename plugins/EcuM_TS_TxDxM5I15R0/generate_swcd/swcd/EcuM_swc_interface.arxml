                        <COMPU-CONST>
                          <VT>ECUM_STATE_STARTUP</VT>
                        </COMPU-CONST>
                      </COMPU-SCALE>
                      <COMPU-SCALE>
                        <LOWER-LIMIT INTERVAL-TYPE="CLOSED">17</LOWER-LIMIT>
                        <UPPER-LIMIT INTERVAL-TYPE="CLOSED">17</UPPER-LIMIT>
                        <COMPU-CONST>
                          <VT>ECUM_STATE_STARTUP_ONE</VT>
                        </COMPU-CONST>
                      </COMPU-SCALE>
                      <COMPU-SCALE>
                        <LOWER-LIMIT INTERVAL-TYPE="CLOSED">18</LOWER-LIMIT>
                        <UPPER-LIMIT INTERVAL-TYPE="CLOSED">18</UPPER-LIMIT>
                        <COMPU-CONST>
                          <VT>ECUM_STATE_STARTUP_TWO</VT>
                        </COMPU-CONST>
                      </COMPU-SCALE>
                      <COMPU-SCALE>
                        <LOWER-LIMIT INTERVAL-TYPE="CLOSED">32</LOWER-LIMIT>
                        <UPPER-LIMIT INTERVAL-TYPE="CLOSED">32</UPPER-LIMIT>
                        <COMPU-CONST>
                          <VT>ECUM_STATE_WAKEUP</VT>
                        </COMPU-CONST>
                      </COMPU-SCALE>
                      <COMPU-SCALE>
                        <LOWER-LIMIT INTERVAL-TYPE="CLOSED">33</LOWER-LIMIT>
                        <UPPER-LIMIT INTERVAL-TYPE="CLOSED">33</UPPER-LIMIT>
                        <COMPU-CONST>
                          <VT>ECUM_STATE_WAKEUP_ONE</VT>
                        </COMPU-CONST>
                      </COMPU-SCALE>
                      <COMPU-SCALE>
                        <LOWER-LIMIT INTERVAL-TYPE="CLOSED">34</LOWER-LIMIT>
                        <UPPER-LIMIT INTERVAL-TYPE="CLOSED">34</UPPER-LIMIT>
                        <COMPU-CONST>
                          <VT>ECUM_STATE_WAKEUP_VALIDATION</VT>
                        </COMPU-CONST>
                      </COMPU-SCALE>
                      <COMPU-SCALE>
                        <LOWER-LIMIT INTERVAL-TYPE="CLOSED">35</LOWER-LIMIT>
                        <UPPER-LIMIT INTERVAL-TYPE="CLOSED">35</UPPER-LIMIT>
                        <COMPU-CONST>
                          <VT>ECUM_STATE_WAKEUP_REACTION</VT>
                        </COMPU-CONST>
                      </COMPU-SCALE>
                      <COMPU-SCALE>
                        <LOWER-LIMIT INTERVAL-TYPE="CLOSED">36</LOWER-LIMIT>
                        <UPPER-LIMIT INTERVAL-TYPE="CLOSED">36</UPPER-LIMIT>
                        <COMPU-CONST>
                          <VT>ECUM_STATE_WAKEUP_TWO</VT>
                        </COMPU-CONST>
                      </COMPU-SCALE>
                      <COMPU-SCALE>
                        <LOWER-LIMIT INTERVAL-TYPE="CLOSED">37</LOWER-LIMIT>
                        <UPPER-LIMIT INTERVAL-TYPE="CLOSED">37</UPPER-LIMIT>
                        <COMPU-CONST>
                          <VT>ECUM_STATE_WAKEUP_WAKESLEEP</VT>
                        </COMPU-CONST>
                      </COMPU-SCALE>
                      <COMPU-SCALE>
                        <LOWER-LIMIT INTERVAL-TYPE="CLOSED">38</LOWER-LIMIT>
                        <UPPER-LIMIT INTERVAL-TYPE="CLOSED">38</UPPER-LIMIT>
                        <COMPU-CONST>
                          <VT>ECUM_STATE_WAKEUP_TTII</VT>
                        </COMPU-CONST>
                      </COMPU-SCALE>
                      <COMPU-SCALE>
                        <LOWER-LIMIT INTERVAL-TYPE="CLOSED">48</LOWER-LIMIT>
                        <UPPER-LIMIT INTERVAL-TYPE="CLOSED">48</UPPER-LIMIT>
                        <COMPU-CONST>
                          <VT>ECUM_STATE_RUN</VT>
                        </COMPU-CONST>
                      </COMPU-SCALE>
                      <COMPU-SCALE>
                        <LOWER-LIMIT INTERVAL-TYPE="CLOSED">50</LOWER-LIMIT>
                        <UPPER-LIMIT INTERVAL-TYPE="CLOSED">50</UPPER-LIMIT>
                        <COMPU-CONST>
                          <VT>ECUM_STATE_APP_RUN</VT>
                        </COMPU-CONST>
                      </COMPU-SCALE>
                      <COMPU-SCALE>
                        <LOWER-LIMIT INTERVAL-TYPE="CLOSED">51</LOWER-LIMIT>
                        <UPPER-LIMIT INTERVAL-TYPE="CLOSED">51</UPPER-LIMIT>
                        <COMPU-CONST>
                          <VT>ECUM_STATE_APP_POST_RUN</VT>
                        </COMPU-CONST>
                      </COMPU-SCALE>
                      <COMPU-SCALE>
                        <LOWER-LIMIT INTERVAL-TYPE="CLOSED">64</LOWER-LIMIT>
                        <UPPER-LIMIT INTERVAL-TYPE="CLOSED">64</UPPER-LIMIT>
                        <COMPU-CONST>
                          <VT>ECUM_STATE_SHUTDOWN</VT>
                        </COMPU-CONST>
                      </COMPU-SCALE>
                      <COMPU-SCALE>
                        <LOWER-LIMIT INTERVAL-TYPE="CLOSED">68</LOWER-LIMIT>
                        <UPPER-LIMIT INTERVAL-TYPE="CLOSED">68</UPPER-LIMIT>
                        <COMPU-CONST>
                          <VT>ECUM_STATE_PREP_SHUTDOWN</VT>
                        </COMPU-CONST>
                      </COMPU-SCALE>
                      <COMPU-SCALE>
                        <LOWER-LIMIT INTERVAL-TYPE="CLOSED">73</LOWER-LIMIT>
                        <UPPER-LIMIT INTERVAL-TYPE="CLOSED">73</UPPER-LIMIT>
                        <COMPU-CONST>
                          <VT>ECUM_STATE_GO_SLEEP</VT>
                        </COMPU-CONST>
                      </COMPU-SCALE>
                      <COMPU-SCALE>
                        <LOWER-LIMIT INTERVAL-TYPE="CLOSED">77</LOWER-LIMIT>
                        <UPPER-LIMIT INTERVAL-TYPE="CLOSED">77</UPPER-LIMIT>
                        <COMPU-CONST>
                          <VT>ECUM_STATE_GO_OFF_ONE</VT>
                        </COMPU-CONST>
                      </COMPU-SCALE>
                      <COMPU-SCALE>
                        <LOWER-LIMIT INTERVAL-TYPE="CLOSED">78</LOWER-LIMIT>
                        <UPPER-LIMIT INTERVAL-TYPE="CLOSED">78</UPPER-LIMIT>
                        <COMPU-CONST>
                          <VT>ECUM_STATE_GO_OFF_TWO</VT>
                        </COMPU-CONST>
                      </COMPU-SCALE>
                      <COMPU-SCALE>
                        <LOWER-LIMIT INTERVAL-TYPE="CLOSED">80</LOWER-LIMIT>
                        <UPPER-LIMIT INTERVAL-TYPE="CLOSED">80</UPPER-LIMIT>
                        <COMPU-CONST>
                          <VT>ECUM_STATE_SLEEP</VT>
                        </COMPU-CONST>
                      </COMPU-SCALE>
                      <COMPU-SCALE>
                        <LOWER-LIMIT INTERVAL-TYPE="CLOSED">128</LOWER-LIMIT>
                        <UPPER-LIMIT INTERVAL-TYPE="CLOSED">128</UPPER-LIMIT>
                        <COMPU-CONST>
                          <VT>ECUM_STATE_OFF</VT>
                        </COMPU-CONST>
                      </COMPU-SCALE>
                      <COMPU-SCALE>
                        <LOWER-LIMIT INTERVAL-TYPE="CLOSED">144</LOWER-LIMIT>
                        <UPPER-LIMIT INTERVAL-TYPE="CLOSED">144</UPPER-LIMIT>
                        <COMPU-CONST>
                          <VT>ECUM_STATE_RESET</VT>
