                      <SHORT-NAME>E_NOT_OK</SHORT-NAME>
                      <ERROR-CODE>1</ERROR-CODE>
                    </APPLICATION-ERROR>
                  </POSSIBLE-ERRORS>
                </CLIENT-SERVER-INTERFACE>

                <SENDER-RECEIVER-INTERFACE>
                  <SHORT-NAME>EcuM_ASR32_CurrentMode</SHORT-NAME>
                  <IS-SERVICE>true</IS-SERVICE>
                  <DATA-ELEMENTS />
                  <MODE-GROUPS>
                    <MODE-DECLARATION-GROUP-PROTOTYPE>
                      <SHORT-NAME>currentMode</SHORT-NAME>
                      <TYPE-TREF DEST="MODE-DECLARATION-GROUP">/AUTOSAR_EcuM/ASR32/ModeDeclarationGroups/EcuM_ASR32_Mode</TYPE-TREF>
                    </MODE-DECLARATION-GROUP-PROTOTYPE>
                  </MODE-GROUPS>
                </SENDER-RECEIVER-INTERFACE>
[!ENDIF!][!//
                <CLIENT-SERVER-INTERFACE>
                  <SHORT-NAME>EcuM_ASR32_ShutdownTarget</SHORT-NAME>
