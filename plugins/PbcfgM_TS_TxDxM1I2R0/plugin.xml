        <manager class="dreisoft.tresos.autosar2.resourcehandling.AutosarConfigManager"/>
        <schemaNode path="ASPath:/TS_TxDxM1I2R0/PbcfgM"/>
      </data>

      <editor id="PbcfgM_TS_TxDxM1I2R0_EditorId"
              label="Default"
              tooltip="ACG-Base: EB PbcfgM module">
