    <extension point="dreisoft.tresos.launcher2.plugin.module"
             id="FrTSyn_TS_TxDxM2I0R0_ModuleId"
             name="FrTSyn_TS_TxDxM2I0R0 Module">
    <module id="FrTSyn_TS_TxDxM2I0R0"
            label="FrTSyn"
            mandatory="false"
            allowMultiple="false"
            description="ACG-TimeSync: AUTOSAR FrTSyn module"
            copyright="(c) 2005-2021 Elektrobit Automotive GmbH"
            swVersionMajor="2"
            swVersionMinor="0"
            swVersionPatch="9"
            swVersionSuffix="B439717"
            specVersionMajor="4"
            specVersionMinor="4"
            specVersionPatch="0"
            specVersionSuffix="0000"
            relVersionMajor="4"
            relVersionMinor="4"
            relVersionPatch="0"
            categoryType="FrTSyn"
            categoryLayer="Communication Services"
            categoryCategory="FlexRay"
            categoryComponent="ECUC">
      <cluster name="FlexRay Stack"/>
<cluster name="Time Sync"/>
    </module>
  </extension>


  
  <extension point="dreisoft.tresos.launcher2.plugin.configuration"
             id="FrTSyn_TS_TxDxM2I0R0_ConfigId"
             name="FrTSyn_TS_TxDxM2I0R0 Configuration">
    <configuration moduleId="FrTSyn_TS_TxDxM2I0R0">
      <schema>
        <manager class="dreisoft.tresos.autosar2.resourcehandling.AutosarSchemaManager"/>
        <resource value="config/FrTSyn.xdm" type="xdm" id="res_default"/>
      </schema>

      <data>
        <manager class="dreisoft.tresos.autosar2.resourcehandling.AutosarConfigManager"/>
        <schemaNode path="ASPath:/TS_TxDxM2I0R0/FrTSyn"/>
      </data>

      <editor id="FrTSyn_TS_TxDxM2I0R0_EditorId"
              label="Default"
              tooltip="ACG-TimeSync: AUTOSAR FrTSyn module">
        <class class="dreisoft.tresos.launcher2.editor.GenericConfigEditor">
          <parameter name="schema" value="ASPath:/TS_TxDxM2I0R0/FrTSyn"/>
          <parameter name="title" value="FrTSyn"/>
          <parameter name="noTabs" value="false"/>
          <parameter name="noLinks" value="true"/>
          <parameter name="groupLinks" value="false"/>
          <parameter name="groupContainers" value="false"/>
          <parameter name="groupTables" value="true"/>
          <parameter name="optionalGeneralTab" value="true"/>
        </class>
      </editor>
    </configuration>
  </extension>



  
      <extension point="dreisoft.tresos.comimporter.api.plugin.comtransformer">
    <comtransformer
           id="ComTransformer_FrTSyn_TS_TxDxM2I0R0"
           moduleId="FrTSyn_TS_TxDxM2I0R0">
      <transformer
             class="dreisoft.tresos.comimporter.api.transformer.asr40.FrTSynTransformer">
      </transformer>
    </comtransformer>
  </extension>


  
  
      <extension point="dreisoft.tresos.launcher2.plugin.generator"
             id="FrTSyn_TS_TxDxM2I0R0_GeneratorId"
             name="FrTSyn_TS_TxDxM2I0R0 Generator">
    <generator class="dreisoft.tresos.launcher2.generator.TemplateBasedCodeGenerator"
               id="FrTSyn_TS_TxDxM2I0R0_TemplateBasedCodeGeneratorId"
               moduleId="FrTSyn_TS_TxDxM2I0R0">
      <!-- common template path parameters -->
      <parameter name="templates"
                 mode="generate,verify" value="generate"/>

      <!-- swcd modes and template path parameters -->
      <parameter name="mode_type"
                 mode="generate_swcd" value="generate"/>
      <parameter name="mode_type"
                 mode="verify_swcd" value="verify"/>
      <parameter name="templates"
                 mode="generate_swcd,verify_swcd" value="generate_swcd"/>
    </generator>
  </extension>

  
  
      <extension point="dreisoft.tresos.guidedconfig.api.plugin.pushservice">
    <pushoperation
        id="SvcAsReq_FrTSyn_TS_TxDxM2I0R0"
        name="FrTSyn service needs requests">
      <operationclass plugin="SvcAsLib" class="eb.tresos.svclib.api.pushservice.SvcRequestOperation">
        <parameter name="module" value="FrTSyn" />
        <parameter name="req://schm.mainFunctions:1" value="resources/FrTSyn_SchM_MainFunctions.xml" />
        <parameter name="req://schm.exclusiveAreas:1" value="resources/FrTSyn_SchM_ExclusiveAreas.xml" />
      </operationclass>
      <event>
        <and>
          <with variable="class">
            <equals value="eb.tresos.svclib.api.event.SvcRequestsEvent" />
          </with>
          <with variable="svc.type">
            <equals value="request" />
          </with>
          <with variable="ecuConfig.moduleId.FrTSyn_TS_TxDxM2I0R0">
            <equals value="true" />
          </with>
          <or>
            <with variable="ecuConfig.relVersion">
              <equals value="4.4" />
            </with>
            <with variable="ecuConfig.relVersion">
              <equals value="0.0" />
            </with>
          </or>
        </and>
      </event>
    </pushoperation>
  </extension>

  
  
      <extension point="dreisoft.tresos.autosar2.api.plugin.ecuresource">
    <ecuresource
      target="*"
      derivate="*"
      moduleId="FrTSyn_TS_TxDxM2I0R0"
      file="resources/AdjacentLayer.properties">
    </ecuresource>
  </extension>

  

    <extension
        point="dreisoft.tresos.guidedconfig.api.plugin.pushservice">
        <pushoperation id="HandleIDs_FrTSyn_TS_TxDxM2I0R0"
            name="FrTSyn/FrTSynHandleIDs"
            desc="Calculates FrTSynGlobalTimeSlaveHandleId and FrTSynGlobalTimeMasterHandleId configuration parameters">
            <operationclass class="dreisoft.tresos.frtsyn.pushservice.operations.handleid.FrTSynHandleIdPushOperation">
                <parameter name="moduleId" value="FrTSyn_TS_TxDxM2I0R0"/>
                <parameter name="modulePath" value="/AUTOSAR/TOP-LEVEL-PACKAGES/TS_TxDxM2I0R0/ELEMENTS/FrTSyn"/>
            </operationclass>
            <event>
                <and>
                    <with variable="class">
                        <equals value="dreisoft.tresos.guidedconfig.api.pushservice.operations.handleid.HandleIdEvent"/>
                    </with>
                    <with variable="moduleId.FrTSyn_TS_TxDxM2I0R0">
                        <equals value="true"/>
                    </with>
                    <with variable="relVersion">
                        <equals value="4.4"/>
                    </with>
                </and>
            </event>
        </pushoperation>
    </extension>

</plugin>
