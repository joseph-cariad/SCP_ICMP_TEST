<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

  <!--
    ** \brief AUTOSAR IpduM_TS_TxDxM3I3R0 plugin
    ** \project AUTOSAR Standard Core
    ** \file plugin.xdm
