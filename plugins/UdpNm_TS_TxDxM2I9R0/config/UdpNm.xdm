                    <a:v>EN:
                      &lt;html&gt;
                        Minor version number of AUTOSAR specification on which the appropriate implementation is based on.
                    &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       value="PublishedInformation"/>
                  <a:a name="LABEL" value="AUTOSAR Minor Version"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" 
                       value="ECUC:2893b920-59d5-4ac2-b2c1-e23742e66d70"/>
                  <a:da name="DEFAULT" value="3"/>
                </v:var>
                <v:var name="ArPatchVersion" type="INTEGER_LABEL">
                  <a:a name="DESC">
                    <a:v>EN:
                      &lt;html&gt;
                        Patch level version number of AUTOSAR specification on which the appropriate implementation is based on.
                    &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       value="PublishedInformation"/>
                  <a:a name="LABEL" value="AUTOSAR Patch Version"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" 
                       value="ECUC:6428eb9b-8790-488a-b9a3-0fba52d0f59e"/>
                  <a:da name="DEFAULT" value="0"/>
                </v:var>
                <v:var name="SwMajorVersion" type="INTEGER_LABEL">
                  <a:a name="DESC">
                    <a:v>EN:
                      &lt;html&gt;
                        Major version number of the vendor specific implementation of the module.
                    &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       value="PublishedInformation"/>
                  <a:a name="LABEL" value="Software Major Version"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" 
                       value="ECUC:605b18ae-3f9a-41d4-9225-67c9c5f6fc34"/>
                  <a:da name="DEFAULT" value="2"/>
                </v:var>
                <v:var name="SwMinorVersion" type="INTEGER_LABEL">
                  <a:a name="DESC">
                    <a:v>EN:
                      &lt;html&gt;
                        Minor version number of the vendor specific implementation of the module. The numbering is vendor specific.
                    &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       value="PublishedInformation"/>
                  <a:a name="LABEL" value="Software Minor Version"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="a7fe44dc-ea25-4b8d-8fad-9fbcae86d56f"/>
                  <a:da name="DEFAULT" value="9"/>
                </v:var>
                <v:var name="SwPatchVersion" type="INTEGER_LABEL">
                  <a:a name="DESC">
                    <a:v>EN:
                      &lt;html&gt;
                        Patch level version number of the vendor specific implementation of the module. The numbering is vendor specific.
                    &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       value="PublishedInformation"/>
                  <a:a name="LABEL" value="Software Patch Version"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="a318d2d9-0e75-49da-ac43-e7e4e682e2f9"/>
                  <a:da name="DEFAULT" value="7"/>
                </v:var>
                <v:var name="ModuleId" type="INTEGER_LABEL">
                  <a:a name="DESC">
                    <a:v>EN:
                      &lt;html&gt;
                        Module ID of this module from Module List
                    &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       value="PublishedInformation"/>
                  <a:a name="LABEL" value="Numeric Module ID"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" 
                       value="ECUC:78bc8362-080f-4253-b3da-804ab69a7154"/>
                  <a:da name="DEFAULT" value="33"/>
                </v:var>
                <v:var name="VendorId" type="INTEGER_LABEL">
                  <a:a name="DESC">
                    <a:v>EN:
                      &lt;html&gt;
                        Vendor ID of the dedicated implementation of this module according to the AUTOSAR vendor list
                    &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       value="PublishedInformation"/>
                  <a:a name="LABEL" value="Vendor ID"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" 
                       value="ECUC:01b0f467-6943-4558-b4f2-3fa1fad28449"/>
                  <a:da name="DEFAULT" value="1"/>
                </v:var>
                <v:var name="Release" type="STRING_LABEL">
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       value="PublishedInformation"/>
                  <a:a name="LABEL" value="Release Information"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" 
                       value="ECUC:1c68a547-f24e-4a4e-9540-69fbd533ec89"/>
                  <a:da name="DEFAULT" value=""/>
                </v:var>
              </v:ctr>
              <v:ctr name="PublishedInformation" type="IDENTIFIABLE">
                <a:a name="DESC">
                  <a:v>EN:
                    &lt;html&gt;
                      Additional published parameters not covered by CommonPublishedInformation container.
                  &lt;/html&gt;</a:v>
                </a:a>
                <a:a name="LABEL" value="EB Published Information"/>
                <a:a name="TAB" value="EB Published Information"/>
                <a:a name="UUID" 
                     value="ECUC:cb4128ed-26c1-4ddd-906e-854bed917e07"/>
                <v:var name="PbcfgMSupport" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>EN:
                      &lt;html&gt;
                        Specifies whether or not the UdpNm can use the PbcfgM module for post-build support.
                    &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       value="PublishedInformation"/>
                  <a:a name="LABEL" value="PbcfgM support"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" 
                       value="ECUC:eb61002e-69c0-4f16-a910-c27adb75f6ee"/>
                  <a:da name="DEFAULT" value="true"/>
                </v:var>
              </v:ctr>
              <v:var name="IMPLEMENTATION_CONFIG_VARIANT" type="ENUMERATION">
                <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                     type="IMPLEMENTATIONCONFIGCLASS">
                  <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                </a:a>
                <a:a name="LABEL" value="Config Variant"/>
                <a:a name="UUID" value="12a1df4e-8806-4ebb-8a93-868e39c247a3"/>
                <a:da name="DEFAULT" value="VariantPostBuild"/>
                <a:da name="EDITABLE" value="false"/>
                <a:da name="RANGE" value="VariantPostBuild"/>
              </v:var>
              <v:ctr name="UdpNmGeneral" type="IDENTIFIABLE">
                <a:a name="UUID" value="1d9330c1-44c9-434b-81c2-dbf1e8696e67"/>
                <v:var name="UdpNmMultiCoreSupport" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                            &lt;p&gt;Enables MultiCoreSupport.&lt;/p&gt;
                          &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="LABEL" value="UdpNm multicore support"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="e243f6f6-0552-4753-a82b-66053eae4c47"/>
                  <a:da name="DEFAULT" value="false"/>
                </v:var>
                <v:var name="UdpNmPnSupported" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                          &lt;p&gt;Enables or disables support of partial networking.&lt;/p&gt;
                          &lt;ul&gt;
                            &lt;li&gt;&lt;code&gt;False&lt;/code&gt;: Partial Networking is disabled &lt;/li&gt;
                            &lt;li&gt;&lt;code&gt;True&lt;/code&gt;: Partial Networking is enabled&lt;/li&gt;
                          &lt;/ul&gt;
                        &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="LABEL" 
                       value="Support for Partial Network Cluster (PNC)"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="b9cadd6b-e796-46b9-8579-a46cb5590e0e"/>
                  <a:da name="DEFAULT" value="false"/>
                  <a:da name="INVALID" type="XPath">
                    <a:tst 
                           expr="(. = &apos;true&apos;) and (not(node:exists(as:modconf(&apos;UdpNm&apos;)[1]/UdpNmGlobalConfig/*[1]/UdpNmPnInfo)))" 
                           true="If Pnc is enabled UdpNmPnInfo shall also be enabled"/>
                    <a:tst 
                           expr="(. = &apos;true&apos;) and (not(node:exists(as:modconf(&apos;UdpNm&apos;)[1]/UdpNmGlobalConfig/*[1]/UdpNmPnResetTime)))" 
                           true="If Pnc is enabled UdpNmPnResetTime shall also be enabled"/>
                  </a:da>
                </v:var>
                <v:var name="UdpNmASR412APICompatibility" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                          &lt;p&gt;UdpNm_SoAdIfRxIndication compatibility with ASR 4.1.2.&lt;/p&gt;
                          &lt;ul&gt;
                            &lt;li&gt;&lt;code&gt;False&lt;/code&gt;: UdpNm_SoAdIfRxIndication ASR 4.1.3 compatibility &lt;/li&gt;
                            &lt;li&gt;&lt;code&gt;True&lt;/code&gt;: UdpNm_SoAdIfRxIndication ASR 4.1.2 compatibility&lt;/li&gt;
                          &lt;/ul&gt;
                        &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="LABEL" 
                       value="UdpNm_SoAdIfRxIndication ASR412 Compatibility"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="41d9b8a1-3cb5-4fd0-91b7-0707c2f68928"/>
                  <a:da name="DEFAULT" value="false"/>
                </v:var>
                <v:var name="UdpNmRelocatablePbcfgEnable" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                         Enables/disables support for relocatable postbuild configuration.
                         &lt;ul&gt;
                           &lt;li&gt;True: Postbuild configuration relocatable in memory.&lt;/li&gt;
                           &lt;li&gt;False: Postbuild configuration not relocatable in memory.&lt;/li&gt;
                         &lt;/ul&gt;
                       &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="LABEL" value="UdpNmRelocatablePbcfgEnable"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="7739400a-b743-4ab4-b8c1-3adbbaea7397"/>
                  <a:da name="DEFAULT" value="true"/>
                  <a:da name="EDITABLE" type="XPath" 
                        expr="(node:contains(node:refs(as:modconf(&apos;PbcfgM&apos;)/PbcfgMBswModules/*/PbcfgMBswModuleRef), as:modconf(&apos;UdpNm&apos;)) = &apos;true&apos;)" 
                        true="false" false="true"/>
                </v:var>
                <v:var name="UdpNmMaxPn" type="INTEGER">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                        &lt;p&gt;The maximum number of Partial Networking Clusters that can be configured.&lt;/p&gt;
                    &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="LABEL" value="UdpNmMaxPn"/>
                  <a:a name="OPTIONAL" value="true"/>
                  <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="0b0c91d4-bda9-4f32-ac60-b0c42d01b533"/>
                  <a:da name="DEFAULT" value="0"/>
                  <a:da name="ENABLE" value="false"/>
                  <a:da name="INVALID" type="Multi">
                    <mt:range>
                      <mt:tst expr="&lt;=56"/>
                      <mt:tst expr="&gt;=0"/>
                    </mt:range>
                    <mt:xpath 
                              true="UdpNmMaxPn cannot be larger than UdpNmPnInfoLength * 8!" 
                              expr="(not(node:exists(../../UdpNmGlobalConfig/*[1]/UdpNmPnInfo)) or                                     (. &gt; (../../UdpNmGlobalConfig/*[1]/UdpNmPnInfo/UdpNmPnInfoLength * 8))) and                                      (as:modconf(&apos;UdpNm&apos;)[1]/UdpNmGeneral/UdpNmPnSupported = &apos;true&apos;)"/>
                  </a:da>
                </v:var>
              </v:ctr>
              <v:lst name="UdpNmGlobalConfig" 
                     type="MULTIPLE-CONFIGURATION-CONTAINER">
                <a:da name="MAX" value="1"/>
                <a:da name="MIN" value="1"/>
                <v:ctr name="UdpNmGlobalConfig" 
                       type="MULTIPLE-CONFIGURATION-CONTAINER">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                          &lt;p&gt;This container contains all global configuration parameters of UDP NM configured
                            from the NM Module perspective.&lt;/p&gt;
                      &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="NAME_PATTERN" value="UdpNm_Config_?"/>
                  <a:a name="UUID" value="2708ddd1-363a-4563-8270-b91821d701ee"/>
                  <v:var name="UdpNmActiveWakeupBitEnabled" type="BOOLEAN">
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                          &lt;p&gt;
                            Enables/Disables the handling of the Active Wakeup Bit in the UdpNm module.
                          &lt;/p&gt;
                        &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="Active Wakeup Bit Enable"/>
                    <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:a name="UUID" 
                         value="e2f67acc-50e2-49de-af0f-78e9874bcdb0"/>
                    <a:da name="DEFAULT" value="false"/>
                    <a:da name="INVALID" type="XPath" 
                          expr="(. = &apos;true&apos;) and (../UdpNmPassiveModeEnabled = &apos;true&apos;)" 
                          true="ActiveWakeupBit is enabled for a channel in passive mode!"/>
                  </v:var>
                  <v:var name="UdpNmBusSynchronizationEnabled" type="BOOLEAN">
                    <a:a name="CALCULATION-FORMULA" 
                         value="if (udpnmpassivemodeenabled == false) then equal(nmbussynchronizationenabled) else equal(false)"/>
                    <a:a name="CALCULATION-LANGUAGE" value="INFORMAL"/>
                    <a:a name="DERIVED" value="TRUE"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                          &lt;p&gt;
                            Pre-processor switch for enabling bus synchronization support. This
                            feature is required for gateway nodes only. It must not be defined if
                            UDPNM_PASSIVE_MODE_ENABLED is defined. This parameter shall
                            be derived from NM_BUS_SYNCHRONIZATION_ENABLED.
                          &lt;/p&gt;
                          &lt;p&gt;The following API function is provided:&lt;/p&gt;
                          &lt;ul&gt;
                            &lt;li&gt;&lt;code&gt;UdpNm_RequestBusSynchronization()&lt;/code&gt;&lt;/li&gt;
                          &lt;/ul&gt;
                          &lt;p&gt;Dependencies:&lt;/p&gt;
                          &lt;ul&gt;
                            &lt;li&gt;The value of this parameter has to be synchronized
                                with the value of respective parameter in the Nm module.&lt;/li&gt;
                            &lt;li&gt;Passive Mode must be disabled.&lt;/li&gt;
                          &lt;/ul&gt;
                        &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="Bus Synchronization"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:a name="UUID" 
                         value="16a49f36-4fc6-407c-92ad-0b6a0c9313a4"/>
                    <a:da name="DEFAULT" value="false"/>
                    <a:da name="INVALID" type="XPath">
                      <a:tst 
                             expr="(. = &apos;true&apos;) and (../UdpNmPassiveModeEnabled = &apos;true&apos;)" 
                             true="Bus Synchronization cannot be enabled if Passive Mode is enabled."/>
                      <a:tst 
                             expr="not(node:exists(as:modconf(&apos;Nm&apos;)[1]/NmGlobalConfig/NmGlobalFeatures/NmBusSynchronizationEnabled)) or                                    (. != as:modconf(&apos;Nm&apos;)[1]/NmGlobalConfig/NmGlobalFeatures/NmBusSynchronizationEnabled)" 
                             true="UdpNm Bus Synchronization has to have same setting as Nm Bus Synchronization Support."/>
                    </a:da>
                  </v:var>
                  <v:var name="UdpNmComControlEnabled" type="BOOLEAN">
                    <a:a name="CALCULATION-FORMULA" 
                         value="equal(nmcomcontrol_enabled)"/>
                    <a:a name="CALCULATION-LANGUAGE" value="INFORMAL"/>
                    <a:a name="DERIVED" value="TRUE"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                          &lt;p&gt;Pre-processor switch for enabling the Communication Control
                             support. This parameter shall be derived from NM_COM_CONTROL_ENABLED.&lt;/p&gt;
                          &lt;p&gt;Dependencies:&lt;/p&gt;
                          &lt;ul&gt;
                            &lt;li&gt;The value of this parameter has to be synchronized
                                with the value of respective parameter in the Nm module.&lt;/li&gt;
                            &lt;li&gt;Passive Mode must be disabled.&lt;/li&gt;
                          &lt;/ul&gt;
                        &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="Communication Control"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:a name="UUID" 
                         value="d2328fd1-9795-4f38-9e76-5bb39d4b9b68"/>
                    <a:da name="DEFAULT" value="false"/>
                    <a:da name="INVALID" type="XPath" 
                          expr="not(node:exists(as:modconf(&apos;Nm&apos;)[1]/NmGlobalConfig/NmGlobalFeatures/NmComControlEnabled)) or                                    (. != as:modconf(&apos;Nm&apos;)[1]/NmGlobalConfig/NmGlobalFeatures/NmComControlEnabled)" 
                          true="UdpNm Communication Control has to use same setting as Nm Communication Control."/>
                    <a:da name="WARNING" type="XPath" 
                          expr="(. = &apos;true&apos;) and (../UdpNmPassiveModeEnabled = &apos;true&apos;)" 
                          true="Passive Mode shall be disabled in order to use the Communication control support."/>
                  </v:var>
                  <v:var name="UdpNmNodeIdCallback" type="FUNCTION-NAME">
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;
                               Name of the callback function to be called if UdpNmNodeIdCallback is enabled.
                            &lt;/p&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v class="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="UdpNm Node Id callback"/>
                    <a:a name="OPTIONAL" value="true"/>
                    <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:a name="UUID" 
                         value="08f604c0-a22e-4d7a-831a-b628f47e4512"/>
                    <a:da name="ENABLE" value="false"/>
                    <a:da name="INVALID" type="XPath" 
                          expr="(. != &apos;&apos;) and text:match(., &apos;^[a-zA-Z][a-zA-Z0-9_]*$&apos;)" 
                          false="Missing or invalid C function name."/>
                  </v:var>
                  <v:var name="UdpNmNodeIdCallbackHeader" type="STRING">
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;
                               The name of a header file that will be included to
                               obtain the external declaration of the callback function.
                            &lt;/p&gt;
                            &lt;p&gt;Dependencies:&lt;/p&gt;
                            &lt;ul&gt;
                              &lt;li&gt;This parameter is only available if UdpNmNodeIdCallback is enabled.&lt;/li&gt;
                            &lt;/ul&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="Node Id callback header"/>
                    <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:a name="UUID" 
                         value="ce0e65ca-820f-4d65-be86-74ca218a1077"/>
                    <a:da name="EDITABLE" type="XPath" 
                          expr="node:exists(../UdpNmNodeIdCallback)"/>
                    <a:da name="INVALID" type="Multi">
                      <mt:xpath>
                        <mt:tst true="Missing C header file." 
                                expr="node:exists(../UdpNmNodeIdCallback) and (. = &apos;&apos;)"/>
                        <mt:tst 
                                true="Invalid naming for C header file. Please use a standard .h file name." 
                                expr="node:exists(../UdpNmNodeIdCallback) and not(text:match(., &apos;^[\w\-\. ]+\.h$&apos;)) and (. != &apos;&apos;)"/>
                      </mt:xpath>
                    </a:da>
                  </v:var>
                  <v:var name="UdpNmComUserDataSupport" type="BOOLEAN">
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                          &lt;p&gt;Enable/disable the user data support.&lt;/p&gt;
                        &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:a name="UUID" 
                         value="8fd3d191-1f34-4e40-b142-e2d9c99bac0c"/>
                    <a:da name="DEFAULT" value="false"/>
                  </v:var>
                  <v:var name="UdpNmCoordinatorSyncSupport" type="BOOLEAN">
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                          &lt;p&gt;
                            &lt;em&gt;
                              The functionality related to this parameter
                              is not supported by the current implementation.
                            &lt;/em&gt;
                          &lt;/p&gt;
                          &lt;p&gt;
                            Enables/disables the coordinator synchronisation support.
                          &lt;/p&gt;
                        &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:a name="UUID" 
                         value="4d8cc2b1-03f6-4ab7-a472-2e47f0c7f605"/>
                    <a:da name="DEFAULT" value="false"/>
                    <a:da name="EDITABLE" value="false"/>
                    <a:da name="ENABLE" value="false"/>
                  </v:var>
                  <v:var name="UdpNmDevErrorDetect" type="BOOLEAN">
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                          Pre-processor switch for enabling development error detection support.
                        &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="Enable Development Error Detection"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:a name="UUID" 
                         value="6154e7f8-75f1-483e-84ce-ff3fa9c5ae3a"/>
                    <a:da name="DEFAULT" value="true"/>
                  </v:var>
                  <v:var name="UdpNmImmediateRestartEnabled" type="BOOLEAN">
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                          &lt;p&gt;Enabling the asynchronous transmission
                          of a NM PDU upon bus communication request
                          in Prepare-Bus-Sleep mode.&lt;/p&gt;

                          &lt;p&gt;Dependencies:&lt;/p&gt;
                          &lt;ul&gt;
                            &lt;li&gt;Passive Mode must be disabled.&lt;/li&gt;
                          &lt;/ul&gt;
                        &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="Immediate Restart"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:a name="UUID" 
                         value="e51bcdb2-876e-4b9d-9e14-5ff4d172ed91"/>
                    <a:da name="DEFAULT" value="false"/>
                    <a:da name="INVALID" type="XPath" 
                          expr="(. = &apos;true&apos;) and (../UdpNmPassiveModeEnabled = &apos;true&apos;)" 
                          true="Immediate Restart is not available when Passive Mode is enabled."/>
                  </v:var>
                  <v:var name="UdpNmMainFunctionPeriod" type="FLOAT">
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                          Call cycle in seconds of UdpNm_MainFunction &lt;code&gt;UdpNm_MainFunction_x&lt;/code&gt;
                        &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="Main Function Period [s]"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:a name="UUID" 
                         value="def76ce3-2bdd-427a-a1fd-7f1c5e76d69f"/>
                    <a:da name="DEFAULT" value="0.001"/>
                    <a:da name="INVALID" type="Range">
                      <a:tst expr="&lt;=0.255"/>
                      <a:tst expr="&gt;=0.001"/>
                    </a:da>
                  </v:var>
                  <v:var name="UdpNmNumberOfChannels" type="INTEGER">
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                          Maximum number of Udp NM channels allowed within one ECU.
                        &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="Number Of Channels"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:a name="UUID" 
                         value="2da7c589-ce67-4782-93ff-007ee386c4f0"/>
                    <a:da name="DEFAULT" value="1"/>
                    <a:da name="INVALID" type="Multi">
                      <mt:range>
                        <mt:tst expr="&lt;=255"/>
                        <mt:tst expr="&gt;=1"/>
                      </mt:range>
                      <mt:xpath 
                                true="The value must not be smaller than the number of configured channels." 
                                expr=". &lt; count(../UdpNmChannelConfig/*)"/>
                    </a:da>
                  </v:var>
                  <v:var name="UdpNmPostBuildRamSize" type="INTEGER">
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                          &lt;p&gt;Number of bytes for TX and RX buffers&lt;/p&gt;
                          &lt;p&gt;
                            Value should be set as: the sum of the first RxPdu lengths
                            on each channel multiplied with 2 (in case passive mode is disabled).
                            Size should be big enugh to hold eventual changes of PDU lenghts at postbuild time
                          &lt;/p&gt;
                        &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="UdpNmPostBuildRamSize"/>
                    <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:a name="UUID" 
                         value="956b57f4-bf6e-44c8-8785-33b084dd191c"/>
                    <a:da name="DEFAULT" value="96"/>
                  </v:var>
                  <v:var name="UdpNmPassiveModeEnabled" type="BOOLEAN">
                    <a:a name="CALCULATION-FORMULA" 
                         value="equal(nmpassivemodeenabled)"/>
                    <a:a name="CALCULATION-LANGUAGE" value="INFORMAL"/>
                    <a:a name="DERIVED" value="TRUE"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                          &lt;p&gt;Pre-processor switch for enabling support of the Passive Mode. This
                          parameter shall be derived from NM_PASSIVE_MODE_ENABLED.&lt;/p&gt;
                          &lt;p&gt;In passive mode, the UdpNm will not be able
                          to wake up the bus and will not send NM messages.
                          It will only listen to the NM messages and silently monitor the bus.&lt;/p&gt;
                        &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="Passive Mode"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:a name="UUID" 
                         value="f6fc6bcc-0b7d-44d5-8d89-f3cc6adc685b"/>
                    <a:da name="DEFAULT" value="false"/>
                    <a:da name="INVALID" type="XPath" 
                          expr="((. = &apos;true&apos;) and (count(as:modconf(&apos;Nm&apos;)[1]/NmChannelConfig/*[(node:exists(NmBusType/NmStandardBusType)) and (NmBusType/NmStandardBusType = &apos;NM_BUSNM_UDPNM&apos;) and ((not(node:exists(NmPassiveModeEnabled))) or (NmPassiveModeEnabled = &apos;false&apos;))]) &gt; 0))" 
                          true="Not all Nm channels with BusType &apos;NM_BUSNM_UDPNM&apos; has passive mode enabled."/>
                  </v:var>
                  <v:var name="UdpNmPduRxIndicationEnabled" type="BOOLEAN">
                    <a:a name="CALCULATION-FORMULA" 
                         value="equal(nmpdurxindicationenabled)"/>
                    <a:a name="CALCULATION-LANGUAGE" value="INFORMAL"/>
                    <a:a name="DERIVED" value="TRUE"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                          &lt;p&gt;Pre-processor switch for enabling the PDU Rx Indication. This
                             parameter shall be derived from NM_PDU_RX_INDICATION_ENABLED.
                             If a NM message is received the function&lt;/p&gt;
                          &lt;p&gt;Dependencies:&lt;/p&gt;
                          &lt;ul&gt;
                            &lt;li&gt;The value of this parameter has to be synchronized
                            with the value of respective parameter in the Nm module.&lt;/li&gt;
                          &lt;/ul&gt;
                        &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="PDU Receive Indication"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:a name="UUID" 
                         value="55ae0225-9e7e-495d-81da-bd0e16b41df4"/>
                    <a:da name="DEFAULT" value="false"/>
                    <a:da name="INVALID" type="XPath" 
                          expr="not(node:exists(as:modconf(&apos;Nm&apos;)[1]/NmGlobalConfig/NmGlobalFeatures/NmPduRxIndicationEnabled)) or                                     (. != as:modconf(&apos;Nm&apos;)[1]/NmGlobalConfig/NmGlobalFeatures/NmPduRxIndicationEnabled)" 
                          true="UdpNmPduRxIndicationEnabled has to use same setting as Nm."/>
                  </v:var>
                  <v:var name="UdpNmPnEiraCalcEnabled" type="BOOLEAN">
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                          &lt;p&gt;
                            Specifies if UdpNm calculates the PN request information for internal and
                            external requests(EIRA).
                          &lt;/p&gt;
                          &lt;ul&gt;
                            &lt;li&gt;true: PN request are calculated&lt;/li&gt;
                            &lt;li&gt;false: PN request are not calculated&lt;/li&gt;
                          &lt;/ul&gt;
                         &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v class="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="OPTIONAL" value="true"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:a name="UUID" 
                         value="78eea88e-87da-4d67-8852-62f3d29d7d1c"/>
                    <a:da name="DEFAULT" value="false"/>
                    <a:da name="ENABLE" value="false"/>
                    <a:da name="INVALID" type="XPath">
                      <a:tst 
                             expr="(. = &apos;true&apos;) and (../../../UdpNmGeneral/UdpNmPnSupported = &apos;false&apos;)" 
                             true="UdpNmPnSupported must be true when UdpNmPnEiraCalcEnabled is set as true"/>
                      <a:tst 
                             expr="(. = &apos;true&apos;) and not(node:exists(../UdpNmPnInfo))" 
                             true="EIRA calculation has been enabled but PN info configuration (UdpNmPnInfo) is missing"/>
                      <a:tst 
                             expr="(. = &apos;true&apos;) and not(node:exists(../UdpNmPnEiraRxNSduRef))" 
                             true="EIRA calculation has been enabled but UdpNmPnEiraRxNSduRef is missing"/>
                    </a:da>
                    <a:da name="WARNING" type="XPath" 
                          expr="(. = &apos;true&apos;) and (num:i(count(../UdpNmChannelConfig/*[node:exists(UdpNmPnEnabled) and (UdpNmPnEnabled = &apos;true&apos;)])) = 0)" 
                          true="There must be atleast one channel with UdpNmPnEnabled set as true when UdpNmPnEiraCalcEnabled is set as true"/>
                  </v:var>
                  <v:var name="UdpNmPnResetTime" type="FLOAT">
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                          &lt;p&gt;
                            Specifies the runtime of the reset timer in seconds.
                            This reset time is valid for the reset of PN requests in the EIRA and in the ERA.
                            The value shall be the same for every channel.
                          &lt;/p&gt;
                        &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v class="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="OPTIONAL" value="true"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:a name="UUID" 
                         value="8f809f9e-18a4-46c1-95eb-454b23aec108"/>
                    <a:da name="DEFAULT" value="0.01"/>
                    <a:da name="ENABLE" value="false"/>
                    <a:da name="INVALID" type="Range">
                      <a:tst expr="&lt;=65.535"/>
                      <a:tst expr="&gt;=0.0010"/>
                    </a:da>
                  </v:var>
                  <v:var name="UdpNmRemoteSleepIndEnabled" type="BOOLEAN">
                    <a:a name="CALCULATION-FORMULA" 
                         value="if (udpnmpassivemodeenabled == false) then equal(nmremotesleepindenabled) else equal(false)"/>
                    <a:a name="CALCULATION-LANGUAGE" value="INFORMAL"/>
                    <a:a name="DERIVED" value="TRUE"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                          &lt;p&gt;Pre-processor switch for enabling remote sleep indication support. This
                          feature is required for gateway nodes only. It must not be defined if
                          UDPNM_PASSIVE_MODE_ENABLED is defined. This parameter shall
                          be derived from NM_REMOTE_SLEEP_IND_ENABLED.&lt;/p&gt;
                          &lt;p&gt;Therefore the following API is provided:&lt;/p&gt;
                          &lt;ul&gt;
                            &lt;li&gt;&lt;code&gt;UdpNm_CheckRemoteSleepIndication()&lt;/code&gt;&lt;/li&gt;
                          &lt;/ul&gt;
                          &lt;p&gt;Dependencies:&lt;/p&gt;
                          &lt;ul&gt;
                            &lt;li&gt;The value of this parameter has to be synchronized
                              with the value of respective parameter in the Nm
                              module.&lt;/li&gt;
                            &lt;li&gt;Passive Mode must be disabled.&lt;/li&gt;
                          &lt;/ul&gt;
                        &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="Remote Sleep Indication"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:a name="UUID" 
                         value="c9514b77-f26c-4e4e-a929-b605f843e6f4"/>
                    <a:da name="DEFAULT" value="false"/>
                    <a:da name="INVALID" type="XPath">
                      <a:tst 
                             expr="(. = &apos;true&apos;) and (../UdpNmPassiveModeEnabled = &apos;true&apos;)" 
                             true="Remote Sleep Indication cannot be enabled if Passive Mode is enabled."/>
                      <a:tst 
                             expr="not(node:exists(as:modconf(&apos;Nm&apos;)[1]/NmGlobalConfig/NmGlobalFeatures/NmRemoteSleepIndEnabled)) or                                     (. != as:modconf(&apos;Nm&apos;)[1]/NmGlobalConfig/NmGlobalFeatures/NmRemoteSleepIndEnabled)" 
                             true="UdpNm Remote Sleep Indication has to use same setting as Nm."/>
                    </a:da>
                  </v:var>
                  <v:var name="UdpNmStateChangeIndEnabled" type="BOOLEAN">
                    <a:a name="CALCULATION-FORMULA" 
                         value="equal(NmStateChangeIndEnabled)"/>
                    <a:a name="CALCULATION-LANGUAGE" value="INFORMAL"/>
                    <a:a name="DERIVED" value="TRUE"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                          &lt;p&gt;Pre-processor switch for enabling the UDP NM state change
                            notification. This parameter shall be derived from NM_STATE_CHANGE_IND_ENABLED.&lt;/p&gt;
                        &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="State Change Indication"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:a name="UUID" 
                         value="7aef7283-59b0-4985-a3da-ba9825d8bdb4"/>
                    <a:da name="DEFAULT" value="false"/>
                    <a:da name="INVALID" type="XPath" 
                          expr="not(node:exists(as:modconf(&apos;Nm&apos;)[1]/NmGlobalConfig/NmGlobalFeatures/NmStateChangeIndEnabled))or                                    (. != as:modconf(&apos;Nm&apos;)[1]/NmGlobalConfig/NmGlobalFeatures/NmStateChangeIndEnabled)" 
                          true="UdpNm State Change Indication has to use same setting as Nm."/>
                  </v:var>
                  <v:var name="UdpNmUserDataEnabled" type="BOOLEAN">
                    <a:a name="CALCULATION-FORMULA" 
                         value="equal(nmuserdataenabled)"/>
                    <a:a name="CALCULATION-LANGUAGE" value="INFORMAL"/>
                    <a:a name="DERIVED" value="TRUE"/>
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                          &lt;p&gt;Pre-processor switch for enabling user data support. This
                             parameter shall be derived from NM_USER_DATA_ENABLED.&lt;/p&gt;
                          &lt;p&gt;Therefore the following API functions are provided:&lt;/p&gt;
                          &lt;ul&gt;
                            &lt;li&gt;&lt;code&gt;UdpNm_GetUserData()&lt;/code&gt;&lt;/li&gt;
                            &lt;li&gt;&lt;code&gt;UdpNm_SetUserData()&lt;/code&gt; &lt;em&gt;(Only if Passive Mode
                            Support is disabled)&lt;/em&gt;&lt;/li&gt;
                          &lt;/ul&gt;
                          &lt;p&gt;Dependencies:&lt;/p&gt;
                          &lt;ul&gt;
                            &lt;li&gt;The value of this parameter has to be synchronized
                            with the value of respective parameter in the Nm module.&lt;/li&gt;
                          &lt;/ul&gt;
                        &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="User Data"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:a name="UUID" 
                         value="1fcc77e1-0c8b-4ce5-8b7c-1fe152169315"/>
                    <a:da name="DEFAULT" value="false"/>
                    <a:da name="INVALID" type="XPath" 
                          expr="not(node:exists(as:modconf(&apos;Nm&apos;)[1]/NmGlobalConfig/NmGlobalFeatures/NmUserDataEnabled)) or                                    (. != as:modconf(&apos;Nm&apos;)[1]/NmGlobalConfig/NmGlobalFeatures/NmUserDataEnabled)" 
                          true="UdpNm User Data setting has to be configured as for Nm."/>
                  </v:var>
                  <v:var name="UdpNmVersionInfoApi" type="BOOLEAN">
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                          Pre-processor switch for enabling version info API support.
                          Provide API function for retrieving version information:
                          &lt;ul&gt;
                            &lt;li&gt;&lt;code&gt;UdpNm_GetVersionInfo()&lt;/code&gt;&lt;/li&gt;
                          &lt;/ul&gt;
                        &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="LABEL" value="Enable Version Info API"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:a name="UUID" 
                         value="8f03a7b4-ac35-4054-83e6-f8f390fe4609"/>
                    <a:da name="DEFAULT" value="false"/>
                  </v:var>
                  <v:ref name="UdpNmPnEiraRxNSduRef" type="REFERENCE">
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                             Reference to a Pdu in the COM-Stack. Only one SduRef is required for
                             UdpNm because the EIRA is the aggregation over all Ethernet Channels.
                        &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v class="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="OPTIONAL" value="true"/>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="UUID" 
                         value="d3f9c384-9ca1-441c-a45e-bbad3c814b34"/>
                    <a:da name="ENABLE" value="false"/>
                    <a:da name="INVALID" type="XPath" 
                          expr="(node:value(../../../UdpNmGeneral/UdpNmPnSupported) = &apos;true&apos;) and (not(node:refvalid(.)))" 
                          true="Invalid reference."/>
                    <a:da name="REF" 
                          value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/EcuC/EcucPduCollection/Pdu"/>
                    <a:da name="WARNING" type="XPath" 
                          expr="not(node:exists(../UdpNmPnEiraCalcEnabled)) or (../UdpNmPnEiraCalcEnabled != &apos;true&apos;)" 
                          true="UdpNmPnEiraRxNSduRef shall not be configured if UdpNmPnEiraCalcEnabled is not set or set to false."/>
                  </v:ref>
                  <v:lst name="UdpNmChannelConfig" type="MAP">
                    <a:a name="LABEL" value="Channel Configuration"/>
                    <a:da name="MIN" value="1"/>
                    <v:ctr name="UdpNmChannelConfig" type="IDENTIFIABLE">
                      <a:a name="DESC">
                        <a:v>&lt;html&gt;
                            This container holds the channel specific configuration parameter of the UdpNm.
                          &lt;/html&gt;</a:v>
                      </a:a>
                      <a:a name="LABEL" value="Channel Configuration"/>
                      <a:a name="POSTBUILDCHANGEABLE" value="false"/>
                      <a:a name="UUID" 
                           value="965e6e18-784d-4eda-a192-42502723036e"/>
                      <a:da name="INVALID" type="XPath" 
                            expr="(../../UdpNmPassiveModeEnabled = &apos;false&apos;) and not(node:exists(./UdpNmTxPdu))" 
                            true="The parameter UdpNmPassiveMode is set to &apos;false&apos;, but the UdpNmTxPdu container has been disabled!&apos;"/>
                      <v:var name="UdpNmNodeIdEnabled" type="BOOLEAN">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                              &lt;p&gt;Enable support for sending of Node Ids in NM messages and provide
                              functions for retrieving the node identifier from the
                              most recently received NM PDU and the local node identifier.&lt;/p&gt;
                      
                              &lt;p&gt;Therefore the following API functions are provided:&lt;/p&gt;
                              &lt;ul&gt;
                                &lt;li&gt;&lt;code&gt;Nm_GetNodeIdentifier()&lt;/code&gt;&lt;/li&gt;
                                &lt;li&gt;&lt;code&gt;Nm_GetLocalNodeIdentifier()&lt;/code&gt;&lt;/li&gt;
                              &lt;/ul&gt;
                      
                              &lt;p&gt;Dependencies:&lt;/p&gt;
                              &lt;ul&gt;
                                &lt;li&gt;The value of this parameter has to be synchronized
                                    with the value of the respective parameter in the Nm module.&lt;/li&gt;
                              &lt;/ul&gt;
                            &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="LABEL" value="Node Identifier"/>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" 
                             value="ff724ed4-1b35-4aa6-bf39-521cb41d6019"/>
                        <a:da name="DEFAULT" value="false"/>
                        <a:da name="INVALID" type="XPath" 
                              expr="node:exists(as:modconf(&apos;Nm&apos;)[1]/NmChannelConfig/*[node:exists(NmComMChannelRef) and (NmComMChannelRef = node:current()/../UdpNmComMNetworkHandleRef)]/NmNodeIdEnabled) and                                        (as:modconf(&apos;Nm&apos;)[1]/NmChannelConfig/*[node:exists(NmComMChannelRef) and (NmComMChannelRef = node:current()/../UdpNmComMNetworkHandleRef)]/NmNodeIdEnabled = node:current())" 
                              false="UdpNm Node Identifier has to use same setting as Nm."/>
                      </v:var>
                      <v:var name="UdpNmRepeatMsgIndEnabled" type="BOOLEAN">
                        <a:a name="CALCULATION-FORMULA" 
                             value="equal(nmrepeatmsgindenabled)"/>
                        <a:a name="CALCULATION-LANGUAGE" value="INFORMAL"/>
                        <a:a name="DERIVED" value="TRUE"/>
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                              &lt;p&gt;Enable/disable the notification that a RepeatMessageRequest bit has
                                been received. This parameter shall be derived from NM_REPEAT_MSG_IND_ENABLED.&lt;/p&gt;
                              &lt;p&gt;Dependencies:&lt;/p&gt;
                              &lt;ul&gt;
                                &lt;li&gt;Node Detection must be enabled.&lt;/li&gt;
                              &lt;/ul&gt;
                            &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="LABEL" value="Repeat Message Indication"/>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" 
                             value="49fc0763-8d4d-452f-8923-9ddbea7fd3a9"/>
                        <a:da name="DEFAULT" value="false"/>
                        <a:da name="INVALID" type="XPath">
                          <a:tst 
                                 expr="(../UdpNmNodeDetectionEnabled = &apos;false&apos;) and (. = &apos;true&apos;)" 
                                 true="Repeat Message Indication needs to have Node Detection enabled."/>
                          <a:tst 
                                 expr="node:exists(as:modconf(&apos;Nm&apos;)[1]/NmChannelConfig/*[node:exists(NmComMChannelRef) and (NmComMChannelRef = node:current()/../UdpNmComMNetworkHandleRef)]/NmRepeatMsgIndEnabled) and                                         (as:modconf(&apos;Nm&apos;)[1]/NmChannelConfig/*[node:exists(NmComMChannelRef) and (NmComMChannelRef = node:current()/../UdpNmComMNetworkHandleRef)]/NmRepeatMsgIndEnabled = node:current())" 
                                 false="UdpNm Repeat Message Indication has to use same setting as Nm."/>
                        </a:da>
                      </v:var>
                      <v:var name="UdpNmNodeDetectionEnabled" type="BOOLEAN">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                              &lt;p&gt;Pre-processor switch for enabling the node detection support. This
                              parameter shall be derived from NM_NODE_DETECTION_ENABLED.
                              This parameter shall only be enabled if UDPNM_NODE_ID_ENABLED
                              is defined. If(UdpNmPduCbvPosition != UDPNM_PDU_OFF) then
                              Equal(NmNodeDetectionEnabled) else Equal(False).&lt;/p&gt;
                              &lt;p&gt;For setting the &lt;em&gt;Repeat Message Request Bit&lt;/em&gt;
                              in NM messages following API function is provided:&lt;/p&gt;
                              &lt;ul&gt;
                                &lt;li&gt;&lt;code&gt;UdpNm_RepeatMessageRequest()&lt;/code&gt;&lt;/li&gt;
                              &lt;/ul&gt;
                              &lt;p&gt;Dependencies:&lt;/p&gt;
                              &lt;ul&gt;
                                &lt;li&gt;The value of this parameter has to be synchronized
                                  with the value of the respective parameter in the Nm
                                  module.&lt;/li&gt;
                                &lt;li&gt;Support for Node Identifiers must be enabled.&lt;/li&gt;
                                &lt;li&gt;Passive Mode must be disabled.&lt;/li&gt;
                              &lt;/ul&gt;
                            &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="LABEL" value="Node Detection"/>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" 
                             value="02c24934-6849-4f37-bd8d-b298913660bf"/>
                        <a:da name="DEFAULT" value="false"/>
                        <a:da name="INVALID" type="XPath">
                          <a:tst 
                                 expr="(. = &apos;true&apos;) and (../UdpNmNodeIdEnabled = &apos;false&apos;)" 
                                 true="UdpNm Node Detection cannot be enabled when Node Id is disabled."/>
                          <a:tst 
                                 expr="(. = &apos;true&apos;) and (../UdpNmPassiveModeEnabled = &apos;true&apos;)" 
                                 true="Node Detection cannot be enabled if Passive Mode is enabled."/>
                          <a:tst 
                                 expr="(node:exists(as:modconf(&apos;Nm&apos;)[1]/NmChannelConfig/*[node:exists(NmComMChannelRef) and (NmComMChannelRef = node:current()/../UdpNmComMNetworkHandleRef)]/NmNodeDetectionEnabled)) and                                         (as:modconf(&apos;Nm&apos;)[1]/NmChannelConfig/*[node:exists(NmComMChannelRef) and (NmComMChannelRef = node:current()/../UdpNmComMNetworkHandleRef)]/NmNodeDetectionEnabled = node:current())" 
                                 false="UdpNm Node Detection has to use same setting as Nm Node Detection."/>
                        </a:da>
                      </v:var>
                      <v:var name="UdpNmAllNmMessagesKeepAwake" type="BOOLEAN">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                              &lt;p&gt;
                                Specifies if UdpNm drops irrelevant NM PDUs
                              &lt;/p&gt;
                            &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="LABEL" value="UdpNmAllNmMessagesKeepAwake"/>
                        <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" 
                             value="4e71502a-2840-4334-b360-e940083fbae8"/>
                        <a:da name="DEFAULT" value="false"/>
                        <a:da name="INVALID" type="XPath" 
                              expr="(. = &apos;true&apos;) and (not(node:exists(../UdpNmPnEnabled)) or (../UdpNmPnEnabled != &apos;true&apos;))" 
                              true="Parameter UdpNmAllNmMessagesKeepAwake shall be disabled in case UdpNmPnEnabled is disabled or set to false"/>
                      </v:var>
                      <v:var name="UdpNmCarWakeUpBitPosition" type="INTEGER">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                              Specifies the Bit position of the CWU within the UdpNmCarWakeUpBytePosition.
                            &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v class="PreCompile">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" 
                             value="ECUC:263e1d1a-25c9-11e7-93ae-92361f002671"/>
                        <a:da name="DEFAULT" value="0"/>
                        <a:da name="ENABLE" type="XPath" 
                              expr="../UdpNmCarWakeUpRxEnabled = &apos;true&apos;"/>
                        <a:da name="INVALID" type="Range">
                          <a:tst expr="&lt;=7"/>
                          <a:tst expr="&gt;=0"/>
                        </a:da>
                      </v:var>
                      <v:var name="UdpNmCarWakeUpBytePosition" type="INTEGER">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                              Specifies the Byte position of the CWU within the NM-Message.
                            &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v class="PreCompile">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" 
                             value="ECUC:263e1f86-25c9-11e7-93ae-92361f002671"/>
                        <a:da name="DEFAULT" value="2"/>
                        <a:da name="ENABLE" type="XPath" 
                              expr="../UdpNmCarWakeUpRxEnabled = &apos;true&apos;"/>
                        <a:da name="INVALID" type="Multi">
                          <mt:range>
                            <mt:tst expr="&lt;=4294967294"/>
                            <mt:tst expr="&gt;=0"/>
                          </mt:range>
                          <mt:xpath>
                            <mt:tst 
                                    true="UdpNmCarWakeUpBytePosition cannot be on byte 0, this position is occupied by Control Bit Vector" 
                                    expr="(node:value(../UdpNmPduCbvPosition) = &apos;UDPNM_PDU_BYTE_0&apos;) and (. = 0)"/>
                            <mt:tst 
                                    true="UdpNmCarWakeUpBytePosition cannot be on byte 1, this position is occupied by Control Bit Vector" 
                                    expr="(node:value(../UdpNmPduCbvPosition) = &apos;UDPNM_PDU_BYTE_1&apos;) and (. = 1)"/>
                            <mt:tst 
                                    true="UdpNmCarWakeUpBytePosition cannot be on byte 0, this position is occupied by Node identifier" 
                                    expr="(node:value(../UdpNmPduNidPosition) = &apos;UDPNM_PDU_BYTE_0&apos;) and (. = 0)"/>
                            <mt:tst 
                                    true="UdpNmCarWakeUpBytePosition cannot be on byte 0, this position is occupied by Node identifier" 
                                    expr="(node:value(../UdpNmPduNidPosition) = &apos;UDPNM_PDU_BYTE_1&apos;) and (. = 1)"/>
                          </mt:xpath>
                        </a:da>
                      </v:var>
                      <v:var name="UdpNmCarWakeUpFilterEnabled" type="BOOLEAN">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                              &lt;p&gt;
                              If CWU filtering is supported, only the CWU bit within the
                              NM PDU message with source node identifier UdpNmCarWakeUpFilterNodeId
                              is considered as CWU request.
                              &lt;/p&gt;
                            &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v class="PreCompile">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" 
                             value="ECUC:263e208a-25c9-11e7-93ae-92361f002671"/>
                        <a:da name="DEFAULT" value="false"/>
                        <a:da name="ENABLE" type="XPath" 
                              expr="(../UdpNmCarWakeUpRxEnabled = &apos;true&apos;) and (../UdpNmPduNidPosition != &apos;UDPNM_PDU_OFF&apos;)"/>
                      </v:var>
                      <v:var name="UdpNmCarWakeUpFilterNodeId" type="INTEGER">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                              Source node identifier for CWU filtering.
                              If CWU filtering is supported, only the CWU bit within the
                              NM message with source node identifier UdpNmCarWakeUpFilterNodeId
                              is considered as CWU request.
                            &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v class="PreCompile">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" 
                             value="ECUC:263e2166-25c9-11e7-93ae-92361f002671"/>
                        <a:da name="DEFAULT" value="0"/>
                        <a:da name="ENABLE" type="XPath" 
                              expr="node:exists(../UdpNmCarWakeUpFilterEnabled) and (../UdpNmCarWakeUpFilterEnabled = &apos;true&apos;)"/>
                        <a:da name="INVALID" type="Range">
                          <a:tst expr="&lt;=255"/>
                          <a:tst expr="&gt;=0"/>
                        </a:da>
                      </v:var>
                      <v:var name="UdpNmCarWakeUpRxEnabled" type="BOOLEAN">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                              Enables or disables support of CarWakeUp bit evaluation in received NM messages.
                            &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" 
                             value="ECUC:263e2238-25c9-11e7-93ae-92361f002671"/>
                        <a:da name="DEFAULT" value="false"/>
                      </v:var>
                      <v:var name="UdpNmPnEnabled" type="BOOLEAN">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                              &lt;p&gt;Enables or disables support of partial networking.&lt;/p&gt;
                              &lt;ul&gt;
                                &lt;li&gt;true: Partial networking supported&lt;/li&gt;
                                &lt;li&gt;false: Partial networking not supported&lt;/li&gt;
                              &lt;/ul&gt;
                            &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v class="PostBuild">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="OPTIONAL" value="true"/>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="POSTBUILDVARIANTMULTIPLICITY" value="true"/>
                        <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" 
                             value="42ea61e7-0c10-43b7-b165-81e0424a1541"/>
                        <a:da name="DEFAULT" value="false"/>
                        <a:da name="ENABLE" value="false"/>
                        <a:da name="INVALID" type="XPath">
                          <a:tst 
                                 expr="(. = &apos;true&apos;) and (../../../UdpNmComUserDataSupport = &apos;false&apos;)" 
                                 true="UdpNmPnEnabled is set to &apos;true&apos; but UdpNmComUserDataSupport is set to &apos;false&apos;!"/>
                          <a:tst 
                                 expr="(. = &apos;true&apos;) and (../../../../../UdpNmGeneral/UdpNmPnSupported = &apos;false&apos;)" 
                                 true="There must not be any Partial Network Cluster(s) configured if UdpNmPnSupported is set as false"/>
                          <a:tst 
                                 expr="(node:value(../UdpNmPduCbvPosition) = &apos;UDPNM_PDU_OFF&apos;) and (. = &apos;true&apos;)" 
                                 true="If &apos;UdpNmPnEnabled&apos; is enabled, the position of the CBV (UdpNmPduCbvPosition) must not be set to &apos;UDPNM_PDU_OFF&apos;."/>
                          <a:tst 
                                 expr="node:exists(../UdpNmPnHandleMultipleNetworkRequests) and ((. = &apos;false&apos; ) and (node:value(../UdpNmPnHandleMultipleNetworkRequests) = &apos;true&apos;))" 
                                 true="Support for partial networks (UdpNmPnEnabled) must be enabled if UdpNmPnHandleMultipleNetworkRequests is set to &apos;true&apos;!"/>
                        </a:da>
                      </v:var>
                      <v:var name="UdpNmPnEraCalcEnabled" type="BOOLEAN">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                              &lt;p&gt;Specifies if UdpNm calculates the PN request information for external requests.(ERA)&lt;/p&gt;
                            &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v class="PreCompile">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="OPTIONAL" value="true"/>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" 
                             value="a3256afb-9bc3-4f30-8965-44355f08360e"/>
                        <a:da name="DEFAULT" value="false"/>
                        <a:da name="ENABLE" value="false"/>
                        <a:da name="INVALID" type="XPath">
                          <a:tst 
                                 expr="(. = &apos;true&apos;) and (node:value(../../../../../UdpNmGeneral/UdpNmPnSupported) = &apos;false&apos;)" 
                                 true="UdpNmPnSupported must be true when UdpNmPnEraCalcEnabled is set as true"/>
                          <a:tst 
                                 expr="(. = &apos;true&apos;) and (not(node:exists(../UdpNmPnEraRxNSduRef)))" 
                                 true="If UdpNmPnEraCalcEnabled is set as true UdpNmPnEraRxNSduRef must be configured"/>
                        </a:da>
                      </v:var>
                      <v:var name="UdpNmPnHandleMultipleNetworkRequests" 
                             type="BOOLEAN">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                              &lt;ul&gt;
                                &lt;li&gt;true: UdpNm_NetworkRequest triggers a change from NO to RM.&lt;/li&gt;
                                &lt;li&gt;false: UdpNm_NetworkRequest is ignored in NO.&lt;/li&gt;
                              &lt;/ul&gt;
                              &lt;p&gt;
                                Dependencies:
                                &lt;ul&gt;
                                  &lt;li&gt;Support for Partial Networks must be enabled for this channel &lt;/li&gt;
                                &lt;/ul&gt;
                              &lt;/p&gt;
                            &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v class="PostBuild">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="OPTIONAL" value="true"/>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" 
                             value="1882810c-1089-4979-a981-db0b066441f9"/>
                        <a:da name="DEFAULT" value="false"/>
                        <a:da name="EDITABLE" value="true"/>
                        <a:da name="ENABLE" value="false"/>
                        <a:da name="INVALID" type="XPath">
                          <a:tst 
                                 expr="(. = &apos;true&apos;) and (../../../../../UdpNmGeneral/UdpNmPnSupported = &apos;false&apos;)" 
                                 true="UdpNmPnHandleMultipleNetworkRequests cannot be true if UdpNmPnSupported is set as false"/>
                          <a:tst 
                                 expr="((../UdpNmImmediateNmTransmissions = 0) and (. = &apos;true&apos;))" 
                                 true="Parameter UdpNmImmediateNmTransmissions should be greater than zero if UdpNmPnHandleMultipleNetworkRequests is set to true"/>
                        </a:da>
                      </v:var>
                      <v:ref name="UdpNmPnEraRxNSduRef" type="REFERENCE">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                              Reference to a Pdu in the COM-Stack.
                              The SduRef is required for every UdpNm Channel, because ERA is reported per channel.
                            &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v class="PreCompile">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="OPTIONAL" value="true"/>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="UUID" 
                             value="767efede-ca8b-4da4-bc82-042b7784cf83"/>
                        <a:da name="ENABLE" value="false"/>
                        <a:da name="INVALID" type="XPath" 
                              expr="(node:exists(../UdpNmPnEraCalcEnabled)) and (../UdpNmPnEraCalcEnabled = &apos;true&apos;) and (not(node:refvalid(.)))" 
                              true="Invalid reference."/>
                        <a:da name="REF" 
                              value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/EcuC/EcucPduCollection/Pdu"/>
                      </v:ref>
                      <v:var name="UdpNmImmediateNmCycleTime" type="FLOAT">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                              &lt;p&gt;Defines the immediate NM PDU cycle time in seconds which is used
                              for UdpNmImmediateNmTransmissions NM PDU transmissions.
                              This parameter is only valid if UdpNmImmediateNmTransmissions
                              is greater one&lt;/p&gt;
                              &lt;p&gt;Dependencies:&lt;/p&gt;
                              &lt;ul&gt;
                                &lt;li&gt;This parameter is active only if
                                &lt;code&gt;UdpNmImmediateNmTransmissions&lt;/code&gt;
                                greater than one&lt;/li&gt;
                              &lt;/ul&gt;
                            &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v class="PreCompile">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="LABEL" value="Immediate NM PDU cycle time"/>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" 
                             value="4acfbdba-f95e-4bed-8849-d309339bca35"/>
                        <a:da name="DEFAULT" value="0.001"/>
                        <a:da name="ENABLE" type="XPath" 
                              expr="../UdpNmImmediateNmTransmissions &gt;1"/>
                        <a:da name="INVALID" type="Multi">
                          <mt:range>
                            <mt:tst expr="&lt;=65.535"/>
                            <mt:tst expr="&gt;=0.001"/>
                          </mt:range>
                          <mt:xpath 
                                    true="The Immediate NmCycleTime must be multiple of the Main Function Period." 
                                    expr="(../../../UdpNmPassiveModeEnabled != &apos;true&apos;) and (../UdpNmImmediateNmTransmissions &gt; 1) and                                        (((. * 1000) mod (../../../UdpNmMainFunctionPeriod * 1000)) != 0)"/>
                        </a:da>
                      </v:var>
                      <v:var name="UdpNmImmediateNmTransmissions" 
                             type="INTEGER">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                              &lt;p&gt;Defines the number of immediate NM PDUs which shall
                              be transmitted. If the value is zero no immediate NM PDUs
                              are transmitted. The cycle time of immeditate NM PDUs is
                              defined by &lt;code&gt;UdpNmImmediateNmCycleTime&lt;/code&gt;.&lt;/p&gt;
                              &lt;p&gt;Dependencies:&lt;/p&gt;
                              &lt;ul&gt;
                                &lt;li&gt;&lt;code&gt;UdpNmImmediateNmCycleTime&lt;/code&gt; is active only if this
                                parameter greater than zero&lt;/li&gt;
                              &lt;/ul&gt;
                            &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="LABEL" value="Number of immediate NM PDUs"/>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" 
                             value="e8f414c7-2877-4b20-a1f0-8594ad233915"/>
                        <a:da name="DEFAULT" value="0"/>
                        <a:da name="EDITABLE" type="XPath" 
                              expr="../../../UdpNmPassiveModeEnabled = &apos;false&apos;"/>
                        <a:da name="INVALID" type="Multi">
                          <mt:range>
                            <mt:tst expr="&lt;=255"/>
                            <mt:tst expr="&gt;=0"/>
                          </mt:range>
                          <mt:xpath 
                                    true="UdpNmImmediateNmTransmissions must be 0 when UdpNmImmediateRestartEnabled is true ." 
                                    expr="(. != 0) and (../../../UdpNmImmediateRestartEnabled = &apos;true&apos;)"/>
                        </a:da>
                      </v:var>
                      <v:var name="UdpNmRetryFirstMessageRequest" 
                             type="BOOLEAN">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                              &lt;p&gt;
                              If UdpNmRetryFirstMessageRequest is enabled and if the first transmit request
                              after transition into NetworkMode is not accepted by SoAd,
                              the message request shall be repeated in the next main function until one
                              transmit request is accepted by SoAd.
                              Note: This feature can be used in case of partial network wakeup filter
                              to avoid a blocking of all messages in case of
                              passive start-up and first message request is not accepted by
                              SoAd due to EthSM could not enable transmission path fast enough
                              (e.g. in case of asynchronous transceiver handling).
                              &lt;/p&gt;
                            &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v class="PreCompile">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="LABEL" value="UdpNmRetryFirstMessageRequest"/>
                        <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" 
                             value="6a3208a2-9adc-4b03-a858-1534fd5c0220"/>
                        <a:da name="DEFAULT" value="false"/>
                        <a:da name="ENABLE" type="XPath" 
                              expr="as:modconf(&apos;UdpNm&apos;)[1]/UdpNmGlobalConfig/*[1]/UdpNmPassiveModeEnabled = &apos;false&apos;"/>
                      </v:var>
                      <v:var name="UdpNmMsgCycleOffset" type="FLOAT">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                              &lt;p&gt;Time offset in the periodic transmission node. It determines the start
                              delay of the transmission. &amp;lt; UDPNM_MSG_CYCLE_TIME This
                              parameter is only valid if UDPNM_PASSIVE_MODE_ENABLED is disabled.&lt;/p&gt;
                              &lt;p&gt;Dependencies:&lt;/p&gt;
                              &lt;ul&gt;
                                &lt;li&gt;If Passive Mode is enabled this parameter is ignored.&lt;/li&gt;
                                &lt;li&gt;The Message Cycle Offset must be smaller than the Message Cycle Time.&lt;/li&gt;
                                &lt;li&gt;The value must be multiple of the Main Function Period.&lt;/li&gt;
                              &lt;/ul&gt;
                            &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="LABEL" value="Message Cycle Offset [s]"/>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" 
                             value="c26a71c1-23f9-4ab6-bef8-b800b98a4140"/>
                        <a:da name="DEFAULT" value="0.001"/>
                        <a:da name="EDITABLE" type="XPath" 
                              expr="../../../UdpNmPassiveModeEnabled = &apos;false&apos;"/>
                        <a:da name="INVALID" type="Multi">
                          <mt:range>
                            <mt:tst expr="&lt;=65.535"/>
                            <mt:tst expr="&gt;=0"/>
                          </mt:range>
                          <mt:xpath>
                            <mt:tst 
                                    true="The Message Cycle Offset must be less than the Message Cycle Time." 
                                    expr="(../../../UdpNmPassiveModeEnabled = &apos;false&apos;) and (. &gt;= ../UdpNmMsgCycleTime)"/>
                            <mt:tst 
                                    true="The Message Cycle Offset must be a multiple of the Main Function Period." 
                                    expr="(../../../UdpNmPassiveModeEnabled = &apos;false&apos;) and (((. * 1000) mod (../../../UdpNmMainFunctionPeriod* 1000)) != 0)"/>
                          </mt:xpath>
                        </a:da>
                      </v:var>
                      <v:var name="UdpNmMsgCycleTime" type="FLOAT">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                              &lt;p&gt;Period of a NM-message. It determines the periodic rate and is the basis
                              for transmit scheduling. NM_TIMEOUT_TIME = n * UDPNM_MSG_CYCLE_TIME This parameter is only valid if
                              UDPNM_PASSIVE_MODE_ENABLED is disabled.&lt;/p&gt;
                              &lt;p&gt;Dependencies:&lt;/p&gt;
                              &lt;ul&gt;
                                &lt;li&gt;If Passive Mode is enabled this parameter is ignored.&lt;/li&gt;
                                &lt;li&gt;The value must be multiple of the Main Function Period.&lt;/li&gt;
                              &lt;/ul&gt;
                            &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="LABEL" value="Message Cycle Time [s]"/>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" 
                             value="72cfcfc7-668c-4ba3-8d93-8083e4a5975b"/>
                        <a:da name="DEFAULT" value="0.002"/>
                        <a:da name="EDITABLE" type="XPath" 
                              expr="../../../UdpNmPassiveModeEnabled = &apos;false&apos;"/>
                        <a:da name="INVALID" type="Multi">
                          <mt:range>
                            <mt:tst expr="&lt;=65.535"/>
                            <mt:tst expr="&gt;=0.001"/>
                          </mt:range>
                          <mt:xpath>
                            <mt:tst 
                                    true="The Message Cycle Time must be multiple of the Main Function Period." 
                                    expr="(./../../../UdpNmPassiveModeEnabled = &apos;false&apos;) and (((. * 1000) mod (../../../UdpNmMainFunctionPeriod * 1000)) != 0)"/>
                            <mt:tst 
                                    true="The Message Cycle Time must be less than UdpNmPnResetTime." 
                                    expr="(./../../../UdpNmPassiveModeEnabled = &apos;false&apos;) and (node:value(../../../../../UdpNmGeneral/UdpNmPnSupported) = &apos;true&apos;) and (not(node:exists(../../../UdpNmPnResetTime)) or (. &gt;= ../../../UdpNmPnResetTime))"/>
                          </mt:xpath>
                        </a:da>
                      </v:var>
                      <v:var name="UdpNmMsgTimeoutTime" type="FLOAT">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                              &lt;p&gt;Transmission Timout of NM-message. If there is no transmission
                              confirmation by the UDP Interface within this timeout, the UDPNM
                              module shall gibe an error notification. This parameter is only valid if
                              UDPNM_PASSIVE_MODE_ENABLED is disabled. UDPNM_MSG_TIMEOUT_TIME should be a multiple of UDPNM_MSG_CYCLE_TIME.&lt;/p&gt;
                              &lt;p&gt;Dependencies:&lt;/p&gt;
                              &lt;ul&gt;
                                &lt;li&gt;If Passive Mode is enabled this parameter is ignored.&lt;/li&gt;
                                &lt;li&gt;Value must be a multiple of the Main Function Period.&lt;/li&gt;
                                &lt;li&gt;Value must be less than the Message Cycle Time.&lt;/li&gt;
                              &lt;/ul&gt;
                            &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="LABEL" value="Message Timeout Time [s]"/>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" 
                             value="10348a31-d6f5-4cdd-8453-201782b84a2e"/>
                        <a:da name="DEFAULT" value="0.002"/>
                        <a:da name="EDITABLE" type="XPath" 
                              expr="../../../UdpNmPassiveModeEnabled = &apos;false&apos;"/>
                        <a:da name="INVALID" type="Multi">
                          <mt:range>
                            <mt:tst expr="&lt;=65.535"/>
                            <mt:tst expr="&gt;=0.001"/>
                          </mt:range>
                          <mt:xpath>
                            <mt:tst 
                                    true="The Message Timeout Time must be less than the Message Cycle Time." 
                                    expr="(../../../UdpNmPassiveModeEnabled = &apos;false&apos;) and (. &gt;= ../UdpNmMsgCycleTime)"/>
                            <mt:tst 
                                    true="The Message Timeout Time must be a multiple of the Main Function Period." 
                                    expr="(../../../UdpNmPassiveModeEnabled = &apos;false&apos;) and (((. * 1000) mod (../../../UdpNmMainFunctionPeriod * 1000)) != 0)"/>
                          </mt:xpath>
                        </a:da>
                      </v:var>
                      <v:var name="UdpNmNodeId" type="INTEGER">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                              &lt;p&gt;Node identifier of local node. This parameter is only valid if
                                UDPNM_PASSIVE_MODE_ENABLED is set to OFF and UDPNM_NODE_DETECTION_ENABLED is set to ON.&lt;/p&gt;
                              &lt;p&gt;Dependencies:&lt;/p&gt;
                              &lt;ul&gt;
                                &lt;li&gt;This parameter is only valid if UdpNmPassiveModeEnabled = False&lt;/li&gt;
                                &lt;li&gt;If the Node Identifier Position is set to UDPNM_PDU_OFF this parameter is ignored.&lt;/li&gt;
                              &lt;/ul&gt;
                            &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v class="PostBuild">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="LABEL" value="Node Identifier"/>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" 
                             value="1380033c-835b-4420-b7d0-4bb0fec28ef4"/>
                        <a:da name="DEFAULT" value="0"/>
                        <a:da name="EDITABLE" type="XPath" 
                              expr="../UdpNmPduNidPosition != &apos;UDPNM_PDU_OFF&apos;"/>
                        <a:da name="ENABLE" type="XPath" 
                              expr="../UdpNmNodeIdEnabled = &apos;true&apos;"/>
                        <a:da name="INVALID" type="Range">
                          <a:tst expr="&lt;=255"/>
                          <a:tst expr="&gt;=0"/>
                        </a:da>
                      </v:var>
                      <v:var name="UdpNmPduCbvPosition" type="ENUMERATION">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                              &lt;p&gt;Defines the position of the control bit vector within the NM
                                PACKET. The value of the parameter represents the location of
                                the control bit vector in the NM PACKET (UDPNM_PDU_BYTE_0
                                means byte 0, UDPNM_PDU_BYTE_1 means byte 1,
                                UDPNM_PDU_OFF means the control bit vector is not part of the
                                NM PACKET) See also UDPNM_PDU_NID_POSITION if
                                (UDPNM_PDU_CBV_POSITION != UDPNM_PDU_OFF &amp;amp;
                                UDPNM_PDU_NID_POSITION != UDPNM_PDU_OFF) then
                                UDPNM_PDU_CBV_POSITION != UDPNM_PDU_NID_POSITION if
                                (UDPNM_PDU_CBV_POSITION != UDPNM_PDU_OFF &amp;amp
                                UDPNM_PDU_NID_POSITION == UDPNM_PDU_OFF) then
                                UDPNM_PDU_CBV_POSITION = UDPNM_PDU_BYTE0&lt;/p&gt;
                              &lt;p&gt;Dependencies:&lt;/p&gt;
                              &lt;ul&gt;
                                &lt;li&gt;If Node Detection support is disabled this parameter is ignored.&lt;/li&gt;
                                &lt;li&gt;The Control Bit Vector must not occupy the same byte as the Node Id.&lt;/li&gt;
                              &lt;/ul&gt;
                            &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="LABEL" 
                             value="PDU Control Bit Vector Position"/>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" 
                             value="a71d3d81-7f88-4011-8d69-bf2ca9b2e391"/>
                        <a:da name="DEFAULT" value="UDPNM_PDU_BYTE_1"/>
                        <a:da name="INVALID" type="XPath">
                          <a:tst 
                                 expr="(. != &apos;UDPNM_PDU_OFF&apos;) and (. = ../UdpNmPduNidPosition)" 
                                 true="The Node ID also occupies this byte position."/>
                          <a:tst 
                                 expr="(. = &apos;UDPNM_PDU_OFF&apos;) and (../UdpNmNodeDetectionEnabled = &apos;true&apos;)" 
                                 true="If Node Detection support is enabled the position for the Control Bit Vector must be specified."/>
                          <a:tst 
                                 expr="(. = &apos;UDPNM_PDU_OFF&apos;) and (../../../UdpNmActiveWakeupBitEnabled = &apos;true&apos;)" 
                                 true="If &apos;UdpNmActiveWakeupBitEnabled&apos; is enabled, the position of the CBV must not be set to &apos;UDPNM_PDU_OFF&apos;."/>
                          <a:tst 
                                 expr="(. = &apos;UDPNM_PDU_BYTE_1&apos;) and (not(node:exists(../../../UdpNmPnInfo)) or (../../../UdpNmPnInfo/UdpNmPnInfoOffset &lt; 2)) and (node:value(../../../../../UdpNmGeneral/UdpNmPnSupported) = &apos;true&apos;)" 
                                 true="If UdpNmPduCbvPosition is set to UDPNM_PDU_BYTE_1 the UdpNmPnInfoOffset cannot be smaller than 2"/>
                        </a:da>
                        <a:da name="RANGE">
                          <a:v>UDPNM_PDU_BYTE_0</a:v>
                          <a:v>UDPNM_PDU_BYTE_1</a:v>
                          <a:v>UDPNM_PDU_OFF</a:v>
                        </a:da>
                        <a:da name="WARNING" type="XPath" 
                              expr="(. = &apos;UDPNM_PDU_BYTE_1&apos;) and (../UdpNmPduNidPosition = &apos;UDPNM_PDU_OFF&apos;)" 
                              true="The first byte of the NM message remains unused in this configuration."/>
                      </v:var>
                      <v:var name="UdpNmPduNidPosition" type="ENUMERATION">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                              &lt;p&gt;Defines the position of the source node identifier within the NM
                                PACKET. ImplementationType: UdpNm_PduPositionType The
                                value of the parameter represents the location of the source node
                                identifier in the NM PACKET (UDPNM_PDU_BYTE_0 means byte
                                0, UDPNM_PDU_BYTE_1 means byte 1, UDPNM_PDU_OFF
                                means source node identifier is not part of the NM PACKET) See
                                also UDPNM_PDU_CBV_POSITION if (UDPNM_PDU_NID_POSITION != UDPNM_PDU_OFF &amp;&amp;
                                UDPNM_PDU_CBV_POSITION != UDPNM_PDU_OFF) then UDPNM_PDU_NID_POSITION != UDPNM_PDU_CBV_POSITION if (UDPNM_PDU_NID_POSITION
                                != UDPNM_PDU_OFF &amp;&amp; UDPNM_PDU_CBV_POSITION == UDPNM_PDU_OFF) then UDPNM_PDU_IND_POSITION = UDPNM_PDU_BYTE0&lt;/p&gt;
                              &lt;p&gt;Dependencies:&lt;/p&gt;
                              &lt;ul&gt;
                                &lt;li&gt;If Node Id support is disabled this parameter is ignored.&lt;/li&gt;
                                &lt;li&gt;The Node Id must not occupy the same byte as the Control Bit Vector&lt;/li&gt;
                              &lt;/ul&gt;
                            &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="LABEL" value="PDU Node Identifier Position"/>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" 
                             value="71ed8dcc-db01-46dd-b75a-94624356f42c"/>
                        <a:da name="DEFAULT" value="UDPNM_PDU_BYTE_0"/>
                        <a:da name="INVALID" type="XPath">
                          <a:tst 
                                 expr="(. = &apos;UDPNM_PDU_OFF&apos;) and (../UdpNmNodeIdEnabled = &apos;true&apos;)" 
                                 true="If Node Identifier support is enabled a position for the node identifier must be specified."/>
                          <a:tst 
                                 expr="(. != &apos;UDPNM_PDU_OFF&apos;) and (../UdpNmNodeIdEnabled = &apos;false&apos;)" 
                                 true="If Node Identifier support is disabled the position for the node identifier must be UDPNM_PDU_OFF."/>
                          <a:tst 
                                 expr="(. != &apos;UDPNM_PDU_OFF&apos;) and (. = ../UdpNmPduCbvPosition)" 
                                 true="The Control Bit Vector also occupies this byte position."/>
                          <a:tst 
                                 expr="(. = &apos;UDPNM_PDU_BYTE_1&apos;) and (not(node:exists(../../../UdpNmPnInfo)) or (../../../UdpNmPnInfo/UdpNmPnInfoOffset &lt; 2)) and (node:value(../../../../../UdpNmGeneral/UdpNmPnSupported) = &apos;true&apos;)" 
                                 true="If UdpNmPduNidPosition is set to UDPNM_PDU_BYTE_1 the UdpNmPnInfoOffset cannot be smaller than 2"/>
                        </a:da>
                        <a:da name="RANGE">
                          <a:v>UDPNM_PDU_BYTE_0</a:v>
                          <a:v>UDPNM_PDU_BYTE_1</a:v>
                          <a:v>UDPNM_PDU_OFF</a:v>
                        </a:da>
                        <a:da name="WARNING" type="XPath" 
                              expr="(. = &apos;UDPNM_PDU_BYTE_1&apos;) and (../UdpNmPduCbvPosition = &apos;UDPNM_PDU_OFF&apos;)" 
                              true="The first byte of the NM message remains unused in this configuration."/>
                      </v:var>
                      <v:var name="UdpNmRemoteSleepIndTime" type="FLOAT">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                              &lt;p&gt;Timeout for Remote Sleep Indication. It defines the time in [s] how
                                long it shall take to recognize that all other nodes are ready to sleep.
                                Typically it should be equal to: n * UDPNM_MSG_CYCLE_TIME,
                                where n denotes the number of NM packets that are normally sent
                                before Remote Sleep Indication is detected. The value of n
                                decremented by one determines the amount of lost NM packets that
                                can be tolerated by the Remote Sleep Indication procedure.&lt;/p&gt;
                              &lt;p&gt;Dependencies:&lt;/p&gt;
                              &lt;ul&gt;
                                &lt;li&gt;UdpNmRemoteSleepIndTime &gt;= UdpNmMsgCycleTime&lt;/li&gt;
                              &lt;/ul&gt;
                            &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="LABEL" 
                             value="Remote Sleep Indication Time [s]"/>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" 
                             value="597686cb-3e59-43ff-84ec-8318a132b38d"/>
                        <a:da name="DEFAULT" value="0.001"/>
                        <a:da name="EDITABLE" type="XPath" 
                              expr="../../../UdpNmRemoteSleepIndEnabled = &apos;true&apos;"/>
                        <a:da name="INVALID" type="Multi">
                          <mt:range>
                            <mt:tst expr="&lt;=65.535"/>
                            <mt:tst expr="&gt;=0.001"/>
                          </mt:range>
                          <mt:xpath>
                            <mt:tst 
                                    true="The Remote Sleep Indication Time must be a multiple of the Main Function Period." 
                                    expr="(../../../UdpNmRemoteSleepIndEnabled = &apos;true&apos;) and (((. * 1000) mod (../../../UdpNmMainFunctionPeriod * 1000)) != 0)"/>
                            <mt:tst 
                                    true="UdpNmRemoteSleepIndTime needs to be greater or equal than UdpNmMsgCycleTime." 
                                    expr="(../../../UdpNmRemoteSleepIndEnabled = &apos;true&apos;) and (node:value(.) &lt; node:value(../UdpNmMsgCycleTime))"/>
                          </mt:xpath>
                        </a:da>
                      </v:var>
                      <v:var name="UdpNmRepeatMessageTime" type="FLOAT">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                              &lt;p&gt;Timeout for Repeat Message State. It defines the time in [s] how
                                long the NM shall stay in the Repeat Message State. Typically it
                                should be equal to: n * UDPNM_MSG_CYCLE_TIME, where n
                                denotes the number of NM packets that are normally sent in the
                                Repeat Message State. The value of n decremented by one
                                determines the amount of lost NM packets that can be tolerated by
                                the node detection procedure. The value 0 denotes that no Repeat
                                Message State is configured. It means that Repeat Message State
                                is transient what implicates that it is left immediately after entrance
                                and in result no start-up stability is guaranteed and no node
                                detection procedure is possible.&lt;/p&gt;
                              &lt;p&gt;Dependencies:&lt;/p&gt;
                              &lt;ul&gt;
                                &lt;li&gt;If Passive Mode is enabled this parameter is ignored.&lt;/li&gt;
                                &lt;li&gt;Value must be a multiple of the Main Function Period.&lt;/li&gt;
                              &lt;/ul&gt;
                            &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="LABEL" value="Repeat Message Time [s]"/>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" 
                             value="e6114dad-3ed5-426d-ad92-fc54fba79693"/>
                        <a:da name="DEFAULT" value="0.001"/>
                        <a:da name="EDITABLE" type="XPath" 
                              expr="../../../UdpNmPassiveModeEnabled = &apos;false&apos;"/>
                        <a:da name="INVALID" type="Multi">
                          <mt:range>
                            <mt:tst expr="&lt;=65.535"/>
                            <mt:tst expr="&gt;=0"/>
                          </mt:range>
                          <mt:xpath>
                            <mt:tst 
                                    true="The UdpNm Repeat Message Time must be a multiple of the Main Function Period." 
                                    expr="(../../../UdpNmPassiveModeEnabled = &apos;false&apos;) and (((. * 1000) mod (../../../UdpNmMainFunctionPeriod * 1000)) != 0)"/>
                            <mt:tst 
                                    true="UdpNmRepeatMessageTime must be greater than UdpNmImmediateNmTransmissions * UdpNmImmediateNmCycleTime" 
                                    expr="(../../../UdpNmPassiveModeEnabled = &apos;false&apos;) and (node:exists(../UdpNmImmediateNmCycleTime)) and ((../UdpNmImmediateNmTransmissions * (../UdpNmImmediateNmCycleTime * 1000)) &gt;= (. * 1000))"/>
                          </mt:xpath>
                        </a:da>
                      </v:var>
                      <v:var name="UdpNmTimeoutTime" type="FLOAT">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                                &lt;p&gt;Network Timeout for NM packets. It denotes the time in [s]
                                  how long the NM shall stay in the Network Mode before
                                  transition into Prepare Bus-Sleep Mode shall take place. It
                                  shall be equal for all nodes in the cluster. It shall be greater
                                  than UDPNM_MSG_CYCLE_TIME. Typically, it should be
                                  equal to: x * UDPNM_MSG_CYCLE_TIME, where n denotes
                                  the number of NM PACKET cycle times in the Ready Sleep
                                  State before transition into the Bus-Sleep Mode is initiated.
                                  The value of n decremented by one determines the amount of
                                  lost NM packets that can be tolerated by the coordination algorithm.&lt;/p&gt;
                                &lt;p&gt;Dependencies:&lt;/p&gt;
                                &lt;ul&gt;
                                  &lt;li&gt;Value must be a multiple of the Main Function Period.&lt;/li&gt;
                                  &lt;li&gt;The UdpNmTimeoutTime must be greater than UdpNmPnResetTime.&lt;/li&gt;
                                  &lt;li&gt;The Timeout Time must be greater than Message cycle time.&lt;/li&gt;
                                  &lt;li&gt;The Timeout Time must be a multiple of the Main Function Period.&lt;/li&gt;
                                &lt;/ul&gt;
                              &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="LABEL" value="Timeout Time [s]"/>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" 
                             value="d4acd59f-bf7d-481d-957f-4579b165b93a"/>
                        <a:da name="DEFAULT" value="0.004"/>
                        <a:da name="INVALID" type="Multi">
                          <mt:range>
                            <mt:tst expr="&lt;=65.535"/>
                            <mt:tst expr="&gt;=0.002"/>
                          </mt:range>
                          <mt:xpath>
                            <mt:tst 
                                    true="The Timeout Time must be a multiple of the Main Function Period." 
                                    expr="((. * 1000) mod (../../../UdpNmMainFunctionPeriod * 1000)) != 0"/>
                            <mt:tst 
                                    true="The Timeout Time must be greater than Message cycle time." 
                                    expr="(. &lt;= ../UdpNmMsgCycleTime)"/>
                            <mt:tst 
                                    true="The UdpNmTimeoutTime must be greater than UdpNmPnResetTime." 
                                    expr="(./../../../UdpNmPassiveModeEnabled = &apos;false&apos;) and (node:value(../../../../../UdpNmGeneral/UdpNmPnSupported) = &apos;true&apos;) and (node:exists(../../../UdpNmPnResetTime)) and (. &lt;= ../../../UdpNmPnResetTime)"/>
                          </mt:xpath>
                        </a:da>
                      </v:var>
                      <v:var name="UdpNmWaitBusSleepTime" type="FLOAT">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                              &lt;p&gt;Timeout for bus calm down phase. It denotes the time in [s] how long the NM shall
                                stay in the Prepare Bus-Sleep Mode before transition into Bus-Sleep Mode
                                shall take place. It shall be equal for all nodes in the cluster. It
                                shall be long enough to empty all Tx buffer empty.&lt;/p&gt;
                              &lt;p&gt;Dependencies:&lt;/p&gt;
                              &lt;ul&gt;
                                &lt;li&gt;Value must be a multiple of the Main Function Period.&lt;/li&gt;
                              &lt;/ul&gt;
                            &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="LABEL" value="Wait Bus Sleep Time [s]"/>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" 
                             value="0cff9cde-c91d-43b2-b583-254c88bf1d53"/>
                        <a:da name="DEFAULT" value="0.004"/>
                        <a:da name="INVALID" type="Multi">
                          <mt:range>
                            <mt:tst expr="&lt;=65.535"/>
                            <mt:tst expr="&gt;=0.001"/>
                          </mt:range>
                          <mt:xpath 
                                    true="The Wait Bus Sleep Time must be a multiple of the Main Function Period." 
                                    expr="((. * 1000) mod (../../../UdpNmMainFunctionPeriod * 1000)) != 0"/>
                        </a:da>
                      </v:var>
                      <v:ref name="UdpNmComMNetworkHandleRef" 
                             type="SYMBOLIC-NAME-REFERENCE">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                              &lt;p&gt;
                                This reference points to the unique channel defined by the ComMChannel and provides
                                access to the unique channel index value in ComMChannelId.
                              &lt;/p&gt;
                            &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="UUID" 
                             value="cb3041db-5edb-42d2-8068-a02c93d8c4f3"/>
                        <a:da name="INVALID" type="XPath">
                          <a:tst expr="node:refvalid(.)" 
                                 false="Invalid ComM channel reference"/>
                          <a:tst 
                                 expr="count(as:modconf(&apos;Nm&apos;)[1]/NmChannelConfig/*[(node:exists(NmComMChannelRef)) and (NmComMChannelRef = node:current()) and                                             (node:exists(NmBusType/NmStandardBusType)) and (NmBusType/NmStandardBusType = &apos;NM_BUSNM_UDPNM&apos;)]) != 1" 
                                 true="For every UdpNmComMNetworkHandleRef there should be configured only one NmComMChannelRef, with NmStandardBusType = NM_BUSNM_UDPNM"/>
                          <a:tst expr="node:exists(node:refs(.)/ComMChannelId)" 
                                 false="Referenced ComM channel id missing "/>
                        </a:da>
                        <a:da name="REF" 
                              value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/ComM/ComMConfigSet/ComMChannel"/>
                      </v:ref>
                      <v:lst name="UdpNmRxPdu" type="MAP">
                        <a:a name="COLUMNS">
                          <a:v>UdpNmRxPduId</a:v>
                          <a:v>UdpNmRxPduRef</a:v>
                        </a:a>
                        <a:da name="MIN" value="1"/>
                        <v:ctr name="UdpNmRxPdu" type="IDENTIFIABLE">
                          <a:a name="DESC">
                            <a:v>&lt;html&gt;
                                &lt;p&gt;
                                  This container holds the &lt;code&gt;UdpNmRxPduId&lt;/code&gt; and the &lt;code&gt;UdpNmRxPduRef.&lt;/code&gt;
                                &lt;/p&gt;
                              &lt;/html&gt;</a:v>
                          </a:a>
                          <a:a name="LABEL" value="Receive PDU ID and Reference"/>
                          <a:a name="POSTBUILDCHANGEABLE" value="false"/>
                          <a:a name="UUID" 
                               value="5df3de01-3da1-49ca-8629-61c8d48ae4d1"/>
                          <v:var name="UdpNmRxPduId" type="INTEGER">
                            <a:a name="DESC">
                              <a:v>&lt;html&gt;
                                  &lt;p&gt;
                                    ID of the RxPdu that will be used by a RxIndication of the lower layer.
                                  &lt;/p&gt;
                                &lt;/html&gt;</a:v>
                            </a:a>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                 type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="LABEL" value="Receive PDU ID"/>
                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="true"/>
                            <a:a name="UUID" 
                                 value="896ae045-15d8-45f4-9924-5bffa801cc91"/>
                            <a:da name="DEFAULT" type="XPath" 
                                  expr="node:fallback(node:pos(./../../..),0)"/>
                            <a:da name="INVALID" type="Multi">
                              <mt:range>
                                <mt:tst expr="&lt;=4294967296"/>
                                <mt:tst expr="&gt;=0"/>
                              </mt:range>
                              <mt:xpath>
                                <mt:tst 
                                        false="UdpNmRxPduIds are not zero based and consecutive from channel to channel." 
                                        expr="node:isconsecutive(../../../../*/UdpNmRxPdu/*[1]/UdpNmRxPduId, 0)"/>
                                <mt:tst 
                                        true="All the UdpNmRxPdu&apos;s of a channel need to have the same id!" 
                                        expr="count(../../../UdpNmRxPdu/*[UdpNmRxPduId != node:current()]) != 0"/>
                              </mt:xpath>
                            </a:da>
                          </v:var>
                          <v:ref name="UdpNmRxPduRef" type="REFERENCE">
                            <a:a name="DESC">
                              <a:v>&lt;html&gt;
                                  &lt;p&gt;
                                    The reference to a PDU in the global PDU structure described in the AUTOSAR ECU Configuration
                                    Specification. This reference will be used by the UdpNm module to derive the PDU Id.
                                  &lt;/p&gt;
                                &lt;/html&gt;</a:v>
                            </a:a>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                 type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="LABEL" value="Receive PDU Reference"/>
                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                            <a:a name="UUID" 
                                 value="4b740185-2281-4ae9-b500-0b1ac9e03cfb"/>
                            <a:da name="INVALID" type="XPath">
                              <a:tst expr="node:refvalid(.)" 
                                     false="Invalid reference"/>
                              <a:tst 
                                     expr="count(../../../../../UdpNmChannelConfig/*/UdpNmRxPdu/*[UdpNmRxPduRef = node:current()]) &gt; 1" 
                                     true="Rx PDU Reference must be unique"/>
                              <a:tst 
                                     expr="count(node:refs(../../../UdpNmRxPdu/*/UdpNmRxPduRef)[node:exists(node:ref(node:current())/PduLength) and (PduLength != node:ref(node:current())/PduLength)]) != 0" 
                                     true="Lengths Must be the same for all the RX PDUs inside a channel"/>
                              <a:tst expr="node:exists(node:ref(.)/PduLength)" 
                                     false="Referenced node PduLength does not exist"/>
                            </a:da>
                            <a:da name="REF" 
                                  value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/EcuC/EcucPduCollection/Pdu"/>
                          </v:ref>
                        </v:ctr>
                      </v:lst>
                      <v:ctr name="UdpNmTxPdu" type="IDENTIFIABLE">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                              This container contains the &lt;code&gt;UdpNmTxConfirmationPduId&lt;/code&gt; and the
                              &lt;code&gt;UdpNmTxPduRef&lt;/code&gt;.
                            &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="LABEL" value="Transmit PDU Reference"/>
                        <a:a name="OPTIONAL" value="true"/>
                        <a:a name="UUID" 
                             value="c42b6e90-dbc7-4196-ba49-bd5de53b53cc"/>
                        <a:da name="ENABLE" value="false"/>
                        <a:da name="INVALID" type="XPath" 
                              expr="count(../../../../../UdpNmGlobalConfig/*/UdpNmChannelConfig/*/UdpNmTxPdu[(asc:getShortName(node:current()) != &apos;&apos;) and (asc:getShortName(.) = asc:getShortName(node:current()))]) &gt; 1" 
                              true="The generated symbolic names for this parameter will not be unique since other UdpNmTxPdu are using the same SHORT-NAME."/>
                        <v:var name="UdpNmTxConfirmationPduId" type="INTEGER">
                          <a:a name="DESC">
                            <a:v>&lt;html&gt;
                                &lt;p&gt;
                                  Id of the TxPdu that will be used by a TxConfirmation from the lower layer.
                                &lt;/p&gt;
                              &lt;/html&gt;</a:v>
                          </a:a>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                               type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                          </a:a>
                          <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                          <a:a name="SYMBOLICNAMEVALUE" value="true"/>
                          <a:a name="UUID" 
                               value="0b86a29d-4cf7-425b-8a99-a1628dc9642e"/>
                          <a:da name="DEFAULT" value="0"/>
                          <a:da name="INVALID" type="Multi">
                            <mt:range>
                              <mt:tst expr="&lt;=4294967296"/>
                              <mt:tst expr="&gt;=0"/>
                            </mt:range>
                            <mt:xpath 
                                      false="UdpNmTxConfirmationPduId shall be unique across the channels" 
                                      expr="text:uniq(../../../../UdpNmChannelConfig/*/UdpNmTxPdu/UdpNmTxConfirmationPduId,.)"/>
                          </a:da>
                        </v:var>
                        <v:ref name="UdpNmTxPduRef" type="REFERENCE">
                          <a:a name="DESC">
                            <a:v>&lt;html&gt;
                                &lt;p&gt;
                                  The reference to a PDU in the global PDU structure described in the AUTOSAR ECU Configuration Specification.
                                  This reference will be used by the UdpNm module to derive the PDU Id.
                                &lt;/p&gt;
                              &lt;/html&gt;</a:v>
                          </a:a>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                               type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                          </a:a>
                          <a:a name="LABEL" value="Transmit PDU Reference"/>
                          <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                          <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                          <a:a name="UUID" 
                               value="ca3c8c44-5946-493d-b1b5-7eef941affcb"/>
                          <a:da name="INVALID" type="XPath">
                            <a:tst 
                                   expr="count(as:modconf(&apos;SoAd&apos;)[1]/SoAdConfig/*/SoAdPduRoute/*[node:exists(SoAdTxUpperLayerType) and (SoAdTxUpperLayerType=&apos;IF&apos;) and node:exists(SoAdTxPduRef) and (SoAdTxPduRef = node:current())]) = 0" 
                                   true="No SoAdTxPduRef with SoAdTxUpperLayerType set to &apos;IF&apos; exists for this UdpNmTxPduRef"/>
                            <a:tst 
                                   expr="not(node:exists(as:modconf(&apos;SoAd&apos;)[1]/SoAdConfig/*/SoAdPduRoute/*[node:exists(SoAdTxUpperLayerType) and (SoAdTxUpperLayerType=&apos;IF&apos;) and node:exists(SoAdTxPduRef) and (SoAdTxPduRef = node:current())]/SoAdTxPduId))" 
                                   true="For SoAdTxPduRef with SoAdTxUpperLayerType set to &apos;IF&apos; of this UdpNmTxPduRef, no SoAdTxPduId exists"/>
                            <a:tst expr="not(node:refvalid(.))" 
                                   true="Invalid reference."/>
                            <a:tst expr="node:exists(node:ref(.)/PduLength)" 
                                   false="Referenced node PduLength does not exist"/>
                          </a:da>
                          <a:da name="REF" 
                                value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/EcuC/EcucPduCollection/Pdu"/>
                        </v:ref>
                      </v:ctr>
                      <v:ctr name="UdpNmUserDataTxPdu" type="IDENTIFIABLE">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                              &lt;p&gt;This optional container is used to configure the UserNm PDU.
                              This container is only available if &lt;code&gt;UdpNmComUserDataSupport&lt;/code&gt;
                              is enabled.&lt;/p&gt;
                            &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="LABEL" value="User Data Transmission PDU"/>
                        <a:a name="OPTIONAL" value="true"/>
                        <a:a name="UUID" 
                             value="1ffa6c09-07b3-403b-b507-1d008a6b0ef1"/>
                        <a:da name="ENABLE" value="false"/>
                        <a:da name="INVALID" type="XPath">
                          <a:tst 
                                 expr="(../../../UdpNmComUserDataSupport = &apos;false&apos;)" 
                                 true="The UdpNmComUserDataSupport should be enabled."/>
                          <a:tst 
                                 expr="count(../../../../../UdpNmGlobalConfig/*/UdpNmChannelConfig/*/UdpNmUserDataTxPdu[(asc:getShortName(node:current()) != &apos;&apos;) and (asc:getShortName(.) = asc:getShortName(node:current()))]) &gt; 1" 
                                 true="The generated symbolic names for this parameter will not be unique since other UdpNmUserDataTxPdu are using the same SHORT-NAME."/>
                        </a:da>
                        <v:var name="UdpNmTxUserDataPduId" type="INTEGER">
                          <a:a name="DESC">
                            <a:v>&lt;html&gt;
                                &lt;p&gt;
                                  This parameter defines the Handle ID of the NM User Data I-PDU.
                                &lt;/p&gt;
                              &lt;/html&gt;</a:v>
                          </a:a>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                               type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                          </a:a>
                          <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                          <a:a name="SYMBOLICNAMEVALUE" value="true"/>
                          <a:a name="UUID" 
                               value="8e4269cb-adba-4055-aee3-57082446276c"/>
                          <a:da name="DEFAULT" type="XPath" 
                                expr="node:fallback(node:pos(./../..),0)"/>
                          <a:da name="INVALID" type="Multi">
                            <mt:range>
                              <mt:tst expr="&lt;=65535"/>
                              <mt:tst expr="&gt;=0"/>
                            </mt:range>
                            <mt:xpath 
                                      false="UdpNmTxUserDataPduIds are not zero based consecutive." 
                                      expr="node:isconsecutive(../../../*/UdpNmUserDataTxPdu/UdpNmTxUserDataPduId, 0)"/>
                          </a:da>
                        </v:var>
                        <v:ref name="UdpNmTxUserDataPduRef" type="REFERENCE">
                          <a:a name="DESC">
                            <a:v>&lt;html&gt;
                                &lt;p&gt;
                                  Reference to the NM User Data I-PDU in the global PDU collection.
                                &lt;/p&gt;
                              &lt;/html&gt;</a:v>
                          </a:a>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                               type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                          </a:a>
                          <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                          <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                          <a:a name="UUID" 
                               value="366e419a-b1c3-4038-bae2-396d21fe2715"/>
                          <a:da name="INVALID" type="XPath">
                            <a:tst expr="node:refvalid(.)" 
                                   false="Invalid reference"/>
                            <a:tst expr="node:exists(node:ref(.)/PduLength)" 
                                   false="Referenced node PduLength does not exist"/>
                          </a:da>
                          <a:da name="REF" 
                                value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/EcuC/EcucPduCollection/Pdu"/>
                        </v:ref>
                      </v:ctr>
                      <v:ctr name="UdpNmUserDataRxPdu" type="IDENTIFIABLE">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                                &lt;p&gt;This optional container is used to configure the UserNm PDU.
                                 This container is only available if &lt;code&gt;UdpNmComUserDataSupport&lt;/code&gt; is enabled.&lt;/p&gt;
                              &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="LABEL" value="User Data Reception PDUs"/>
                        <a:a name="OPTIONAL" value="true"/>
                        <a:a name="UUID" 
                             value="ba18ff7f-83e9-45e1-98fd-d8a9e0bfe434"/>
                        <a:da name="ENABLE" value="false"/>
                        <a:da name="INVALID" type="XPath" 
                              expr="(../../../UdpNmComUserDataSupport = &apos;false&apos;)" 
                              true="The UdpNmComUserDataSupport should be enabled."/>
                        <v:ref name="UdpNmRxUserDataPduRef" type="REFERENCE">
                          <a:a name="DESC">
                            <a:v>&lt;html&gt;
                                &lt;p&gt;Reference to the Rx NM User Data I-PDU in the global PDU collection&lt;/p&gt;
                              &lt;/html&gt;</a:v>
                          </a:a>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                               type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                          </a:a>
                          <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                          <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                          <a:a name="UUID" 
                               value="04675f1a-8683-41b6-bb64-420fff7e08af"/>
                          <a:da name="INVALID" type="XPath">
                            <a:tst expr="node:refvalid(.)" 
                                   false="Invalid reference"/>
                            <a:tst 
                                   expr="node:exists(../../../../UdpNmPnEiraRxNSduRef) and (node:value(../../../../UdpNmPnEiraRxNSduRef) = node:value(.))" 
                                   true="UdpNmPnEiraRxNSduRef and UdpNmRxUserDataPduRef references shouldn&apos;t be identical to each other!"/>
                            <a:tst expr="node:exists(node:ref(.)/PduLength)" 
                                   false="Referenced node PduLength does not exist"/>
                          </a:da>
                          <a:da name="REF" 
                                value="ASPathDataOfSchema:/AUTOSAR/EcuC/EcucPduCollection/Pdu"/>
                        </v:ref>
                      </v:ctr>
                    </v:ctr>
                  </v:lst>
                  <v:ctr name="UdpNmPnInfo" type="IDENTIFIABLE">
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                          &lt;p&gt;PN information configuration.&lt;/p&gt;
                        &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="OPTIONAL" value="true"/>
                    <a:a name="UUID" 
                         value="2d272464-efa6-4dee-8f46-d74199100b71"/>
                    <a:da name="ENABLE" value="false"/>
                    <a:da name="INVALID" type="XPath" 
                          expr="((UdpNmPnInfoOffset + UdpNmPnInfoLength) - 1) &gt; 31" 
                          true="The length of the (((UdpNmPnInfoOffset + UdpNmPnInfoLength) -1) should not be greater then 31"/>
                    <a:da name="WARNING" type="XPath" 
                          expr="node:exists(.) and count(../UdpNmChannelConfig/*[node:exists(UdpNmPnEnabled) and UdpNmPnEnabled=&apos;true&apos;]) = 0" 
                          true="PN information is available but no channel supports partial networking"/>
                    <v:var name="UdpNmPnInfoLength" type="INTEGER">
                      <a:a name="DESC">
                        <a:v>&lt;html&gt;
                              &lt;p&gt;Specifies the length of the PN request information in the NM message.&lt;/p&gt;
                          &lt;/html&gt;</a:v>
                      </a:a>
                      <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                           type="IMPLEMENTATIONCONFIGCLASS">
                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                      </a:a>
                      <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                      <a:a name="UUID" 
                           value="15acedc4-9db0-4954-bb61-58985188b2e8"/>
                      <a:da name="DEFAULT" value="1"/>
                      <a:da name="INVALID" type="Range">
                        <a:tst expr="&lt;=7"/>
                        <a:tst expr="&gt;=1"/>
                      </a:da>
                    </v:var>
                    <v:var name="UdpNmPnInfoOffset" type="INTEGER">
                      <a:a name="DESC">
                        <a:v>&lt;html&gt;
                              &lt;p&gt;Specifies the offset of the PN request information in the NM message.&lt;/p&gt;
                          &lt;/html&gt;</a:v>
                      </a:a>
                      <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                           type="IMPLEMENTATIONCONFIGCLASS">
                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                      </a:a>
                      <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                      <a:a name="UUID" 
                           value="8ab1bf36-4558-46c0-ac8f-89606d4962b6"/>
                      <a:da name="DEFAULT" value="1"/>
                      <a:da name="INVALID" type="Range">
                        <a:tst expr="&lt;=31"/>
                        <a:tst expr="&gt;=1"/>
                      </a:da>
                    </v:var>
                    <v:lst name="UdpNmPnFilterMaskByte" type="MAP">
                      <a:da name="INVALID" type="XPath" 
                            expr="(count(../../UdpNmChannelConfig/*[node:exists(UdpNmPnEnabled) and (UdpNmPnEnabled = &apos;true&apos;)]) &gt; 0) and (count(*) != ../UdpNmPnInfoLength)" 
                            true="The number of entries of UdpNmPnFilterMaskByte is not equal to the value of UdpNmPnInfoLength"/>
                      <a:da name="MAX" value="7"/>
                      <v:ctr name="UdpNmPnFilterMaskByte" type="IDENTIFIABLE">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                              &lt;p&gt;PN information configuration.&lt;/p&gt;
                            &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="POSTBUILDCHANGEABLE" value="false"/>
                        <a:a name="UUID" 
                             value="ab462c4f-5713-47d5-85da-0c70ef767adc"/>
                        <v:var name="UdpNmPnFilterMaskByteIndex" type="INTEGER">
                          <a:a name="DESC">
                            <a:v>&lt;html&gt;
                                &lt;p&gt;
                                  Index of the filter mask byte. Specifies the position within the filter mask
                                &lt;/p&gt;
                              &lt;/html&gt;</a:v>
                          </a:a>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                               type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                          </a:a>
                          <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                          <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                          <a:a name="UUID" 
                               value="70895a94-422d-4ea6-9b48-690fe79e46e7"/>
                          <a:da name="DEFAULT" value="0"/>
                          <a:da name="INVALID" type="Multi">
                            <mt:range>
                              <mt:tst expr="&lt;=6"/>
                              <mt:tst expr="&gt;=0"/>
                            </mt:range>
                            <mt:xpath>
                              <mt:tst 
                                      true="Value out of range (0..(UdpNmPnInfoLength-1))" 
                                      expr="(as:modconf(&apos;UdpNm&apos;)[1]/UdpNmGeneral/UdpNmPnSupported = &apos;true&apos;) and                                             (. &gt;= ../../../UdpNmPnInfoLength)"/>
                              <mt:tst 
                                      true="PN filter mask index shall be unique" 
                                      expr="(node:value(../../../../../../UdpNmGeneral/UdpNmPnSupported) = &apos;true&apos;) and not(text:uniq(../../*/UdpNmPnFilterMaskByteIndex,.))"/>
                            </mt:xpath>
                          </a:da>
                        </v:var>
                        <v:var name="UdpNmPnFilterMaskByteValue" type="INTEGER">
                          <a:a name="DESC">
                            <a:v>&lt;html&gt;
                                 &lt;p&gt;Parameter to configure the filter mask byte.&lt;/p&gt;
                              &lt;/html&gt;</a:v>
                          </a:a>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                               type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                          </a:a>
                          <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                          <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                          <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                          <a:a name="UUID" 
                               value="28d7b7ab-4a4f-4b61-a222-b3bbbbe5738f"/>
                          <a:da name="DEFAULT" value="0"/>
                          <a:da name="INVALID" type="Range">
                            <a:tst expr="&lt;=255"/>
                            <a:tst expr="&gt;=0"/>
                          </a:da>
                        </v:var>
                      </v:ctr>
                    </v:lst>
                  </v:ctr>
                </v:ctr>
              </v:lst>
              <d:ref type="REFINED_MODULE_DEF" 
                     value="ASPath:/AUTOSAR/EcucDefs/UdpNm"/>
              <v:ctr name="UdpNmDefensiveProgramming" type="IDENTIFIABLE">
                <a:a name="DESC">
                  <a:v>&lt;html&gt;
                &lt;p&gt;Parameters for defensive programming&lt;/p&gt;
                &lt;/html&gt;</a:v>
                </a:a>
                <a:a name="LABEL" value="Defensive Programming Options"/>
                <v:var name="UdpNmDefProgEnabled" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                    &lt;p&gt;Enables or disables the defensive
                    programming feature for the module UdpNm.&lt;/p&gt;
                    &lt;p&gt;Note: This feature is dependent on the use of the development error detection module.
                    To use the defensive programming feature, proceed as follows:
                    &lt;ol&gt;
                    &lt;li&gt;Enable development error detection&lt;/li&gt;
                    &lt;li&gt;Enable defensive programming&lt;/li&gt;
                    &lt;li&gt;Enable assertions as required&lt;/li&gt;
                    &lt;/ol&gt;
                    &lt;/p&gt;
                    &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="LABEL" value="Enable Defensive Programming"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:da name="DEFAULT" value="false"/>
                  <a:da name="EDITABLE" type="XPath" 
                        expr="../../UdpNmGlobalConfig/*[1]/UdpNmDevErrorDetect = &apos;true&apos;" 
                        true="true" false="false"/>
                </v:var>
                <v:var name="UdpNmPrecondAssertEnabled" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                  &lt;p&gt;Enables handling of precondition assertion checks reported from
                  the module UdpNm.&lt;/p&gt;
                  &lt;p&gt;Dependency on parameter(s):
                  &lt;ul&gt;
                  &lt;li&gt;Enable Development Error Detection (&lt;code&gt;UdpNmDevErrorDetect&lt;/code&gt;): must be enabled &lt;/li&gt;
                  &lt;li&gt;Enable Defensive Programming (&lt;code&gt;UdpNmDefProgEnabled&lt;/code&gt;): must be enabled&lt;/li&gt;
                  &lt;/ul&gt;
                  &lt;/p&gt;
                  &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="LABEL" value="Enable Precondition Assertions"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:da name="DEFAULT" value="false"/>
                  <a:da name="EDITABLE" type="XPath" 
                        expr="(../../UdpNmGlobalConfig/*[1]/UdpNmDevErrorDetect = &apos;true&apos;) and (../UdpNmDefProgEnabled = &apos;true&apos;)" 
                        true="true" false="false"/>
                </v:var>
                <v:var name="UdpNmPostcondAssertEnabled" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                  &lt;p&gt;Enables handling of postcondition assertion checks reported from
                  the module UdpNm.&lt;/p&gt;
                  &lt;p&gt;Dependency on parameter(s):
                  &lt;ul&gt;
                  &lt;li&gt;Enable Development Error Detection (&lt;code&gt;UdpNmDevErrorDetect&lt;/code&gt;): must be enabled &lt;/li&gt;
                  &lt;li&gt;Enable Defensive Programming (&lt;code&gt;UdpNmDefProgEnabled&lt;/code&gt;): must be enabled&lt;/li&gt;
                  &lt;/ul&gt;
                  &lt;/p&gt;
                  &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="LABEL" value="Enable Postcondition Assertions"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:da name="DEFAULT" value="false"/>
                  <a:da name="EDITABLE" type="XPath" 
                        expr="(../../UdpNmGlobalConfig/*[1]/UdpNmDevErrorDetect = &apos;true&apos;) and (../UdpNmDefProgEnabled = &apos;true&apos;)" 
                        true="true" false="false"/>
                </v:var>
                <v:var name="UdpNmStaticAssertEnabled" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                  &lt;p&gt;Enables handling of static assertion checks reported from
                  the module UdpNm.&lt;/p&gt;
                  &lt;p&gt;Dependency on parameter(s):
                  &lt;ul&gt;
                  &lt;li&gt;Enable Development Error Detection (&lt;code&gt;UdpNmDevErrorDetect&lt;/code&gt;): must be enabled &lt;/li&gt;
                  &lt;li&gt;Enable Defensive Programming (&lt;code&gt;UdpNmDefProgEnabled&lt;/code&gt;): must be enabled&lt;/li&gt;
                  &lt;/ul&gt;
                  &lt;/p&gt;
                  &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="LABEL" value="Enable Static Assertions"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:da name="DEFAULT" value="false"/>
                  <a:da name="EDITABLE" type="XPath" 
                        expr="(../../UdpNmGlobalConfig/*[1]/UdpNmDevErrorDetect = &apos;true&apos;) and (../UdpNmDefProgEnabled =&apos;true&apos;)" 
                        true="true" false="false"/>
                </v:var>
                <v:var name="UdpNmUnreachAssertEnabled" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                  &lt;p&gt;Enables handling of unreachable code assertion checks
                  reported from the module UdpNm.&lt;/p&gt;
                  &lt;p&gt;Dependency on parameter(s):
                  &lt;ul&gt;
                  &lt;li&gt;Enable Development Error Detection (&lt;code&gt;UdpNmDevErrorDetect&lt;/code&gt;): must be enabled &lt;/li&gt;
                  &lt;li&gt;Enable Defensive Programming (&lt;code&gt;UdpNmDefProgEnabled&lt;/code&gt;): must be enabled&lt;/li&gt;
                  &lt;/ul&gt;
                  &lt;/p&gt;
                  &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="LABEL" value="Enable Unreachable Code Assertions"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:da name="DEFAULT" value="false"/>
                  <a:da name="EDITABLE" type="XPath" 
                        expr="(../../UdpNmGlobalConfig/*[1]/UdpNmDevErrorDetect = &apos;true&apos;) and (../UdpNmDefProgEnabled = &apos;true&apos;)" 
                        true="true" false="false"/>
                </v:var>
                <v:var name="UdpNmInvariantAssertEnabled" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                  &lt;p&gt;Enables handling of invariant assertion checks
                  reported from functions of the module UdpNm.&lt;/p&gt;
                  &lt;p&gt;Dependency on parameter(s):
                  &lt;ul&gt;
                  &lt;li&gt;Enable Development Error Detection (&lt;code&gt;UdpNmDevErrorDetect&lt;/code&gt;): must be enabled &lt;/li&gt;
                  &lt;li&gt;Enable Defensive Programming (&lt;code&gt;UdpNmDefProgEnabled&lt;/code&gt;): must be enabled&lt;/li&gt;
                  &lt;/ul&gt;
                  &lt;/p&gt;
                  &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="LABEL" value="Enable Invariant Assertions"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:da name="DEFAULT" value="false"/>
                  <a:da name="EDITABLE" type="XPath" 
                        expr="(../../UdpNmGlobalConfig/*[1]/UdpNmDevErrorDetect = &apos;true&apos;) and (../UdpNmDefProgEnabled = &apos;true&apos;)" 
                        true="true" false="false"/>
                </v:var>
              </v:ctr>
            </v:ctr>
          </d:chc>
        </d:lst>
      </d:ctr>
    </d:lst>
  </d:ctr>

</datamodel>
