[!AUTOSPACING!][!//
[!MACRO "ReportErrorStatus", "Type", "AsrVersion", "InterfaceName" = "'DETService'", "TopLevelPackage" = "'/AUTOSAR_Det'"!]
[!VAR "InterfacePath" = "concat($TopLevelPackage,'/',$Type,'/',$InterfaceName)"!]
            <AR-PACKAGE>
              <SHORT-NAME>[!"$Type"!]</SHORT-NAME>
              <ELEMENTS>
                <CLIENT-SERVER-INTERFACE>
                  <SHORT-NAME>[!"$InterfaceName"!]</SHORT-NAME>
                  <IS-SERVICE>true</IS-SERVICE>
                  <OPERATIONS>
[!IF "$AsrVersion != 'AUTOSAR_43'"!][!//
                    <CLIENT-SERVER-OPERATION>
                      <SHORT-NAME>ReportError</SHORT-NAME>
                      <ARGUMENTS>
                        <ARGUMENT-DATA-PROTOTYPE>
                          <SHORT-NAME>InstanceId</SHORT-NAME>
                          <TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_Platform/ImplementationDataTypes/uint8</TYPE-TREF>
                          <DIRECTION>IN</DIRECTION>
                        </ARGUMENT-DATA-PROTOTYPE>
                        <ARGUMENT-DATA-PROTOTYPE>
                          <SHORT-NAME>ApiId</SHORT-NAME>
                          <TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_Platform/ImplementationDataTypes/uint8</TYPE-TREF>
                          <DIRECTION>IN</DIRECTION>
                        </ARGUMENT-DATA-PROTOTYPE>
                        <ARGUMENT-DATA-PROTOTYPE>
                          <SHORT-NAME>ErrorId</SHORT-NAME>
                          <TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_Platform/ImplementationDataTypes/uint8</TYPE-TREF>
                          <DIRECTION>IN</DIRECTION>
                        </ARGUMENT-DATA-PROTOTYPE>
                      </ARGUMENTS>
  [!IF "$AsrVersion != 'AUTOSAR_32'"!][!//
                      <POSSIBLE-ERROR-REFS>
                        <!-- although the service does never return E_NOT_OK we need to specify
                             this so that the function prototype of Det_ReportError() generated by
                             the Rte matches those of the Det SWS -->
                        <POSSIBLE-ERROR-REF DEST="APPLICATION-ERROR">[!"$InterfacePath"!]/E_OK</POSSIBLE-ERROR-REF>
                      </POSSIBLE-ERROR-REFS>
  [!ENDIF!][!//
                    </CLIENT-SERVER-OPERATION>
[!ENDIF!][!//
[!IF "$AsrVersion = 'AUTOSAR_43'"!][!//
                    <CLIENT-SERVER-OPERATION>
                      <SHORT-NAME>ReportError</SHORT-NAME>
                      <ARGUMENTS>
                        <ARGUMENT-DATA-PROTOTYPE>
                          <SHORT-NAME>ApiId</SHORT-NAME>
                          <TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_Platform/ImplementationDataTypes/uint8</TYPE-TREF>
                          <DIRECTION>IN</DIRECTION>
                        </ARGUMENT-DATA-PROTOTYPE>
                        <ARGUMENT-DATA-PROTOTYPE>
                          <SHORT-NAME>ErrorId</SHORT-NAME>
                          <TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_Platform/ImplementationDataTypes/uint8</TYPE-TREF>
                          <DIRECTION>IN</DIRECTION>
                        </ARGUMENT-DATA-PROTOTYPE>
                      </ARGUMENTS>
                      <POSSIBLE-ERROR-REFS>
                        <!-- although the service does never return E_NOT_OK we need to specify
                             this so that the function prototype of Det_ReportError() generated by
                             the Rte matches those of the Det SWS -->
                        <POSSIBLE-ERROR-REF DEST="APPLICATION-ERROR">[!"$InterfacePath"!]/E_OK</POSSIBLE-ERROR-REF>
                      </POSSIBLE-ERROR-REFS>
                    </CLIENT-SERVER-OPERATION>
                    <CLIENT-SERVER-OPERATION>
                      <SHORT-NAME>ReportRuntimeError</SHORT-NAME>
                      <ARGUMENTS>
                        <ARGUMENT-DATA-PROTOTYPE>
                          <SHORT-NAME>ApiId</SHORT-NAME>
                          <TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_Platform/ImplementationDataTypes/uint8</TYPE-TREF>
                          <DIRECTION>IN</DIRECTION>
                        </ARGUMENT-DATA-PROTOTYPE>
                        <ARGUMENT-DATA-PROTOTYPE>
                          <SHORT-NAME>ErrorId</SHORT-NAME>
                          <TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_Platform/ImplementationDataTypes/uint8</TYPE-TREF>
                          <DIRECTION>IN</DIRECTION>
                        </ARGUMENT-DATA-PROTOTYPE>
                      </ARGUMENTS>
                      <POSSIBLE-ERROR-REFS>
                        <!-- although the service does never return E_NOT_OK we need to specify
                             this so that the function prototype of Det_ReportRuntimeError() generated by
                             the Rte matches those of the Det SWS -->
                        <POSSIBLE-ERROR-REF DEST="APPLICATION-ERROR">[!"$InterfacePath"!]/E_OK</POSSIBLE-ERROR-REF>
                      </POSSIBLE-ERROR-REFS>
                    </CLIENT-SERVER-OPERATION>
[!ENDIF!][!//
                  </OPERATIONS>
                  [!IF "$AsrVersion != 'AUTOSAR_32'"!]
                  <POSSIBLE-ERRORS>
                    <APPLICATION-ERROR>
                      <SHORT-NAME>E_OK</SHORT-NAME>
                      <ERROR-CODE>0</ERROR-CODE>
                    </APPLICATION-ERROR>
                  </POSSIBLE-ERRORS>
                  [!ENDIF!]
                </CLIENT-SERVER-INTERFACE>
              </ELEMENTS>
            </AR-PACKAGE>
[!ENDMACRO!][!//
<?xml version="1.0" encoding="UTF-8"?>
<AUTOSAR xmlns="http://autosar.org/schema/r4.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://autosar.org/schema/r4.0 AUTOSAR_4-0-3_STRICT_COMPACT.xsd">
[!/* do not generate this file if RTE is not used */!][!//
[!SKIPFILE "DetGeneral/DetRteUsage = 'false'"!][!//
  <AR-PACKAGES>
    <AR-PACKAGE>
      <SHORT-NAME>AUTOSAR_Det</SHORT-NAME>
      <ELEMENTS/>
      <AR-PACKAGES>
[!IF "DetGeneral/DetRteUsage = 'true' and DetGeneral/DetServiceAPI/DetEnableASR32ServiceAPI = 'true'"!][!//
        <!-- Provide AUTOSAR 3.2 service interface -->
        <AR-PACKAGE>
          <SHORT-NAME>ASR32</SHORT-NAME>
          <AR-PACKAGES>
           [!CALL "ReportErrorStatus", "Type"="'PortInterfaces'", "AsrVersion"="'AUTOSAR_32'", "InterfaceName"="'DET_ASR32_Service'", "TopLevelPackage"="'/AUTOSAR_Det/ASR32'"!]
           </AR-PACKAGES>
        </AR-PACKAGE>
[!ENDIF!][!//
[!IF "DetGeneral/DetRteUsage = 'true' and DetGeneral/DetServiceAPI/DetEnableASR40ServiceAPI = 'true'"!][!//
        <!-- Provide AUTOSAR 4.0 service interface -->
        <AR-PACKAGE>
          <SHORT-NAME>ASR40</SHORT-NAME>
          <AR-PACKAGES>
            [!CALL "ReportErrorStatus", "Type"="'PortInterfaces'", "AsrVersion"="'AUTOSAR_40'", "InterfaceName"="'DET_ASR40_Service'", "TopLevelPackage"="'/AUTOSAR_Det/ASR40'"!]
          </AR-PACKAGES>
        </AR-PACKAGE>
[!ENDIF!][!//
[!IF "DetGeneral/DetRteUsage = 'true' and DetGeneral/DetServiceAPI/DetEnableASR42ServiceAPI = 'true'"!][!//
        <!-- Provide AUTOSAR 4.2 service interface -->
        <AR-PACKAGE>
          <SHORT-NAME>ASR42</SHORT-NAME>
          <AR-PACKAGES>
            [!CALL "ReportErrorStatus", "Type"="'PortInterfaces'", "AsrVersion"="'AUTOSAR_42'", "InterfaceName"="'DET_ASR42_Service'", "TopLevelPackage"="'/AUTOSAR_Det/ASR42'"!]
          </AR-PACKAGES>
        </AR-PACKAGE>
[!ENDIF!][!//
[!IF "DetGeneral/DetRteUsage = 'true' and DetGeneral/DetServiceAPI/DetEnableASR43ServiceAPI = 'true'"!][!//
        <!-- Provide AUTOSAR 4.3 service interface -->
        <AR-PACKAGE>
          <SHORT-NAME>ASR43</SHORT-NAME>
          <AR-PACKAGES>
            [!CALL "ReportErrorStatus", "Type"="'PortInterfaces'", "AsrVersion"="'AUTOSAR_43'", "InterfaceName"="'DET_ASR43_Service'", "TopLevelPackage"="'/AUTOSAR_Det/ASR43'"!]
          </AR-PACKAGES>
        </AR-PACKAGE>
[!ENDIF!][!//
[!IF "DetGeneral/DetRteUsage = 'true' and DetGeneral/DetServiceAPI/DetDefaultASRServiceAPI != 'NONE'"!][!//
        <!-- Provide default service interface as AUTOSAR 4.3, AUTOSAR 4.2, AUTOSAR 4.0 or AUTOSAR 3.2 -->
        [!CALL "ReportErrorStatus", "Type"="'PortInterfaces'", "AsrVersion"="DetGeneral/DetServiceAPI/DetDefaultASRServiceAPI"!]
[!ENDIF!][!//
      </AR-PACKAGES>
    </AR-PACKAGE>
  </AR-PACKAGES>
</AUTOSAR>
