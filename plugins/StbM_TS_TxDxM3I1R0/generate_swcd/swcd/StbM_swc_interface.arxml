                  </ARGUMENTS>
                  <POSSIBLE-ERROR-REFS>
                    <POSSIBLE-ERROR-REF DEST="APPLICATION-ERROR">/AUTOSAR_StbM/PortInterfaces/StbM_GlobalTime_Slave_[!"$tmpCtrName"!]/E_OK</POSSIBLE-ERROR-REF>
                    <POSSIBLE-ERROR-REF DEST="APPLICATION-ERROR">/AUTOSAR_StbM/PortInterfaces/StbM_GlobalTime_Slave_[!"$tmpCtrName"!]/E_NOT_OK</POSSIBLE-ERROR-REF>
                  </POSSIBLE-ERROR-REFS>
                </CLIENT-SERVER-OPERATION>
[!ENDIF!][!//


[!IF "$StbMTimeRecordingSupport = 'true'"!][!//
[!IF "$isTimeRecordingEnabledOnThisTimeBase = 'true'"!][!//

[!IF "($tmpTimeBaseId < 16)"!][!//
[!// GetSyncTimeRecordHead OPERATION shall be generated only for SYNC time bases, which have Sync TimeRecording activated
                <!-- !LINKSTO StbM.ASR44.SWS_StbM_00247.GetSyncTimeRecordHead,2 -->
                <CLIENT-SERVER-OPERATION>
                  <SHORT-NAME>GetSyncTimeRecordHead</SHORT-NAME>
