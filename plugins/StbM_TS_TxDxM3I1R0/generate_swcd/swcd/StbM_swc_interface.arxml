                    <POSSIBLE-ERROR-REF DEST="APPLICATION-ERROR">/AUTOSAR_StbM/PortInterfaces/StbM_GlobalTime_Slave_[!"$tmpCtrName"!]/E_NOT_OK</POSSIBLE-ERROR-REF>
                  </POSSIBLE-ERROR-REFS>
                </CLIENT-SERVER-OPERATION>

[!IF "$StbMExtendedTimeStampUsed = 'true'"!][!//
                <CLIENT-SERVER-OPERATION>
                  <!-- !LINKSTO StbM.ASR44.SWS_StbM_00247.GetCurrentTimeExtended,1 -->
                  <SHORT-NAME>GetCurrentTimeExtended</SHORT-NAME>
                  <ARGUMENTS>
                    <ARGUMENT-DATA-PROTOTYPE>
                      <SHORT-NAME>timeStampPtr</SHORT-NAME>
                      <TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_StbM/ImplementationDataTypes/StbM_TimeStampExtendedType</TYPE-TREF>
