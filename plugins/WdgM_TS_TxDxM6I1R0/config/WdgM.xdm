              <v:ctr name="WdgMDefensiveProgramming" type="IDENTIFIABLE">
                <a:a name="DESC">
                  <a:v>&lt;html&gt;
                &lt;p&gt;Parameters for defensive programming&lt;/p&gt;
                &lt;/html&gt;</a:v>
                </a:a>
                <a:a name="LABEL" value="Defensive Programming Options"/>
                <v:var name="WdgMDefProgEnabled" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                    &lt;p&gt;Enables or disables the defensive
                    programming feature for the module WdgM.&lt;/p&gt;
                    &lt;p&gt;Note: This feature is dependent on the use of the development error detection module.
                    To use the defensive programming feature, proceed as follows:
                    &lt;ol&gt;
                    &lt;li&gt;Enable development error detection&lt;/li&gt;
                    &lt;li&gt;Enable defensive programming&lt;/li&gt;
                    &lt;li&gt;Enable assertions as required&lt;/li&gt;
                    &lt;/ol&gt;
                    &lt;/p&gt;
                    &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                  </a:a>
                  <a:a name="LABEL" value="Enable Defensive Programming"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:da name="DEFAULT" value="false"/>
                  <a:da name="EDITABLE" type="XPath" 
                        expr="../../WdgMGeneral/WdgMDevErrorDetect = &apos;true&apos;" 
                        true="true" false="false"/>
                </v:var>
                <v:var name="WdgMPrecondAssertEnabled" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                  &lt;p&gt;Enables handling of precondition assertion checks reported from
                  the module WdgM.&lt;/p&gt;
                  &lt;p&gt;Dependency on parameter(s):
                  &lt;ul&gt;
                  &lt;li&gt;Enable Development Error Detection (&lt;code&gt;WdgMDevErrorDetect&lt;/code&gt;): must be enabled &lt;/li&gt;
                  &lt;li&gt;Enable Defensive Programming (&lt;code&gt;WdgMDefProgEnabled&lt;/code&gt;): must be enabled&lt;/li&gt;
                  &lt;/ul&gt;
                  &lt;/p&gt;
                  &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                  </a:a>
                  <a:a name="LABEL" value="Enable Precondition Assertions"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:da name="DEFAULT" value="false"/>
                  <a:da name="EDITABLE" type="XPath" 
                        expr="(../../WdgMGeneral/WdgMDevErrorDetect = &apos;true&apos;) and (../WdgMDefProgEnabled = &apos;true&apos;)" 
                        true="true" false="false"/>
                </v:var>
                <v:var name="WdgMPostcondAssertEnabled" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                  &lt;p&gt;Enables handling of postcondition assertion checks reported from
                  the module WdgM.&lt;/p&gt;
                  &lt;p&gt;Dependency on parameter(s):
                  &lt;ul&gt;
                  &lt;li&gt;Enable Development Error Detection (&lt;code&gt;WdgMDevErrorDetect&lt;/code&gt;): must be enabled &lt;/li&gt;
                  &lt;li&gt;Enable Defensive Programming (&lt;code&gt;WdgMDefProgEnabled&lt;/code&gt;): must be enabled&lt;/li&gt;
                  &lt;/ul&gt;
                  &lt;/p&gt;
                  &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                  </a:a>
                  <a:a name="LABEL" value="Enable Postcondition Assertions"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:da name="DEFAULT" value="false"/>
                  <a:da name="EDITABLE" type="XPath" 
                        expr="(../../WdgMGeneral/WdgMDevErrorDetect = &apos;true&apos;) and (../WdgMDefProgEnabled = &apos;true&apos;)" 
                        true="true" false="false"/>
                </v:var>
                <v:var name="WdgMStaticAssertEnabled" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                  &lt;p&gt;Enables handling of static assertion checks reported from
                  the module WdgM.&lt;/p&gt;
                  &lt;p&gt;Dependency on parameter(s):
                  &lt;ul&gt;
                  &lt;li&gt;Enable Development Error Detection (&lt;code&gt;WdgMDevErrorDetect&lt;/code&gt;): must be enabled &lt;/li&gt;
                  &lt;li&gt;Enable Defensive Programming (&lt;code&gt;WdgMDefProgEnabled&lt;/code&gt;): must be enabled&lt;/li&gt;
                  &lt;/ul&gt;
                  &lt;/p&gt;
                  &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                  </a:a>
                  <a:a name="LABEL" value="Enable Static Assertions"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:da name="DEFAULT" value="false"/>
                  <a:da name="EDITABLE" type="XPath" 
                        expr="(../../WdgMGeneral/WdgMDevErrorDetect = &apos;true&apos;) and (../WdgMDefProgEnabled = &apos;true&apos;)" 
                        true="true" false="false"/>
                </v:var>
                <v:var name="WdgMUnreachAssertEnabled" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                  &lt;p&gt;Enables handling of unreachable code assertion checks
                  reported from the module WdgM.&lt;/p&gt;
                  &lt;p&gt;Dependency on parameter(s):
                  &lt;ul&gt;
                  &lt;li&gt;Enable Development Error Detection (&lt;code&gt;WdgMDevErrorDetect&lt;/code&gt;): must be enabled &lt;/li&gt;
                  &lt;li&gt;Enable Defensive Programming (&lt;code&gt;WdgMDefProgEnabled&lt;/code&gt;): must be enabled&lt;/li&gt;
                  &lt;/ul&gt;
                  &lt;/p&gt;
                  &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                  </a:a>
                  <a:a name="LABEL" value="Enable Unreachable Code Assertions"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:da name="DEFAULT" value="false"/>
                  <a:da name="EDITABLE" type="XPath" 
                        expr="(../../WdgMGeneral/WdgMDevErrorDetect = &apos;true&apos;) and (../WdgMDefProgEnabled = &apos;true&apos;)" 
                        true="true" false="false"/>
                </v:var>
                <v:var name="WdgMInvariantAssertEnabled" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                  &lt;p&gt;Enables handling of invariant assertion checks
                  reported from functions of the module WdgM.&lt;/p&gt;
                  &lt;p&gt;Dependency on parameter(s):
                  &lt;ul&gt;
                  &lt;li&gt;Enable Development Error Detection (&lt;code&gt;WdgMDevErrorDetect&lt;/code&gt;): must be enabled &lt;/li&gt;
                  &lt;li&gt;Enable Defensive Programming (&lt;code&gt;WdgMDefProgEnabled&lt;/code&gt;): must be enabled&lt;/li&gt;
                  &lt;/ul&gt;
                  &lt;/p&gt;
                  &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                  </a:a>
                  <a:a name="LABEL" value="Enable Invariant Assertions"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:da name="DEFAULT" value="false"/>
                  <a:da name="EDITABLE" type="XPath" 
                        expr="(../../WdgMGeneral/WdgMDevErrorDetect = &apos;true&apos;) and (../WdgMDefProgEnabled = &apos;true&apos;)" 
                        true="true" false="false"/>
                </v:var>
              </v:ctr>
              <v:ctr name="ReportToDem" type="IDENTIFIABLE">
                <a:a name="DESC">
                  <a:v>&lt;html&gt;
                      Production error handling
                    &lt;/html&gt;</a:v>
                </a:a>
                <a:a name="LABEL" value="Production error handling"/>
                <a:a name="TAB" value="Optimization"/>
                <a:da name="TITLE" value="Production error handling"/>
                <v:var name="WdgMSupervisionReportToDem" type="ENUMERATION">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                        &lt;p&gt;Selects the handling of the production error:
                        &lt;em&gt;WDGM_E_MONITORING (formerly WDGM_E_SUPERVISION)&lt;/em&gt;&lt;/p&gt;
                        &lt;ul&gt;
                        &lt;li&gt;&lt;code&gt;DEM&lt;/code&gt;: The production error WDGM_E_MONITORING is reported to the Diagnostics Event Manager (Dem).&lt;/li&gt;
                        &lt;li&gt;&lt;code&gt;DET&lt;/code&gt;: The production error WDGM_E_MONITORING is reported to the Development Error Tracer (Det) if enabled.&lt;/li&gt;
                        &lt;li&gt;&lt;code&gt;DISABLE&lt;/code&gt;: The production error WDGM_E_MONITORING is not reported at all.&lt;/li&gt;
                        &lt;/ul&gt;
                      &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPreCompile</icc:v>
                  </a:a>
                  <a:a name="LABEL" value="WdgM Supervision Failure"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="TAB" value="Optimization"/>
                  <a:da name="DEFAULT" value="DEM"/>
                  <a:da name="ENABLE" type="XPath" 
                        expr="../../WdgMGeneral/WdgMDemStoppedSupervisionReport != &apos;false&apos;"/>
                  <a:da name="RANGE">
                    <a:v>DEM</a:v>
                    <a:v>DET</a:v>
                    <a:v>DISABLE</a:v>
                  </a:da>
                </v:var>
                <v:var name="WdgMSupervisionDemDetErrId" type="INTEGER">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;

                          &lt;p&gt;If a production error is reported towards
                          the Det, this parameter defines the error id
                          which is reported towards the Det.&lt;/p&gt;

                          &lt;p&gt;The preprocessor define
                          WDGM_E_DEMTODET_E_SUPERVISION is generated
                          holding the value of
                          &lt;code&gt;WdgMSupervisionDemDetErrId&lt;/code&gt;.&lt;/p&gt;

                        &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPreCompile</icc:v>
                  </a:a>
                  <a:a name="LABEL" 
                       value="WdgM Supervision Failure DemToDet ErrorId"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:da name="DEFAULT" value="30"/>
                  <a:da name="ENABLE" type="XPath" 
                        expr="node:exists(../WdgMSupervisionReportToDem) and (../WdgMSupervisionReportToDem = &apos;DET&apos;)"/>
                  <a:da name="INVALID" type="Range">
                    <a:tst expr="&lt;=255"/>
                    <a:tst expr="&gt;=30"/>
                  </a:da>
                  <a:da name="WARNING" type="XPath" expr=". &gt;= 30" 
                        false="Values from 0 to 30 are used as standard IDs according to AUTOSAR SWS."/>
                </v:var>
                <v:var name="WdgMSetModeReportToDem" type="ENUMERATION">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                        &lt;p&gt;Selects the handling of the production error:
                        &lt;em&gt;WDGM_E_SET_MODE&lt;/em&gt;&lt;/p&gt;
                        &lt;ul&gt;
                        &lt;li&gt;&lt;code&gt;DEM&lt;/code&gt;: The production error WDGM_E_SET_MODE is reported to the Diagnostics Event Manager (Dem).&lt;/li&gt;
                        &lt;li&gt;&lt;code&gt;DET&lt;/code&gt;: The production error WDGM_E_SET_MODE is reported to the Development Error Tracer (Det) if enabled.&lt;/li&gt;
                        &lt;li&gt;&lt;code&gt;DISABLE&lt;/code&gt;:  The production error WDGM_E_SET_MODE is not reported at all.&lt;/li&gt;
                        &lt;/ul&gt;
                      &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                  </a:a>
                  <a:a name="LABEL" value="WdgM SetMode Failure"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="TAB" value="Optimization"/>
                  <a:da name="DEFAULT" value="DEM"/>
                  <a:da name="RANGE">
                    <a:v>DEM</a:v>
                    <a:v>DET</a:v>
                    <a:v>DISABLE</a:v>
                  </a:da>
                </v:var>
                <v:var name="WdgMSetModeDemDetErrId" type="INTEGER">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;

                          &lt;p&gt;If a production error is reported towards
                          the Det, this parameter defines the error id
                          which is reported towards the Det.&lt;/p&gt;

                          &lt;p&gt;The preprocessor define
                          WDGM_EB_E_DEMTODET_SET_MODE is generated
                          holding the value of
                          &lt;code&gt;WdgMSetModeDemDetErrId&lt;/code&gt;.&lt;/p&gt;

                        &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPreCompile</icc:v>
                  </a:a>
                  <a:a name="LABEL" 
                       value="WdgM SetMode Failure DemToDet ErrorId"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:da name="DEFAULT" value="30"/>
                  <a:da name="ENABLE" type="XPath" 
                        expr="node:exists(../WdgMSetModeReportToDem) and (../WdgMSetModeReportToDem = &apos;DET&apos;)"/>
                  <a:da name="INVALID" type="Range">
                    <a:tst expr="&lt;=255"/>
                    <a:tst expr="&gt;=30"/>
                  </a:da>
                  <a:da name="WARNING" type="XPath" expr=". &gt;= 30" 
                        false="Values from 0 to 30 are used as standard IDs according to AUTOSAR SWS."/>
                </v:var>
                <v:var name="WdgMImproperCallerReportToDem" type="ENUMERATION">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                        &lt;p&gt;Selects the handling of the production error:
                        &lt;em&gt;WDGM_E_IMPROPER_CALLER&lt;/em&gt;&lt;/p&gt;
                        &lt;ul&gt;
                        &lt;li&gt;&lt;code&gt;DEM&lt;/code&gt;: The production error WDGM_E_IMPROPER_CALLER is reported to the Diagnostics Event Manager (Dem).&lt;/li&gt;
                        &lt;li&gt;&lt;code&gt;DET&lt;/code&gt;: The production error WDGM_E_IMPROPER_CALLER is reported to the Development Error Tracer (Det) if enabled.&lt;/li&gt;
                        &lt;li&gt;&lt;code&gt;DISABLE&lt;/code&gt;: The production error WDGM_E_IMPROPER_CALLER is not reported at all.&lt;/li&gt;
                        &lt;/ul&gt;
                      &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPreCompile</icc:v>
                  </a:a>
                  <a:a name="LABEL" value="WdgM ImproperCaller Failure"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="TAB" value="Optimization"/>
                  <a:da name="DEFAULT" value="DEM"/>
                  <a:da name="ENABLE" type="XPath" 
                        expr="(node:value(../../WdgMGeneral/WdgMBSWCompatibilityMode) = &apos;AUTOSAR_40&apos;)                     and not(node:exists(../../WdgMGeneral/WdgMSupervisorCallouts/WdgMGetExpectedWdgMModeCallout))                     and node:exists(../../WdgMGeneral/WdgMCallerIds)"/>
                  <a:da name="RANGE">
                    <a:v>DEM</a:v>
                    <a:v>DET</a:v>
                    <a:v>DISABLE</a:v>
                  </a:da>
                </v:var>
                <v:var name="WdgMImproperCallerDemDetErrId" type="INTEGER">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;

                          &lt;p&gt;If a production error is reported towards
                          the Det, this parameter defines the error id
                          which is reported towards the Det.&lt;/p&gt;

                          &lt;p&gt;The preprocessor define
                          WDGM_EB_E_DEMTODET_IMPROPER_CALLER is generated
                          holding the value of
                          &lt;code&gt;WdgMImproperCallerDemDetErrId&lt;/code&gt;.&lt;/p&gt;

                        &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPreCompile</icc:v>
                  </a:a>
                  <a:a name="LABEL" 
                       value="WdgM ImproperCaller Failure DemToDet ErrorId"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:da name="DEFAULT" value="30"/>
                  <a:da name="ENABLE" type="XPath" 
                        expr="node:exists(../WdgMImproperCallerReportToDem) and (../WdgMImproperCallerReportToDem = &apos;DET&apos;)"/>
                  <a:da name="INVALID" type="Range">
                    <a:tst expr="&lt;=255"/>
                    <a:tst expr="&gt;=30"/>
                  </a:da>
                  <a:da name="WARNING" type="XPath" expr=". &gt;= 30" 
                        false="Values from 0 to 30 are used as standard IDs according to AUTOSAR SWS."/>
                </v:var>
                <v:var name="WdgMMFTimingViolationReportToDem" 
                       type="ENUMERATION">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                        &lt;p&gt;Selects the handling of the production error:
                        &lt;em&gt;WDGM_E_MF_TIMINGVIOLATION&lt;/em&gt;&lt;/p&gt;
                        &lt;ul&gt;
                        &lt;li&gt;&lt;code&gt;DEM&lt;/code&gt;: The production error WDGM_E_MF_TIMINGVIOLATION is reported to the Diagnostics Event Manager (Dem).&lt;/li&gt;
                        &lt;li&gt;&lt;code&gt;DET&lt;/code&gt;: The production error WDGM_E_MF_TIMINGVIOLATION is reported to the Development Error Tracer (Det) if enabled.&lt;/li&gt;
                        &lt;li&gt;&lt;code&gt;DISABLE&lt;/code&gt;: The production error WDGM_E_MF_TIMINGVIOLATION is not reported at all.&lt;/li&gt;
                        &lt;/ul&gt;
                      &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPreCompile</icc:v>
                  </a:a>
                  <a:a name="LABEL" value="WdgM MainFunction Timing Failure"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="TAB" value="Optimization"/>
                  <a:da name="DEFAULT" value="DEM"/>
                  <a:da name="ENABLE" type="XPath" 
                        expr="num:i(count(../../WdgMConfigSet/*[1]/WdgMMode/*/WdgMDeadlineSupervision/*)) != 0"/>
                  <a:da name="RANGE">
                    <a:v>DEM</a:v>
                    <a:v>DET</a:v>
                    <a:v>DISABLE</a:v>
                  </a:da>
                </v:var>
                <v:var name="WdgMMFTimingViolationDemDetErrId" type="INTEGER">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;

                          &lt;p&gt;If a production error is reported towards
                          the Det, this parameter defines the error id
                          which is reported towards the Det.&lt;/p&gt;

                          &lt;p&gt;The preprocessor define
                          WDGM_E_DEMTODET_E_MF_TIMINGVIOLATION is generated
                          holding the value of
                          &lt;code&gt;WdgMMFTimingViolationDemDetErrId&lt;/code&gt;.&lt;/p&gt;

                        &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPreCompile</icc:v>
                  </a:a>
                  <a:a name="LABEL" 
                       value="WdgM MainFunction Timing Failure DemToDet ErrorId"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:da name="DEFAULT" value="30"/>
                  <a:da name="ENABLE" type="XPath" 
                        expr="node:exists(../WdgMMFTimingViolationReportToDem) and (../WdgMMFTimingViolationReportToDem = &apos;DET&apos;)"/>
                  <a:da name="INVALID" type="Range">
                    <a:tst expr="&lt;=255"/>
                    <a:tst expr="&gt;=30"/>
                  </a:da>
                  <a:da name="WARNING" type="XPath" expr=". &gt;= 30" 
                        false="Values from 0 to 30 are used as standard IDs according to AUTOSAR SWS."/>
                </v:var>
                <v:var name="WdgMDataCorruptionReportToDem" type="ENUMERATION">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                        &lt;p&gt;Selects the handling of the production error:
                        &lt;em&gt;WDGM_E_DATA_CORRUPTION&lt;/em&gt;&lt;/p&gt;
                        &lt;ul&gt;
                        &lt;li&gt;&lt;code&gt;DEM&lt;/code&gt;: The production error WDGM_E_DATA_CORRUPTION is reported to the Diagnostics Event Manager (Dem).&lt;/li&gt;
                        &lt;li&gt;&lt;code&gt;DET&lt;/code&gt;: The production error WDGM_E_DATA_CORRUPTION is reported to the Development Error Tracer (Det) if enabled.&lt;/li&gt;
                        &lt;li&gt;&lt;code&gt;DISABLE&lt;/code&gt;: The production error WDGM_E_DATA_CORRUPTION is not reported at all.&lt;/li&gt;
                        &lt;/ul&gt;
                      &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                  </a:a>
                  <a:a name="LABEL" value="WdgM Data Corruption Failure"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="TAB" value="Optimization"/>
                  <a:da name="DEFAULT" value="DEM"/>
                  <a:da name="RANGE">
                    <a:v>DEM</a:v>
                    <a:v>DET</a:v>
                    <a:v>DISABLE</a:v>
                  </a:da>
                </v:var>
                <v:var name="WdgMDataCorruptionDemDetErrId" type="INTEGER">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;

                          &lt;p&gt;If a production error is reported towards
                          the Det, this parameter defines the error id
                          which is reported towards the Det.&lt;/p&gt;

                          &lt;p&gt;The preprocessor define
                          WDGM_E_DEMTODET_E_DATA_CORRUPTION is generated
                          holding the value of
                          &lt;code&gt;WdgMDataCorruptionDemDetErrId&lt;/code&gt;.&lt;/p&gt;

                        &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPreCompile</icc:v>
                  </a:a>
                  <a:a name="LABEL" 
                       value="WdgM Data Corruption Failure DemToDet ErrorId"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:da name="DEFAULT" value="30"/>
                  <a:da name="ENABLE" type="XPath" 
                        expr="node:exists(../WdgMDataCorruptionReportToDem) and (../WdgMDataCorruptionReportToDem = &apos;DET&apos;)"/>
                  <a:da name="INVALID" type="Range">
                    <a:tst expr="&lt;=255"/>
                    <a:tst expr="&gt;=30"/>
                  </a:da>
                  <a:da name="WARNING" type="XPath" expr=". &gt;= 30" 
                        false="Values from 0 to 30 are used as standard IDs according to AUTOSAR SWS."/>
                </v:var>
              </v:ctr>
              <d:ref type="REFINED_MODULE_DEF" 
                     value="ASPath:/AUTOSAR/EcucDefs/WdgM"/>
              <v:lst name="WdgMConfigSet" 
                     type="MULTIPLE-CONFIGURATION-CONTAINER">
                <a:da name="INVALID" type="XPath" expr="count(./*) = 1" 
                      false="Only one config set is allowed in this release."/>
                <a:da name="MIN" value="1"/>
                <v:ctr name="WdgMConfigSet" 
                       type="MULTIPLE-CONFIGURATION-CONTAINER">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                        This container describes one of multiple configuration sets of WdgM.
                        This is a MultipleConfigurationContainer,
                        i.e. this container and its sub-containers exist once per configuration set.
                      &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="NAME_PATTERN" value="WdgM_Config_?"/>
                  <a:a name="UUID" 
                       value="ECUC:df6338ce-c64d-4457-9304-67ac2b1aa516"/>
                  <a:da name="INVALID" type="XPath" 
                        expr="text:match(node:name(.), &apos;^[a-zA-Z_][a-zA-Z0-9_]*$&apos;)" 
                        false="WdgMConfigSet container name must be a valid C identifier."/>
                  <v:ref name="WdgMInitialMode" type="SYMBOLIC-NAME-REFERENCE">
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                        The mode that the Watchdog Manager is in after it has been initialized.
                      &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                    </a:a>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="UUID" 
                         value="ECUC:0d4155c0-613a-4132-a555-e2454061c74c"/>
                    <a:da name="INVALID" type="XPath" expr="node:refvalid(.)" 
                          false="An initial mode must be referenced."/>
                    <a:da name="REF" 
                          value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/WdgM/WdgMConfigSet/WdgMMode"/>
                  </v:ref>
                  <v:ctr name="WdgMDemEventParameterRefs" type="IDENTIFIABLE">
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                        Container for the references to DemEventParameter elements which shall be
                        invoked using the API Dem_ReportErrorStatus API in case the corresponding
                        error occurs. The EventId is taken from the referenced
                        DemEventParameter&apos;s DemEventId value. The standardized errors are
                        provided in the container and can be extended by vendor specific
                        error references.
                      &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="UUID" 
                         value="ECUC:83469853-9485-490e-b532-f9d523b81681"/>
                    <v:ref name="WDGM_E_DATA_CORRUPTION" 
                           type="SYMBOLIC-NAME-REFERENCE">
                      <a:a name="DESC">
                        <a:v>&lt;html&gt;
                        Reference to the DemEventParameter that shall be issued when data
                        corruption is detected in the internal WdgM data, which is stored
                        double inverse.
                        &lt;p&gt;Dependency on parameter(s):&lt;/p&gt;
                        &lt;ul&gt;
                        &lt;li&gt;&lt;code&gt;WdgMDataCorruptionReportToDem&lt;/code&gt;: Select DEM to enable the reporting of &lt;code&gt;WDGM_E_DATA_CORRUPTION&lt;/code&gt;.&lt;/li&gt;
                        &lt;/ul&gt;
                        &lt;p&gt;Further notes:&lt;/p&gt;
                          &lt;ul&gt;
                            &lt;li&gt;Activation: Thrown, if data corruption is detected in the internal WdgM data.&lt;/li&gt;
                            &lt;li&gt;Healing: None. The error resides in memory until it is deleted.&lt;/li&gt;
                            &lt;li&gt;Trigger debounce: None. The error is reported on first occurrence.&lt;/li&gt;
                            &lt;li&gt;Rate of diagnostic checks: Checked on every call of the service that reports this error.&lt;/li&gt;
                          &lt;/ul&gt;
                      &lt;/html&gt;</a:v>
                      </a:a>
                      <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                           type="IMPLEMENTATIONCONFIGCLASS">
                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                      </a:a>
                      <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                      <a:a name="UUID" 
                           value="ECUC:1da637f0-d7f8-11e2-a28f-0800200c9a66"/>
                      <a:da name="ENABLE" type="XPath" 
                            expr="node:exists(../../../../ReportToDem/WdgMDataCorruptionReportToDem) and (../../../../ReportToDem/WdgMDataCorruptionReportToDem = &apos;DEM&apos;)"/>
                      <a:da name="INVALID" type="XPath" expr="node:refvalid(.)" 
                            false="WdgM requires the configuration of the DEM event WDGM_E_DATA_CORRUPTION in the DEM plugin."/>
                      <a:da name="REF" 
                            value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/Dem/DemConfigSet/DemEventParameter"/>
                      <a:da name="WARNING" type="XPath" 
                            expr="(node:refvalid(.)) and (as:ref(.)/@name != &apos;WDGM_E_DATA_CORRUPTION&apos;)" 
                            true="The referenced DEM event does not match WDGM_E_DATA_CORRUPTION."/>
                    </v:ref>
                    <v:ref name="WDGM_E_IMPROPER_CALLER" 
                           type="SYMBOLIC-NAME-REFERENCE">
                      <a:a name="DESC">
                        <a:v>&lt;html&gt;
                        Reference to the DemEventParameter that shall be issued when the defensive
                        behavior checks detect an improper caller.
                        &lt;p&gt;Dependency on parameter(s):&lt;/p&gt;
                        &lt;ul&gt;
                        &lt;li&gt;&lt;code&gt;WdgMImproperCallerReportToDem&lt;/code&gt;: Select DEM to enable the reporting of &lt;code&gt;WDGM_E_IMPROPER_CALLER&lt;/code&gt;.&lt;/li&gt;
                        &lt;/ul&gt;
                        &lt;p&gt;Further notes:&lt;/p&gt;
                        &lt;ul&gt;
                          &lt;li&gt;Activation: Thrown, if the passed CallerID is not in the list of the
                          configured list of allowed CallerIDs.&lt;/li&gt;
                          &lt;li&gt;Healing: None. The error resides in memory until it is deleted.&lt;/li&gt;
                          &lt;li&gt;Trigger debounce: None. The error is reported on first occurrence.&lt;/li&gt;
                          &lt;li&gt;Rate of diagnostic checks: Checked on every call of the service that reports this error.&lt;/li&gt;
                        &lt;/ul&gt;
                      &lt;/html&gt;</a:v>
                      </a:a>
                      <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                           type="IMPLEMENTATIONCONFIGCLASS">
                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                      </a:a>
                      <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                      <a:a name="UUID" 
                           value="ECUC:2388851c-3bee-4dcc-b570-cb9d0db92777"/>
                      <a:da name="ENABLE" type="XPath" 
                            expr="node:exists(../../../../ReportToDem/WdgMImproperCallerReportToDem) and (../../../../ReportToDem/WdgMImproperCallerReportToDem = &apos;DEM&apos;)"/>
                      <a:da name="INVALID" type="XPath" expr="node:refvalid(.)" 
                            false="WdgM requires the configuration of the DEM event WDGM_E_IMPROPER_CALLER in the DEM plugin."/>
                      <a:da name="REF" 
                            value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/Dem/DemConfigSet/DemEventParameter"/>
                      <a:da name="WARNING" type="XPath" 
                            expr="(node:refvalid(.)) and (as:ref(.)/@name != &apos;WDGM_E_IMPROPER_CALLER&apos;)" 
                            true="The referenced DEM event does not match WDGM_E_IMPROPER_CALLER."/>
                    </v:ref>
                    <v:ref name="WDGM_E_MF_TIMINGVIOLATION" 
                           type="SYMBOLIC-NAME-REFERENCE">
                      <a:a name="DESC">
                        <a:v>&lt;html&gt;
                        Reference to the DemEventParameter that shall be issued when the
                        actual Watchdog Manager main function period deviates from the configured
                        mode-dependent schedule period (&lt;code&gt;WdgMSupervisionCycle&lt;/code&gt;).
                        &lt;p&gt;Dependency on parameter(s):&lt;/p&gt;
                        &lt;ul&gt;
                        &lt;li&gt;&lt;code&gt;WdgMMFTimingViolationReportToDem&lt;/code&gt;: Select DEM to enable the reporting of &lt;code&gt;WDGM_E_MF_TIMINGVIOLATION&lt;/code&gt;.&lt;/li&gt;
                        &lt;/ul&gt;
                        &lt;p&gt;Further notes:&lt;/p&gt;
                          &lt;ul&gt;
                            &lt;li&gt;Activation: Thrown, if the &lt;code&gt;WdgM_MainFunction()&lt;/code&gt; period deviates from the
                            configured mode-dependent schedule period (&lt;code&gt;WdgMSupervisionCycle&lt;/code&gt;).&lt;/li&gt;
                            &lt;li&gt;Healing: None. The error resides in memory until it is deleted.&lt;/li&gt;
                            &lt;li&gt;Trigger debounce: None. The error is reported on first occurrence.&lt;/li&gt;
                            &lt;li&gt;Rate of diagnostic checks: Checked cyclically within &lt;code&gt;WdgM_MainFunction()&lt;/code&gt;.&lt;/li&gt;
                          &lt;/ul&gt;
                      &lt;/html&gt;</a:v>
                      </a:a>
                      <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                           type="IMPLEMENTATIONCONFIGCLASS">
                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                      </a:a>
                      <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                      <a:a name="UUID" 
                           value="ECUC:a07d71f0-872d-11e2-9e96-0800200c9a66"/>
                      <a:da name="ENABLE" type="XPath" 
                            expr="node:exists(../../../../ReportToDem/WdgMMFTimingViolationReportToDem) and (../../../../ReportToDem/WdgMMFTimingViolationReportToDem = &apos;DEM&apos;)"/>
                      <a:da name="INVALID" type="XPath" expr="node:refvalid(.)" 
                            false="WdgM requires the configuration of the DEM event WDGM_E_MF_TIMINGVIOLATION in the DEM plugin."/>
                      <a:da name="REF" 
                            value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/Dem/DemConfigSet/DemEventParameter"/>
                      <a:da name="WARNING" type="XPath" 
                            expr="(node:refvalid(.)) and (as:ref(.)/@name != &apos;WDGM_E_MF_TIMINGVIOLATION&apos;)" 
                            true="The referenced DEM event does not match WDGM_E_MF_TIMINGVIOLATION."/>
                    </v:ref>
                    <v:ref name="WDGM_E_MONITORING" 
                           type="SYMBOLIC-NAME-REFERENCE">
                      <a:a name="DESC">
                        <a:v>&lt;html&gt;
                        Reference to the DemEventParameter that shall be issued when the following error occurs:
                        &lt;em&gt;Monitoring has failed and a watchdog reset will occur.&lt;/em&gt;
                        &lt;p&gt;Dependency on parameter(s):&lt;/p&gt;
                        &lt;ul&gt;
                        &lt;li&gt;&lt;code&gt;WdgMSupervisionReportToDem&lt;/code&gt;: Select DEM to enable the reporting of &lt;code&gt;WDGM_E_MONITORING&lt;/code&gt;.&lt;/li&gt;
                        &lt;/ul&gt;
                        &lt;p&gt;Further notes:&lt;/p&gt;
                          &lt;ul&gt;
                            &lt;li&gt;Activation: Thrown, if supervision fails for a supervised entity.&lt;/li&gt;
                            &lt;li&gt;Healing: None. The error resides in memory until it is deleted.&lt;/li&gt;
                            &lt;li&gt;Trigger debounce: None. The error is reported on first occurrence.&lt;/li&gt;
                            &lt;li&gt;Rate of diagnostic checks: Checked cyclically within &lt;code&gt;WdgM_MainFunction()&lt;/code&gt;.&lt;/li&gt;
                          &lt;/ul&gt;
                      &lt;/html&gt;</a:v>
                      </a:a>
                      <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                           type="IMPLEMENTATIONCONFIGCLASS">
                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                      </a:a>
                      <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                      <a:a name="UUID" 
                           value="ECUC:80360d1f-e977-41a3-a996-ecc2d8af4780"/>
                      <a:da name="ENABLE" type="XPath" 
                            expr="node:exists(../../../../ReportToDem/WdgMSupervisionReportToDem) and (../../../../ReportToDem/WdgMSupervisionReportToDem = &apos;DEM&apos;)"/>
                      <a:da name="INVALID" type="XPath" expr="node:refvalid(.)" 
                            false="WdgM requires the configuration of the DEM event WDGM_E_MONITORING in the DEM plugin."/>
                      <a:da name="REF" 
                            value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/Dem/DemConfigSet/DemEventParameter"/>
                      <a:da name="WARNING" type="XPath" 
                            expr="(node:refvalid(.)) and (as:ref(.)/@name != &apos;WDGM_E_MONITORING&apos;)" 
                            true="The referenced DEM event does not match WDGM_E_MONITORING."/>
                    </v:ref>
                    <v:ref name="WDGM_E_SET_MODE" 
                           type="SYMBOLIC-NAME-REFERENCE">
                      <a:a name="DESC">
                        <a:v>&lt;html&gt;
                        Reference to the DemEventParameter that shall be issued when the following error occurs:
                        &lt;em&gt;Watchdog drivers&apos; mode switch has failed.&lt;/em&gt;
                        &lt;p&gt;Dependency on parameter(s):&lt;/p&gt;
                        &lt;ul&gt;
                        &lt;li&gt;&lt;code&gt;WdgMSetModeReportToDem&lt;/code&gt;: Select DEM to enable the reporting of &lt;code&gt;WDGM_E_SET_MODE&lt;/code&gt;.&lt;/li&gt;
                        &lt;/ul&gt;
                        &lt;p&gt;Further notes:&lt;/p&gt;
                          &lt;ul&gt;
                            &lt;li&gt;Activation: Thrown, if watchdog drivers&apos; mode switch has failed.&lt;/li&gt;
                            &lt;li&gt;Healing: None. The error resides in memory until it is deleted.&lt;/li&gt;
                            &lt;li&gt;Trigger debounce: None. The error is reported on first occurrence.&lt;/li&gt;
                            &lt;li&gt;Rate of diagnostic checks: Checked cyclically within &lt;code&gt;WdgM_MainFunction()&lt;/code&gt;.&lt;/li&gt;
                          &lt;/ul&gt;
                      &lt;/html&gt;</a:v>
                      </a:a>
                      <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                           type="IMPLEMENTATIONCONFIGCLASS">
                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                      </a:a>
                      <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                      <a:a name="UUID" 
                           value="ECUC:e878340c-b52e-436b-9573-9605d3937379"/>
                      <a:da name="ENABLE" type="XPath" 
                            expr="node:exists(../../../../ReportToDem/WdgMSetModeReportToDem) and (../../../../ReportToDem/WdgMSetModeReportToDem = &apos;DEM&apos;)"/>
                      <a:da name="INVALID" type="XPath" expr="node:refvalid(.)" 
                            false="WdgM requires the configuration of the DEM event WDGM_E_MONITORING in the DEM plugin."/>
                      <a:da name="REF" 
                            value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/Dem/DemConfigSet/DemEventParameter"/>
                      <a:da name="WARNING" type="XPath" 
                            expr="(node:refvalid(.)) and (as:ref(.)/@name != &apos;WDGM_E_SET_MODE&apos;)" 
                            true="The referenced DEM event does not match WDGM_E_SET_MODE."/>
                    </v:ref>
                  </v:ctr>
                  <v:lst name="WdgMMode" type="MAP">
                    <a:da name="MAX" value="255"/>
                    <a:da name="MIN" value="1"/>
                    <v:ctr name="WdgMMode" type="IDENTIFIABLE">
                      <a:a name="DESC">
                        <a:v>&lt;html&gt;
                        The container describes one of several modes of the Watchdog Manager.
                      &lt;/html&gt;</a:v>
                      </a:a>
                      <a:a name="UUID" 
                           value="ECUC:57abb8dd-f2ae-4f7a-995f-5a102c125824"/>
                      <v:var name="WdgMExpiredSupervisionCycleTol" 
                             type="INTEGER">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                        This parameter shall be used to define a value that fixes the amount
                        of expired supervision cycles for how long the blocking of
                        watchdog triggering shall be postponed,
                        AFTER THE GLOBAL SUPERVISION STATUS HAS REACHED THE STATE EXPIRED.
                      &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" 
                             value="ECUC:5527db00-defa-42f3-9b17-bf4ecbd64092"/>
                        <a:da name="DEFAULT" value="5"/>
                        <a:da name="INVALID" type="Range">
                          <a:tst expr="&lt;=65535"/>
                          <a:tst expr="&gt;=0"/>
                        </a:da>
                      </v:var>
                      <v:var name="WdgMModeId" type="INTEGER">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                        This parameter fixes the identfier for the mode.
                        This identifier is for instance passed as a parameter to the
                        WdgM_SetMode service.
                      &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                        </a:a>
                        <a:a name="LABEL" value="WdgMModeId (0 -&gt; 255)"/>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="true"/>
                        <a:a name="UUID" 
                             value="ECUC:2006dc2d-a878-4367-a2b2-b36b855468aa"/>
                        <a:da name="DEFAULT" type="XPath" 
                              expr="num:i(../@index)"/>
                        <a:da name="INVALID" type="Multi">
                          <mt:range>
                            <mt:tst expr="&lt;=255"/>
                            <mt:tst expr="&gt;=0"/>
                          </mt:range>
                          <mt:xpath>
                            <mt:tst 
                                    false="IDs must be zero-based and consecutive. This value is not unique." 
                                    expr="text:uniq(../../*/WdgMModeId, .)"/>
                            <mt:tst 
                                    false="IDs must be zero-based and consecutive. The value one less than this value is missing." 
                                    expr="   (. = 0)                             or ( count(../../*[WdgMModeId = (node:current() - 1)])                             &gt; 0)"/>
                          </mt:xpath>
                        </a:da>
                      </v:var>
                      <v:var name="WdgMSupervisionCycle" type="FLOAT">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                        This parameter defines the schedule period of the main function
                        WdgM_MainFunction.    Unit: [s]
                      &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" 
                             value="ECUC:eb454bd1-fb86-45cc-9196-8f4e37f40091"/>
                        <a:da name="DEFAULT" value="0.01"/>
                        <a:da name="INVALID" type="Multi">
                          <mt:range>
                            <mt:tst expr="&lt;=65535"/>
                            <mt:tst expr="&gt;=0"/>
                          </mt:range>
                          <mt:xpath>
                            <mt:tst 
                                    true="The schedule period must be a positive float value." 
                                    expr="node:value(.) &lt;= 0"/>
                            <mt:tst 
                                    true="All WdgM Modes must have the same schedule period of the main function. This value differs from the schedule period of the previously configured WdgM Mode." 
                                    expr="(num:i(../@index) &gt; 0) and                               (node:value(.) != ../../*[num:i(node:current()/../@index)]/WdgMSupervisionCycle)"/>
                            <mt:tst 
                                    true="The configured value must be greater than or equal to the time granularity provided via parameter WdgMTimeGranularity." 
                                    expr="node:exists(../../../../../WdgMGeneral/WdgMSupervisorCallouts/WdgMTimeGranularity)                               and (node:value(.) &lt; ../../../../../WdgMGeneral/WdgMSupervisorCallouts/WdgMTimeGranularity)"/>
                            <mt:tst 
                                    true="The configured value must be a multiple of the time granularity provided via parameter WdgMTimeGranularity." 
                                    expr="node:exists(../../../../../WdgMGeneral/WdgMSupervisorCallouts/WdgMTimeGranularity)                               and (num:mod(round(node:value(.) * &apos;1E10&apos;), round(node:value(../../../../../WdgMGeneral/WdgMSupervisorCallouts/WdgMTimeGranularity) * &apos;1E10&apos;)) != 0)"/>
                          </mt:xpath>
                        </a:da>
                      </v:var>
                      <v:lst name="WdgMAliveSupervision" type="MAP">
                        <a:da name="MAX" value="65535"/>
                        <v:ctr name="WdgMAliveSupervision" type="IDENTIFIABLE">
                          <a:a name="DESC">
                            <a:v>&lt;html&gt;
                        This container collects all configuration parameters of Alive-Supervision
                        of one Checkpoint. Note that each Checkpoint may have different parameters.
                        For example, it may have different min and max margin.
                      &lt;/html&gt;</a:v>
                          </a:a>
                          <a:a name="UUID" 
                               value="ECUC:c44ab53e-7a86-4f12-9c9c-f5a1b0a69e8c"/>
                          <a:da name="INVALID" type="XPath" 
                                expr="(var:set(&apos;SeId&apos;, as:ref(./WdgMAliveSupervisionCheckpointRef)/../../WdgMSupervisedEntityId) = &apos;true&apos;)                               and (count(../../WdgMLocalStatusParams/*[as:ref(./WdgMLocalStatusSupervisedEntityRef)/WdgMSupervisedEntityId = $SeId]) &gt; 0)" 
                                false="WdgMFailedAliveSupervisionRefCycleTol is not configured for the Supervised Entity of this Checkpoint in container WdgMLocalStatusParams."/>
                          <v:var name="WdgMExpectedAliveIndications" 
                                 type="INTEGER">
                            <a:a name="DESC">
                              <a:v>&lt;html&gt;
                        This parameter contains the amount of expected alive indications of the
                        Checkpoint within the referenced amount of defined supervision cycles
                        according to corresponding SE.
                      &lt;/html&gt;</a:v>
                            </a:a>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                 type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                            </a:a>
                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID" 
                                 value="ECUC:0adf20c6-581b-482d-943b-d868a65172ee"/>
                            <a:da name="DEFAULT" value="1"/>
                            <a:da name="INVALID" type="Range">
                              <a:tst expr="&lt;=65535"/>
                              <a:tst expr="&gt;=0"/>
                            </a:da>
                          </v:var>
                          <v:var name="WdgMMaxMargin" type="INTEGER">
                            <a:a name="DESC">
                              <a:v>&lt;html&gt;
                        This parameter contains the amount of alive indications of the Checkpoint
                        that are acceptable to be additional to the expected alive indications
                        within the corresponding supervision reference cycle.
                      &lt;/html&gt;</a:v>
                            </a:a>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                 type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                            </a:a>
                            <a:a name="LABEL" value="WdgMMaxMargin (0 -&gt; 255)"/>
                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID" 
                                 value="ECUC:54ba6825-39c2-4455-9f94-972c9d93f2d3"/>
                            <a:da name="DEFAULT" value="0"/>
                            <a:da name="INVALID" type="Multi">
                              <mt:range>
                                <mt:tst expr="&lt;=255"/>
                                <mt:tst expr="&gt;=0"/>
                              </mt:range>
                              <mt:xpath 
                                        false="Sum of WdgMExpectedAliveIndications and WdgMMaxMargin must not exceed 65535" 
                                        expr="(. + ../WdgMExpectedAliveIndications) &lt;= 65535"/>
                            </a:da>
                          </v:var>
                          <v:var name="WdgMMinMargin" type="INTEGER">
                            <a:a name="DESC">
                              <a:v>&lt;html&gt;
                        This parameter contains the amount of alive indications of the Checkpoint
                        that are acceptable to be missed from the expected alive indications
                        within the corresponding supervision reference cycle.
                      &lt;/html&gt;</a:v>
                            </a:a>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                 type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                            </a:a>
                            <a:a name="LABEL" value="WdgMMinMargin (0 -&gt; 255)"/>
                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID" 
                                 value="ECUC:863ef215-dc2b-4e4b-8326-f66b16ac9b99"/>
                            <a:da name="DEFAULT" value="0"/>
                            <a:da name="INVALID" type="Multi">
                              <mt:range>
                                <mt:tst expr="&lt;=255"/>
                                <mt:tst expr="&gt;=0"/>
                              </mt:range>
                              <mt:xpath 
                                        false="Value must be smaller or equal the number of expected alive indiations" 
                                        expr=". &lt;= ../WdgMExpectedAliveIndications"/>
                            </a:da>
                          </v:var>
                          <v:var name="WdgMSupervisionReferenceCycle" 
                                 type="INTEGER">
                            <a:a name="DESC">
                              <a:v>&lt;html&gt;
                        This parameter shall contain the amount of supervision cycles to be used
                        as reference by the alive-supervision mechanism to perform the checkup
                        with counted alive indications according to corresponding SE.
                      &lt;/html&gt;</a:v>
                            </a:a>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                 type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                            </a:a>
                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID" 
                                 value="ECUC:e7c93a08-a9c8-4d5b-ad81-cd5162629c17"/>
                            <a:da name="DEFAULT" value="1"/>
                            <a:da name="INVALID" type="Range">
                              <a:tst expr="&lt;=65535"/>
                              <a:tst expr="&gt;=1"/>
                            </a:da>
                          </v:var>
                          <v:ref name="WdgMAliveSupervisionCheckpointRef" 
                                 type="SYMBOLIC-NAME-REFERENCE">
                            <a:a name="DESC">
                              <a:v>&lt;html&gt;
                        Reference to Checkpoint within a Supervised Entity that shall be supervised.
                      &lt;/html&gt;</a:v>
                            </a:a>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                 type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                            </a:a>
                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                            <a:a name="UUID" 
                                 value="ECUC:13b0a782-86c1-4d7d-8881-296c1d7ae526"/>
                            <a:da name="INVALID" type="XPath">
                              <a:tst expr="node:refvalid(.)" 
                                     false="A checkpoint must be referenced."/>
                              <a:tst 
                                     expr="text:uniq(../../*/WdgMAliveSupervisionCheckpointRef, .)" 
                                     false="A checkpoint may only be referenced once for the same mode. This entity is referenced more than once."/>
                            </a:da>
                            <a:da name="REF" 
                                  value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/WdgM/WdgMGeneral/WdgMSupervisedEntity/WdgMCheckpoint"/>
                          </v:ref>
                        </v:ctr>
                      </v:lst>
                      <v:lst name="WdgMDeadlineSupervision" type="MAP">
                        <a:da name="INVALID" type="XPath">
                          <a:tst 
                                 expr="(count(./*) &gt; 0) and (not(lic:feature(&apos;TIME_FEATURE_DM&apos;)) and not(lic:feature(&apos;EB_TIME_DM&apos;)))" 
                                 true="Your EB tresos license does not include Deadline Supervision. If you want to use Deadline Supervision, contact EB."/>
                          <a:tst 
                                 expr="(count(./*) &gt; 0) and                             not(node:exists(../../../../../WdgMGeneral/WdgMSupervisorCallouts/WdgMGetElapsedTimeCallout))" 
                                 true="The configuration of Deadline Supervision requires the configuration of the WdgMGetElapsedTimeCallout API."/>
                        </a:da>
                        <a:da name="MAX" value="65535"/>
                        <v:ctr name="WdgMDeadlineSupervision" 
                               type="IDENTIFIABLE">
                          <a:a name="DESC">
                            <a:v>&lt;html&gt;
                        This container collects all configuration parameters for Deadline
                        Supervision for a Supervised Entity.
                      &lt;/html&gt;</a:v>
                          </a:a>
                          <a:a name="UUID" 
                               value="ECUC:7d45be84-c819-4c3e-8708-f14e27a383bc"/>
                          <a:da name="INVALID" type="XPath">
                            <a:tst 
                                   expr="node:refvalid(WdgMDeadlineStartRef) and node:refvalid(WdgMDeadlineStopRef) and                               (node:path(as:ref(WdgMDeadlineStartRef)) = node:path(as:ref(WdgMDeadlineStopRef)))" 
                                   true="A checkpoint can not be start and stop checkpoint at the same time."/>
                            <a:tst 
                                   expr="node:refvalid(WdgMDeadlineStartRef) and node:refvalid(WdgMDeadlineStopRef) and                               (node:path(as:ref(WdgMDeadlineStartRef)/../..) != node:path(as:ref(WdgMDeadlineStopRef)/../..))" 
                                   true="The referenced start and stop checkpoints do not belong to the same Supervised Entity."/>
                            <a:tst 
                                   expr="node:refvalid(WdgMDeadlineStartRef) and                               node:refvalid(WdgMDeadlineStopRef) and                               (count(../../../*/WdgMDeadlineSupervision/*[                                 (node:path(.) != node:path(node:current())) and                                 node:refvalid(WdgMDeadlineStartRef) and                                 node:refvalid(WdgMDeadlineStopRef) and                                 ((node:path(node:ref(node:current()/WdgMDeadlineStartRef)) =                                   node:path(node:ref(./WdgMDeadlineStartRef))) or                                   (node:path(node:ref(node:current()/WdgMDeadlineStopRef)) =                                   node:path(node:ref(./WdgMDeadlineStopRef))))                               ]) &gt; 0)" 
                                   true="There is more than one Deadline Supervision configured for the referenced checkpoints."/>
                          </a:da>
                          <v:var name="WdgMDeadlineMax" type="FLOAT">
                            <a:a name="DESC">
                              <a:v>&lt;html&gt;
                        This parameter contains the longest time span after which the deadline
                        is considered to be met.    Unit: [s]
                      &lt;/html&gt;</a:v>
                            </a:a>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                 type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                            </a:a>
                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID" 
                                 value="ECUC:3c3ce3fe-61c0-48eb-a57b-d9b3eaefa758"/>
                            <a:da name="INVALID" type="Multi">
                              <mt:range>
                                <mt:tst expr="&lt;=65535"/>
                                <mt:tst expr="&gt;=0"/>
                              </mt:range>
                              <mt:xpath>
                                <mt:tst 
                                        false="The deadline must be a positive value." 
                                        expr="node:value(.) &gt; 0"/>
                                <mt:tst 
                                        false="The WdgMDeadlineMax must be greater or equal WdgMDeadlineMin." 
                                        expr="node:value(.) &gt;= node:value(../WdgMDeadlineMin)"/>
                                <mt:tst 
                                        true="The configured value must be greater than or equal to the time granularity provided via parameter WdgMTimeGranularity." 
                                        expr="node:exists(../../../../../../../WdgMGeneral/WdgMSupervisorCallouts/WdgMTimeGranularity)                                   and (node:value(.) &lt; ../../../../../../../WdgMGeneral/WdgMSupervisorCallouts/WdgMTimeGranularity)"/>
                                <mt:tst 
                                        true="The configured value must be a multiple of the time granularity provided via parameter WdgMTimeGranularity." 
                                        expr="node:exists(../../../../../../../WdgMGeneral/WdgMSupervisorCallouts/WdgMTimeGranularity)                                   and (num:mod(round(node:value(.) * &apos;1E10&apos;), round(node:value(../../../../../../../WdgMGeneral/WdgMSupervisorCallouts/WdgMTimeGranularity) * &apos;1E10&apos;)) != 0)"/>
                                <mt:tst 
                                        true="The configured value divided by the WdgMTimeGranularity parameter exceeds the maximum value which can be represented by 32 bit which is due to an implementation constraint." 
                                        expr="node:exists(../../../../../../../WdgMGeneral/WdgMSupervisorCallouts/WdgMTimeGranularity)                                   and (num:i(num:div(node:value(.), ../../../../../../../WdgMGeneral/WdgMSupervisorCallouts/WdgMTimeGranularity)) &gt;= 4294967296)"/>
                                <mt:tst 
                                        true="The configured value corresponds to a number of main function cycles until detection of a deadline violation which exceeds the maximum value which can be represented by 16 bit which is due to an implementation constraint." 
                                        expr="num:i(num:div(node:value(.), ../../../WdgMSupervisionCycle)) &gt;= 32768"/>
                              </mt:xpath>
                            </a:da>
                            <a:da name="WARNING" type="XPath" 
                                  expr="num:i(num:div(node:value(.), num:max(../../../../*/WdgMSupervisionCycle))) &gt;= 10" 
                                  true="The configured value is too coarse-grained (one time unit for Deadline Monitoring contains more than 10 main function cycles in any mode) that it may be more efficient to configure Alive Supervision instead of Deadline Monitoring."/>
                          </v:var>
                          <v:var name="WdgMDeadlineMin" type="FLOAT">
                            <a:a name="DESC">
                              <a:v>&lt;html&gt;
                        This parameter contains the shortest time span after which the deadline
                        is considered to be met.    Unit: [s]
                      &lt;/html&gt;</a:v>
                            </a:a>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                 type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                            </a:a>
                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID" 
                                 value="ECUC:84c81576-4bc3-4e78-afe8-03c6dbdddeae"/>
                            <a:da name="INVALID" type="Multi">
                              <mt:range>
                                <mt:tst expr="&lt;=65535"/>
                                <mt:tst expr="&gt;=0"/>
                              </mt:range>
                              <mt:xpath>
                                <mt:tst 
                                        false="The deadline must be a positive value." 
                                        expr="node:value(.) &gt;= 0"/>
                                <mt:tst 
                                        true="The configured value must be greater than or equal to the time granularity provided via parameter WdgMTimeGranularity." 
                                        expr="node:exists(../../../../../../../WdgMGeneral/WdgMSupervisorCallouts/WdgMTimeGranularity)                                   and (node:value(.) &lt; ../../../../../../../WdgMGeneral/WdgMSupervisorCallouts/WdgMTimeGranularity)                                   and (node:value(.) != 0)"/>
                                <mt:tst 
                                        true="The configured value must be a multiple of the time granularity provided via parameter WdgMTimeGranularity." 
                                        expr="node:exists(../../../../../../../WdgMGeneral/WdgMSupervisorCallouts/WdgMTimeGranularity)                                   and (num:mod(round(node:value(.) * &apos;1E10&apos;), round(node:value(../../../../../../../WdgMGeneral/WdgMSupervisorCallouts/WdgMTimeGranularity) * &apos;1E10&apos;)) != 0)"/>
                                <mt:tst 
                                        true="The configured value divided by the WdgMTimeGranularity parameter exceeds the maximum value which can be represented by 32 bit which is due to an implementation constraint." 
                                        expr="node:exists(../../../../../../../WdgMGeneral/WdgMSupervisorCallouts/WdgMTimeGranularity)                                   and (num:i(num:div(node:value(.), ../../../../../../../WdgMGeneral/WdgMSupervisorCallouts/WdgMTimeGranularity)) &gt;= 4294967296)"/>
                              </mt:xpath>
                            </a:da>
                            <a:da name="WARNING" type="XPath" 
                                  expr="num:i(num:div(node:value(.), num:max(../../../../*/WdgMSupervisionCycle))) &gt;= 10" 
                                  true="The configured value is too coarse-grained (one time unit for Deadline Monitoring contains more than 10 main function cycles in any mode) that it may be more efficient to configure Alive Supervision instead of Deadline Monitoring."/>
                          </v:var>
                          <v:ref name="WdgMDeadlineStartRef" 
                                 type="SYMBOLIC-NAME-REFERENCE">
                            <a:a name="DESC">
                              <a:v>&lt;html&gt;
                        This is the reference to the start Checkpoint for Deadline Supervision.
                      &lt;/html&gt;</a:v>
                            </a:a>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" 
