                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID" 
                                 value="ECUC:b69101dc-fbeb-4530-8fba-7201fd38e3b5"/>
                            <a:da name="DEFAULT" value="WDGIF_FAST_MODE"/>
