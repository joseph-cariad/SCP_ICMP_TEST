                               value="ECUC:b81d7037-a060-4e9c-9e16-175443d60577"/>
                          <v:var name="WdgMTriggerConditionValue" 
                                 type="INTEGER">
                            <a:a name="DESC">
                              <a:v>&lt;html&gt;
