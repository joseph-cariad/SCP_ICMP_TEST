              <v:ctr name="SoAdDefensiveProgramming" type="IDENTIFIABLE">
                <a:a name="DESC">
                  <a:v>&lt;html&gt;
                &lt;p&gt;Parameters for defensive programming&lt;/p&gt;
                &lt;/html&gt;</a:v>
                </a:a>
                <a:a name="LABEL" value="Defensive Programming Options"/>
                <v:var name="SoAdDefProgEnabled" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                    &lt;p&gt;Enables or disables the defensive
                    programming feature for the module SoAd.&lt;/p&gt;
                    &lt;p&gt;Note: This feature is dependent on the use of the development error detection module.
                    To use the defensive programming feature, proceed as follows:
                    &lt;ol&gt;
                    &lt;li&gt;Enable development error detection&lt;/li&gt;
                    &lt;li&gt;Enable defensive programming&lt;/li&gt;
                    &lt;li&gt;Enable assertions as required&lt;/li&gt;
                    &lt;/ol&gt;
                    &lt;/p&gt;
                    &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="LABEL" value="Enable Defensive Programming"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:da name="DEFAULT" value="false"/>
                  <a:da name="EDITABLE" type="XPath" 
                        expr="../../SoAdGeneral/SoAdDevErrorDetect = &apos;true&apos;" 
                        true="true" false="false"/>
                </v:var>
                <v:var name="SoAdPrecondAssertEnabled" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                  &lt;p&gt;Enables handling of precondition assertion checks reported from
                  the module SoAd.&lt;/p&gt;
                  &lt;p&gt;Dependency on parameter(s):
                  &lt;ul&gt;
                  &lt;li&gt;Enable Development Error Detection (&lt;code&gt;SoAdDevErrorDetect&lt;/code&gt;): must be enabled &lt;/li&gt;
                  &lt;li&gt;Enable Defensive Programming (&lt;code&gt;SoAdDefProgEnabled&lt;/code&gt;): must be enabled&lt;/li&gt;
                  &lt;/ul&gt;
                  &lt;/p&gt;
                  &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="LABEL" value="Enable Precondition Assertions"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:da name="DEFAULT" value="false"/>
                  <a:da name="EDITABLE" type="XPath" 
                        expr="(../../SoAdGeneral/SoAdDevErrorDetect = &apos;true&apos;) and (../SoAdDefProgEnabled = &apos;true&apos;)" 
                        true="true" false="false"/>
                </v:var>
                <v:var name="SoAdPostcondAssertEnabled" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                  &lt;p&gt;Enables handling of postcondition assertion checks reported from
                  the module SoAd.&lt;/p&gt;
                  &lt;p&gt;Dependency on parameter(s):
                  &lt;ul&gt;
                  &lt;li&gt;Enable Development Error Detection (&lt;code&gt;SoAdDevErrorDetect&lt;/code&gt;): must be enabled &lt;/li&gt;
                  &lt;li&gt;Enable Defensive Programming (&lt;code&gt;SoAdDefProgEnabled&lt;/code&gt;): must be enabled&lt;/li&gt;
                  &lt;/ul&gt;
                  &lt;/p&gt;
                  &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="LABEL" value="Enable Postcondition Assertions"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:da name="DEFAULT" value="false"/>
                  <a:da name="EDITABLE" type="XPath" 
                        expr="(../../SoAdGeneral/SoAdDevErrorDetect = &apos;true&apos;) and (../SoAdDefProgEnabled = &apos;true&apos;)" 
                        true="true" false="false"/>
                </v:var>
                <v:var name="SoAdStaticAssertEnabled" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                  &lt;p&gt;Enables handling of static assertion checks reported from
                  the module SoAd.&lt;/p&gt;
                  &lt;p&gt;Dependency on parameter(s):
                  &lt;ul&gt;
                  &lt;li&gt;Enable Development Error Detection (&lt;code&gt;SoAdDevErrorDetect&lt;/code&gt;): must be enabled &lt;/li&gt;
                  &lt;li&gt;Enable Defensive Programming (&lt;code&gt;SoAdDefProgEnabled&lt;/code&gt;): must be enabled&lt;/li&gt;
                  &lt;/ul&gt;
                  &lt;/p&gt;
                  &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="LABEL" value="Enable Static Assertions"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:da name="DEFAULT" value="false"/>
                  <a:da name="EDITABLE" type="XPath" 
                        expr="(../../SoAdGeneral/SoAdDevErrorDetect = &apos;true&apos;) and (../SoAdDefProgEnabled = &apos;true&apos;)" 
                        true="true" false="false"/>
                </v:var>
                <v:var name="SoAdUnreachAssertEnabled" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                  &lt;p&gt;Enables handling of unreachable code assertion checks
                  reported from the module SoAd.&lt;/p&gt;
                  &lt;p&gt;Dependency on parameter(s):
                  &lt;ul&gt;
                  &lt;li&gt;Enable Development Error Detection (&lt;code&gt;SoAdDevErrorDetect&lt;/code&gt;): must be enabled &lt;/li&gt;
                  &lt;li&gt;Enable Defensive Programming (&lt;code&gt;SoAdDefProgEnabled&lt;/code&gt;): must be enabled&lt;/li&gt;
                  &lt;/ul&gt;
                  &lt;/p&gt;
                  &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="LABEL" value="Enable Unreachable Code Assertions"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:da name="DEFAULT" value="false"/>
                  <a:da name="EDITABLE" type="XPath" 
                        expr="(../../SoAdGeneral/SoAdDevErrorDetect = &apos;true&apos;) and (../SoAdDefProgEnabled = &apos;true&apos;)" 
                        true="true" false="false"/>
                </v:var>
                <v:var name="SoAdInvariantAssertEnabled" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                  &lt;p&gt;Enables handling of invariant assertion checks
                  reported from functions of the module SoAd.&lt;/p&gt;
                  &lt;p&gt;Dependency on parameter(s):
                  &lt;ul&gt;
                  &lt;li&gt;Enable Development Error Detection (&lt;code&gt;SoAdDevErrorDetect&lt;/code&gt;): must be enabled &lt;/li&gt;
                  &lt;li&gt;Enable Defensive Programming (&lt;code&gt;SoAdDefProgEnabled&lt;/code&gt;): must be enabled&lt;/li&gt;
                  &lt;/ul&gt;
                  &lt;/p&gt;
                  &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="LABEL" value="Enable Invariant Assertions"/>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:da name="DEFAULT" value="false"/>
                  <a:da name="EDITABLE" type="XPath" 
                        expr="(../../SoAdGeneral/SoAdDevErrorDetect = &apos;true&apos;) and (../SoAdDefProgEnabled = &apos;true&apos;)" 
                        true="true" false="false"/>
                </v:var>
              </v:ctr>
              <v:ctr name="SoAdGeneral" type="IDENTIFIABLE">
                <a:a name="DESC">
                  <a:v>&lt;html&gt;
                        This container contains all global configuration parameters
                        of SoAd.
                      &lt;/html&gt;</a:v>
                </a:a>
                <a:a name="UUID" 
                     value="ECUC:4202ade9-3a6c-48f5-a9dc-23b17eff1b56"/>
                <v:var name="SoAdDevErrorDetect" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                          Pre-processor switch for enabling development error detection support.
                        &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                  <a:a name="SCOPE" value="LOCAL"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" 
                       value="ECUC:794cba6f-b434-4d3e-bbd0-964f43cbe9c3"/>
                  <a:da name="DEFAULT" value="true"/>
                </v:var>
                <v:var name="SoAdGetAndResetMeasurementDataApi" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                          Pre-processor switch for enabling the Get and Reset Measurement Data API.
                        &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                  <a:a name="SCOPE" value="ECU"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:da name="DEFAULT" value="false"/>
                </v:var>
                <v:var name="SoAdIPv6AddressEnabled" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                          &lt;p&gt;Allows for increased memory allocation to store IPv6 addresses.&lt;/p&gt;
                          &lt;ul&gt;
                            &lt;li&gt;true: Enables support for IPv6 addresses&lt;/li&gt;
                            &lt;li&gt;false: Only IPv4 addresses are supported&lt;/li&gt;
                          &lt;/ul&gt;
                        &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                  <a:a name="SCOPE" value="ECU"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" 
                       value="ECUC:1476fc8f-53cd-449b-87fb-290eaf61abc6"/>
                  <a:da name="DEFAULT" value="false"/>
                  <a:da name="INVALID" type="Multi">
                    <mt:xpath 
                              true="SoAdIPv6AddressEnabled must be enabled if at least one referenced local IP address is of type IPv6." 
                              expr="(.=&apos;false&apos;) and                          (num:i(count(as:modconf(&apos;SoAd&apos;)[1]/SoAdConfig/*/SoAdSocketConnectionGroup/*[node:refvalid(SoAdSocketLocalAddressRef) and as:ref(SoAdSocketLocalAddressRef)/TcpIpDomainType = &apos;TCPIP_AF_INET6&apos;])) &gt;0)                         "/>
                  </a:da>
                </v:var>
                <v:var name="SoAdMainFunctionPeriod" type="FLOAT">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                          Determines the frequency at which the SoAd_MainFunction()
                          is called in [s].
                        &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                  <a:a name="SCOPE" value="ECU"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" 
                       value="ECUC:0d68aaf1-6ac6-4703-9f87-6ed9bc77fcb3"/>
                  <a:da name="DEFAULT" value="0.1"/>
                  <a:da name="INVALID" type="Multi">
                    <mt:range>
                      <mt:tst expr="&lt;=1000.0"/>
                      <mt:tst expr="&gt;=0.000001"/>
                    </mt:range>
                  </a:da>
                </v:var>
                <v:var name="SoAdSoConMax" type="INTEGER">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                          &lt;p&gt; Specifies the maximum number of SoAd socket connections.&lt;/p&gt;
                          &lt;p&gt;&lt;strong&gt;
                            Parameter is unused. The maximum number of socket connections is defined
                            by the amount of reserved RAM (to be configured with SoAdDataMemSize)
                            and the used type (to be configured with SoAdSoConIdType).
                          &lt;/strong&gt;&lt;/p&gt;
                        &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                  <a:a name="SCOPE" value="LOCAL"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" 
                       value="ECUC:0b9eb24e-bc16-469d-be91-0ac798b57ea6"/>
                  <a:da name="ENABLE" value="false"/>
                </v:var>
                <v:var name="SoAdRoutingGroupMax" type="INTEGER">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                          &lt;p&gt;Specifies the maximum number of SoAd routing groups.&lt;/p&gt;
                          &lt;p&gt;&lt;strong&gt;
                            Parameter is unused. The maximum number of routing groups is defined by
                            the amount of reserved RAM (to be configured with SoAdDataMemSize) and
                            the used type (to be configured with SoAdRoutingGroupIdType).
                          &lt;/strong&gt;&lt;/p&gt;
                        &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                  <a:a name="SCOPE" value="LOCAL"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" 
                       value="ECUC:c09a057f-1b6c-4158-be7d-a8be2c930896"/>
                  <a:da name="ENABLE" value="false"/>
                </v:var>
                <v:var name="SoAdVersionInfoApi" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                          &lt;p&gt;Activates the SoAd_GetVersionInfo() API.&lt;/p&gt;
                          &lt;ul&gt;
                            &lt;li&gt;true: Enables the SoAd_GetVersionInfo() API.&lt;/li&gt;
                            &lt;li&gt;false: SoAd_GetVersionInfo() API is not included.&lt;/li&gt;
                          &lt;/ul&gt;
                        &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                  <a:a name="SCOPE" value="LOCAL"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" 
                       value="ECUC:e37f5bb1-2a02-48b6-8f8b-5bdabdd69153"/>
                  <a:da name="DEFAULT" value="true"/>
                </v:var>
                <v:var name="SoAdTlsEnabled" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                          &lt;p&gt;&lt;/p&gt;
                          &lt;ul&gt;
                            &lt;li&gt;true: Enables support for TLS extension.&lt;/li&gt;
                            &lt;li&gt;false: Disables support for TLS extension.&lt;/li&gt;
                          &lt;/ul&gt;
                        &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                  <a:a name="SCOPE" value="ECU"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:da name="DEFAULT" value="false"/>
                </v:var>
                <v:ctr name="VendorSpecific" type="IDENTIFIABLE">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                        Contains the vendor specific configuration parameters of the AUTOSAR SoAd module.
                      &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="TAB" value="Optimization"/>
                  <v:var name="SoAdDataMemSize" type="INTEGER">
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                          &lt;p&gt;
                            Size of internal SoAd data in units of bytes (static memory allocation)
                            - Memory required by post-build configuration must be smaller than this
                            constant. If parameter is disabled, the MCG calculates itself based on
                            the current configuration.
                          &lt;/p&gt;
                          &lt;p&gt;&lt;strong&gt;
                            This parameter shall be used to reserve RAM at precompile time to be
                            used for configuration changes at post build time.
                          &lt;/strong&gt;&lt;/p&gt;
                        &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v class="Link">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="OPTIONAL" value="true"/>
                    <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:da name="DEFAULT" value="0"/>
                    <a:da name="ENABLE" value="false"/>
                    <a:da name="INVALID" type="Multi">
                      <mt:range>
                        <mt:tst expr="&lt;4294967296"/>
                        <mt:tst expr="&gt;=0"/>
                      </mt:range>
                      <mt:xpath 
                                true="SoAdDataMemSize exceeds 64 KiB. Please activate SoAdDataMemSizeExtension." 
                                expr="(as:modconf(&apos;SoAd&apos;)[1]/SoAdGeneral/VendorSpecific/SoAdDataMemSizeExtension = &apos;false&apos;) and (num:i(.) &gt;65536)"/>
                    </a:da>
                  </v:var>
                  <v:var name="SoAdDataMemSizeExtension" type="BOOLEAN">
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            Configuration parameter to allow RAM size usage of more than 64 KiB.
                            &lt;ul&gt;
                              &lt;li&gt;&lt;code&gt;TRUE&lt;/code&gt;: RAM size NOT limited by 64 KiB at maximum (switched on).&lt;/li&gt;
                              &lt;li&gt;&lt;code&gt;FALSE&lt;/code&gt;: RAM size limited by 64 KiB at maximum (switched off).&lt;/li&gt;
                            &lt;/ul&gt;
                            &lt;p&gt;&lt;strong&gt;Optimization Effect:&lt;/strong&gt;&lt;/p&gt;
                            &lt;ul&gt;
                              &lt;li class=&quot;optimization.config.rom.reduction&quot;&gt;
                                &lt;strong&gt;ROM reduction (config):&lt;/strong&gt;
                                Disabling this parameter reduces the ROM consumption of the module configuration.
                              &lt;/li&gt;
                            &lt;/ul&gt;
                        &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:da name="DEFAULT" value="false"/>
                  </v:var>
                  <v:var name="SoAdRelocatableCfgEnable" type="BOOLEAN">
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                        If set to TRUE the post-build-time configuration data contains only
                        relative offsets to the configuration start address (and is therefore
                        relocatable).
                        If set to FALSE the post-build-time configuration data
                        contains absolute pointers (and is therefore not relocatable).
                        &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:da name="DEFAULT" value="true"/>
                    <a:da name="EDITABLE" type="XPath" 
                          expr="(node:contains(node:refs(as:modconf(&apos;PbcfgM&apos;)/PbcfgMBswModules/*/PbcfgMBswModuleRef), as:modconf(&apos;SoAd&apos;)) = &apos;true&apos;)" 
                          true="false" false="true"/>
                  </v:var>
                  <v:var name="SoAdRouteMax" type="ENUMERATION">
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                          &lt;p&gt;
                            This parameter defines the maximum number of SoAdPduRouteDest containers and the maximum number of SoAdSocketRouteDest containers.
                          &lt;/p&gt;
                          &lt;ul&gt;
                            &lt;li&gt;&lt;code&gt;INDEX_UINT8&lt;/code&gt;: The maximum number of SoAdPduRouteDest containers is 255 and the maximum number of SoAdSocketRouteDest containers is 255.&lt;/li&gt;
                            &lt;li&gt;&lt;code&gt;INDEX_UINT16&lt;/code&gt;: The maximum number of SoAdPduRouteDest containers is 65535 and the maximum number of SoAdSocketRouteDest containers is 65535.&lt;/li&gt;
                          &lt;/ul&gt;
                          &lt;p&gt;&lt;strong&gt;Optimization Effect:&lt;/strong&gt;&lt;/p&gt;
                          &lt;ul&gt;
                            &lt;li class=&quot;optimization.config.rom.reduction&quot;&gt;&lt;strong&gt;ROM reduction (config):&lt;/strong&gt;
                            The smaller the index, the smaller the ROM consumption of the module configuration.&lt;/li&gt;
                          &lt;/ul&gt;
                        &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:da name="DEFAULT" value="INDEX_UINT16"/>
                    <a:da name="RANGE">
                      <a:v>INDEX_UINT8</a:v>
                      <a:v>INDEX_UINT16</a:v>
                    </a:da>
                  </v:var>
                  <v:var name="SoAdTimerType" type="ENUMERATION">
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                          &lt;p&gt;
                            SoAdTimerType defines the maximum range for timers and timeouts that can
                            be used in the configuration of SoAd (e.g. for the nPduUdpTx Feature) by
                            determining the size of SoAd_TimerType.
                            The configurable timer/timeout values also depend on the configuration
                            of SoAdMainFunctionPeriod.
                          &lt;/p&gt;
                          &lt;p&gt;&lt;strong&gt;Example:&lt;/strong&gt;&lt;/p&gt;
                          &lt;p&gt;
                            If SoAdMainFunctionPeriod is set to 10 ms and the following
                            SoAdTimerType setting allow the respective timer/timeout values:
                            &lt;ul&gt;
                              &lt;li&gt;&lt;code&gt;UINT8&lt;/code&gt;: values up to 2,55 s&lt;/li&gt;
                              &lt;li&gt;&lt;code&gt;UINT16&lt;/code&gt;: values up to 655,35 s (~11 min)&lt;/li&gt;
                              &lt;li&gt;&lt;code&gt;UINT32&lt;/code&gt;: values up to 42949672,95 s (well over one
                                year)&lt;/li&gt;
                            &lt;/ul&gt;
                          &lt;/p&gt;
                          &lt;p&gt;&lt;strong&gt;Optimization Effect:&lt;/strong&gt;&lt;/p&gt;
                          &lt;ul&gt;
                            &lt;li class=&quot;optimization.config.rom.reduction&quot;&gt;&lt;strong&gt;ROM reduction
                            (config):&lt;/strong&gt; The smaller the type, the smaller the ROM consumption
                            of the module configuration.&lt;/li&gt;
                          &lt;/ul&gt;
                        &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:da name="DEFAULT" value="UINT8"/>
                    <a:da name="RANGE">
                      <a:v>UINT8</a:v>
                      <a:v>UINT16</a:v>
                      <a:v>UINT32</a:v>
                    </a:da>
                  </v:var>
                  <v:var name="SoAdSoConIdType" type="ENUMERATION">
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                          &lt;p&gt;
                            SoAdSoConIdType allows to optimize the memory consumtion
                            of the socket connections.
                            Changing this value has an impact on the number of socket connection
                            which can be configured at post build time.
                          &lt;/p&gt;
                          &lt;p&gt;&lt;strong&gt;Possible values:&lt;/strong&gt;&lt;/p&gt;
                          &lt;p&gt;
                            &lt;ul&gt;
                              &lt;li&gt;&lt;code&gt;UINT8&lt;/code&gt;: The maximum number of connections is 254.&lt;/li&gt;
                              &lt;li&gt;&lt;code&gt;UINT16&lt;/code&gt;: The maximum number of connections is 65534.&lt;/li&gt;
                            &lt;/ul&gt;
                          &lt;/p&gt;
                          &lt;p&gt;&lt;strong&gt;Optimization Effect:&lt;/strong&gt;&lt;/p&gt;
                          &lt;ul&gt;
                            &lt;li class=&quot;optimization.config.rom.reduction&quot;&gt;&lt;strong&gt;ROM reduction
                            (config):&lt;/strong&gt; The smaller the type, the smaller the ROM consumption
                            of the module configuration.&lt;/li&gt;
                          &lt;/ul&gt;
                          &lt;ul&gt;
                            &lt;li class=&quot;optimization.config.ram.reduction&quot;&gt;&lt;strong&gt;RAM reduction
                            (config):&lt;/strong&gt; The smaller the type, the smaller the RAM consumption
                            of the module.&lt;/li&gt;
                          &lt;/ul&gt;
                        &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:da name="DEFAULT" value="UINT16"/>
                    <a:da name="RANGE">
                      <a:v>UINT8</a:v>
                      <a:v>UINT16</a:v>
                    </a:da>
                  </v:var>
                  <v:var name="SoAdRoutingGroupIdType" type="ENUMERATION">
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                          &lt;p&gt;
                            SoAdRoutingGroupIdType allows to optimize the memory consumtion
                            of the routing groups.
                            Changing this value has an impact on the number of routing groups
                            which can be configured at post build time.
                          &lt;/p&gt;
                          &lt;p&gt;&lt;strong&gt;
                            The number of routing groups can be higher than the
                            maximum of SoAdRoutingGroupId if specific routing is used.
                          &lt;/strong&gt;&lt;/p&gt;
                          &lt;p&gt;&lt;strong&gt;Possible values:&lt;/strong&gt;&lt;/p&gt;
                          &lt;p&gt;
                            &lt;ul&gt;
                              &lt;li&gt;&lt;code&gt;UINT8&lt;/code&gt;: The maximum number of connections is 254.&lt;/li&gt;
                              &lt;li&gt;&lt;code&gt;UINT16&lt;/code&gt;: The maximum number of connections is 65534.&lt;/li&gt;
                              &lt;li&gt;&lt;code&gt;UINT32&lt;/code&gt;: The maximum number of connections is 4294967295.&lt;/li&gt;
                            &lt;/ul&gt;
                          &lt;/p&gt;
                          &lt;p&gt;&lt;strong&gt;Optimization Effect:&lt;/strong&gt;&lt;/p&gt;
                          &lt;ul&gt;
                            &lt;li class=&quot;optimization.config.rom.reduction&quot;&gt;&lt;strong&gt;ROM reduction
                            (config):&lt;/strong&gt; The smaller the type, the smaller the ROM consumption
                            of the module configuration.&lt;/li&gt;
                          &lt;/ul&gt;
                          &lt;ul&gt;
                            &lt;li class=&quot;optimization.config.ram.reduction&quot;&gt;&lt;strong&gt;RAM reduction
                            (config):&lt;/strong&gt; The smaller the type, the smaller the RAM consumption
                            of the module.&lt;/li&gt;
                          &lt;/ul&gt;
                        &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:da name="DEFAULT" value="UINT16"/>
                    <a:da name="RANGE">
                      <a:v>UINT8</a:v>
                      <a:v>UINT16</a:v>
                      <a:v>UINT32</a:v>
                    </a:da>
                  </v:var>
                  <v:var name="SoAdReportInvPDUHeaderIdToDetEnable" 
                         type="BOOLEAN">
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                                &lt;p&gt;Enables the reporting of SOAD_E_INV_PDUHEADER_ID to DET
                                whenever an invalid PDU header ID is received and DET reporting
                                is enabled (i.e. SoAdDevErrorDetect = TRUE)&lt;/p&gt;
                                &lt;ul&gt;
                                  &lt;li&gt;true: Report of SOAD_E_INV_PDUHEADER_ID to DET.&lt;/li&gt;
                                  &lt;li&gt;false: No report to DET.&lt;/li&gt;
                                &lt;/ul&gt;
                                &lt;p&gt;&lt;strong&gt;Optimization Effect:&lt;/strong&gt;&lt;/p&gt;
                                &lt;ul&gt;
                                  &lt;li class=&quot;optimization.code.rom.reduction&quot;&gt;
                                    &lt;strong&gt;ROM reduction (code):&lt;/strong&gt;
                                    Setting this parameter to FALSE reduces the ROM consumption of the module code.
                                  &lt;/li&gt;
                                  &lt;li class=&quot;optimization.code.exetime.reduction&quot;&gt;
                                    &lt;strong&gt;Execution time reduction (code):&lt;/strong&gt;
                                    Setting this parameter to FALSE reduces the execution time of the module code.
                                  &lt;/li&gt;
                                &lt;/ul&gt;
                              &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:da name="DEFAULT" value="true"/>
                  </v:var>
                  <v:var name="SoAdSocketRouteDestFanInEnable" type="BOOLEAN">
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                                &lt;p&gt;Determines whether it shall be allowed that a global Pdu can be referenced by more than one SoAdSocketRouteDest with SoAdRxUpperLayerType = &apos;IF&apos;.&lt;/p&gt;
                                &lt;ul&gt;
                                  &lt;li&gt;true: Global Pdus can be referenced by multiple SoAdSocketRouteDest (SoAdRxUpperLayerType = &apos;IF&apos;).&lt;/li&gt;
                                  &lt;li&gt;false: Global Pdus can only be reference by one SoAdSocketRouteDest (SoAdRxUpperLayerType = &apos;IF&apos;).&lt;/li&gt;
                                &lt;/ul&gt;
                              &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:da name="DEFAULT" value="false"/>
                  </v:var>
                </v:ctr>
                <v:var name="SoAdEnableMainFunctionTx" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                          &lt;p&gt;
                            This configuration parameter enables the use of SoAd_MainFunctionTx()
                            to be triggered externally. SoAd_MainFunctionTx() triggers
                            the timing intependend Tx frame processing of SoAd_MainFunction().
                          &lt;/p&gt;
                          &lt;ul&gt;
                            &lt;li&gt;true: SoAd_MainFunctionTx() can be called externally. &lt;/li&gt;
                            &lt;li&gt;false: SoAd_MainFunctionTx() is defined internally only.&lt;/li&gt;
                          &lt;/ul&gt;
                        &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:da name="DEFAULT" value="false"/>
                </v:var>
              </v:ctr>
              <v:lst name="SoAdBswModules" type="MAP">
                <a:da name="INVALID" type="Multi">
                  <mt:xpath 
                            true="Please add a valid upper layer module configuration for SoAd by creating a SoAdBswModules entry with the name of the module." 
                            expr="num:i(count(./*)) = &apos;0&apos;"/>
                </a:da>
                <a:da name="MIN" value="0"/>
                <v:ctr name="SoAdBswModules" type="IDENTIFIABLE">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                          Each container describes a specific BSW module that the SoAd
                          shall interface to.
                          The reason to have it as own configuration container instead
                          of implication of the routing path is to be able to configure
                          CDD:s properly and to force module&apos;s to be used in a post-build
                          situation even though no routing is made to/from this module
                          (future configurations may include these modules).
                        &lt;/html&gt;</a:v>
                  </a:a>
                  <a:a name="POSTBUILDCHANGEABLE" value="false"/>
                  <a:da name="INVALID" type="Multi">
                    <mt:xpath 
                              true="SoAdBswModule containers shall have the same name as the upper layer module they represent (e.g. PduR or Sd)." 
                              expr="text:contains(name(.), &apos;SoAdBswModule&apos;)"/>
                  </a:da>
                  <v:var name="SoAdIf" type="BOOLEAN">
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            Specifies if the BSW module supports the Communication
                            Interface APIs or not. Value true the APIs are supported.
                            A module can have both Communication Interface APIs and
                            Transport Protocol APIs (e.g. the PduR module).
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="SCOPE" value="LOCAL"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:a name="UUID" 
                         value="ECUC:61c7651e-6ad8-41c3-abc3-2dd2cee6a53e"/>
                    <a:da name="DEFAULT" value="true"/>
                  </v:var>
                  <v:var name="SoAdTp" type="BOOLEAN">
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            Specifies if the BSW module supports the TransportProtocol
                            APIs or not. Value true the APIs are supported. A module
                            can have both Communication Interface APIs and Transport
                            Protocol APIs (e.g. the PduR module).
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="SCOPE" value="LOCAL"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:a name="UUID" 
                         value="ECUC:2825d602-fc8e-4d64-a2c7-30226d4a826e"/>
                    <a:da name="DEFAULT" value="false"/>
                  </v:var>
                  <v:var name="SoAdIfTriggerTransmit" type="BOOLEAN">
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            Specifies if the BSW module supports the TriggerTransmit API or not.
                            Value true means that the API is supported.
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="SCOPE" value="LOCAL"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:a name="UUID" 
                         value="ECUC:69f17a2e-4075-40ca-b055-2cca5f9c23fb"/>
                    <a:da name="DEFAULT" value="false"/>
                  </v:var>
                  <v:var name="SoAdIfTxConfirmation" type="BOOLEAN">
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            Specifies if the BSW module supports the TxConfirmation API
                            or not. Value true the API is supported.
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="SCOPE" value="LOCAL"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:a name="UUID" 
                         value="ECUC:8e120498-e9dc-4049-9ad9-4637c25402cd"/>
                    <a:da name="DEFAULT" value="true"/>
                  </v:var>
                  <v:var name="SoAdSoConModeChg" type="BOOLEAN">
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            Specifies if the BSW module supports the SoConModeChg API
                            or not. Value true the API is supported.
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="SCOPE" value="LOCAL"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:a name="UUID" 
                         value="ECUC:2851d043-899d-43c6-8c1d-cf6218cc7bd0"/>
                    <a:da name="DEFAULT" value="false"/>
                  </v:var>
                  <v:var name="SoAdLocalIpAddrAssigmentChg" type="BOOLEAN">
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            Specifies if the BSW module supports the
                            LocalIpAddrAssigmentChg API or not. Value true the
                            API is supported.
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="SCOPE" value="LOCAL"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:a name="UUID" 
                         value="ECUC:9de9200d-13d9-4459-8f2c-0f928ebfcf1d"/>
                    <a:da name="DEFAULT" value="false"/>
                  </v:var>
                  <v:var name="SoAdUseCallerInfix" type="BOOLEAN">
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;Specifies if SoAd shall use the infix &quot;SoAd&quot; when
                            calling an upper layer module function or not.&lt;/p&gt;
                            &lt;p&gt;E.g. if SoAdUseCallerInfix is TRUE for the upper layer
                            &quot;ABC&quot; then SoAd will call ABC_SoAdIfRxIndication() otherwise
                            SoAd would call ABC_IfRxIndication().&lt;/p&gt;
                            &lt;ul&gt;
                              &lt;li&gt;true: CallerInfix is used.&lt;/li&gt;
                              &lt;li&gt;false: CallerInfix is not used.&lt;/li&gt;
                            &lt;/ul&gt;
                            &lt;p&gt;&lt;strong&gt;This parameter is not used.&lt;/strong&gt;&lt;/p&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="SCOPE" value="LOCAL"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:a name="UUID" 
                         value="ECUC:ae620584-c765-4574-90e9-6f9c7690b6e5"/>
                    <a:da name="DEFAULT" value="true"/>
                    <a:da name="ENABLE" value="false"/>
                  </v:var>
                  <v:var name="SoAdUseTypeInfix" type="BOOLEAN">
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;Specifies if SoAd shall use the API type infix &quot;TP&quot; or
                            &quot;IF&quot; when calling an upper layer module function or not.&lt;/p&gt;
                            &lt;p&gt;E.g. if SoAdUseTypeInfix is TRUE for the upper layer &quot;ABC&quot;
                            then SoAd will call ABC_IfRxIndication(), otherwise SoAd would
                            call ABC_RxIndication().&lt;/p&gt;
                            &lt;ul&gt;
                              &lt;li&gt;true: TypeInfix is used.&lt;/li&gt;
                              &lt;li&gt;false: TypeInfix is not used.&lt;/li&gt;
                            &lt;/ul&gt;
                            &lt;p&gt;&lt;strong&gt;This parameter is not used.&lt;/strong&gt;&lt;/p&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="SCOPE" value="LOCAL"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:a name="UUID" 
                         value="ECUC:103a7f7b-0839-4d90-ac0a-830925ad10ea"/>
                    <a:da name="DEFAULT" value="true"/>
                    <a:da name="ENABLE" value="false"/>
                  </v:var>
                  <v:ref name="SoAdBswModuleRef" type="FOREIGN-REFERENCE">
                    <a:a name="DESC">
                      <a:v>&lt;html&gt;
                            &lt;p&gt;This is a reference to one BSW module&apos;s configuration
                            (i.e. not the ECUC parameter definition template). Example,
                            there could be several configurations of LinIf and this
                            reference selects one of them.&lt;/p&gt;
                            &lt;p&gt;&lt;strong&gt;This parameter is not used.&lt;/strong&gt;&lt;/p&gt;
                          &lt;/html&gt;</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="SCOPE" value="LOCAL"/>
                    <a:a name="UUID" 
                         value="ECUC:bc97b938-7b4a-4243-8833-22afec289625"/>
                    <a:da name="ENABLE" value="false"/>
                    <a:da name="REF" 
                          value="ASTyped:ECUC-MODULE-CONFIGURATION-VALUES"/>
                  </v:ref>
                </v:ctr>
              </v:lst>
              <d:ref type="REFINED_MODULE_DEF" 
                     value="ASPath:/AUTOSAR/EcucDefs/SoAd"/>
              <v:lst name="SoAdConfig" type="MULTIPLE-CONFIGURATION-CONTAINER">
                <a:da name="MIN" value="1"/>
                <v:ctr name="SoAdConfig" 
                       type="MULTIPLE-CONFIGURATION-CONTAINER">
                  <a:a name="DESC">
                    <a:v>&lt;html&gt;
                          This container contains the configuration parameters and
                          sub containers of the AUTOSAR SoAd module. This container
                          is a MultipleConfigurationContainer, i.e. this container
                          and its sub-containers exist once per configuration set.
                        &lt;/html&gt;</a:v>
                  </a:a>
                  <v:lst name="SoAdPduRoute" type="MAP">
                    <a:da name="INVALID" type="Multi">
                      <mt:xpath 
                                false="SoAdTxPduIds of all SoAdPduRoutes must be zero-based and dense!" 
                                expr="(count(*) = 0) or (count(*) = (num:max(*/SoAdTxPduId)+1))"/>
                    </a:da>
                    <a:da name="MIN" value="0"/>
                    <v:ctr name="SoAdPduRoute" type="IDENTIFIABLE">
                      <a:a name="DESC">
                        <a:v>&lt;html&gt;
                              Describes the path of a PDU from an upper layer of the SoAd to the socket
                              in the TCP/IP stack for transmission.
                            &lt;/html&gt;</a:v>
                      </a:a>
                      <a:a name="POSTBUILDCHANGEABLE" value="true"/>
                      <a:a name="UUID" 
                           value="ECUC:db0b3473-ac6c-42ba-8d58-21770ad5d14f"/>
                      <v:var name="SoAdTxPduId" type="INTEGER">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                                Tx PDU ID of the PDU coming from the PDU Router.
                              &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="SCOPE" value="ECU"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="true"/>
                        <a:a name="UUID" 
                             value="ECUC:0607b193-60b3-4acf-8ca0-f903cc227bd7"/>
                        <a:da name="DEFAULT" value="0"/>
                        <a:da name="INVALID" type="Multi">
                          <mt:range>
                            <mt:tst expr="&lt;=65535"/>
                            <mt:tst expr="&gt;=0"/>
                          </mt:range>
                          <mt:xpath 
                                    false="SoAdTxPduIds of all SoAdPduRoutes must be unique." 
                                    expr="count(../../*[SoAdTxPduId=node:current()]) = 1"/>
                        </a:da>
                      </v:var>
                      <v:ref name="SoAdTxPduRef" type="REFERENCE">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                                Reference to the global PDU structure.
                              &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                        <a:a name="SCOPE" value="ECU"/>
                        <a:a name="UUID" 
                             value="ECUC:c753c894-c1eb-47ee-88e7-6396ea183e05"/>
                        <a:da name="INVALID" type="Multi">
                          <mt:xpath>
                            <mt:tst 
                                    false="SoAdTxPduRef needs to be configured to a valid reference!" 
                                    expr="node:refvalid(.)"/>
                            <mt:tst 
                                    true="Each global PDU must not be referenced by more than one SoAdPduRoute." 
                                    expr="node:refvalid(.) and count(../../*[SoAdTxPduRef = node:current()]) &gt; 1"/>
                          </mt:xpath>
                        </a:da>
                        <a:da name="REF" 
                              value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/EcuC/EcucPduCollection/Pdu"/>
                      </v:ref>
                      <v:var name="SoAdTxUpperLayerType" type="ENUMERATION">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                                Specifies the upper layer interface type (must be
                                &quot;If&quot; in case of multiple PduRoutes).
                              &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                        <a:a name="SCOPE" value="ECU"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" 
                             value="ECUC:56642173-affc-4c54-b407-ea215fc4a9d8"/>
                        <a:da name="DEFAULT" value="IF"/>
                        <a:da name="RANGE">
                          <a:v>IF</a:v>
                          <a:v>TP</a:v>
                        </a:da>
                      </v:var>
                      <v:var name="SoAdTxPduCollectionSemantics" 
                             type="ENUMERATION">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                                &lt;p&gt;Specifies if this PDU shall be collected using a queued or
                                last-is-best semantics.&lt;/p&gt;
                                &lt;p&gt;This parameter is only relevant if the PDU collection feature
                                is enabled.&lt;/p&gt;
                                &lt;p&gt;This feature get enabled if SoAdSocketnPduUdpTxBufferMin is used and SoAdTxUpperLayerType
                                is set to &apos;IF&apos;.&lt;/p&gt;
                                &lt;p&gt;All PDU routes refering to the same socket connnection or socket connection group shall have the same semantics
                                (Either SOAD_COLLECT_LAST_IS_BEST or SOAD_COLLECT_QUEUED).&lt;/p&gt;
                                &lt;p&gt;&lt;strong&gt;This parameter shall only be set to SOAD_COLLECT_LAST_IS_BEST if
                                the related upper layer is configured with SoAdIfTriggerTransmit
                                set to TRUE.&lt;/strong&gt;&lt;/p&gt;
                              &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                        <a:a name="SCOPE" value="LOCAL"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" 
                             value="ECUC:43fae156-8a10-43f9-bd37-89b31667ebec"/>
                        <a:da name="DEFAULT" value="SOAD_COLLECT_QUEUED"/>
                        <a:da name="ENABLE" type="XPath" 
                              expr="node:exists(node:ref(../SoAdPduRouteDest/*/SoAdTxSocketConnOrSocketConnBundleRef)/../../SoAdSocketProtocol/SoAdSocketnPduUdpTxBufferMin) and (../SoAdTxUpperLayerType = &apos;IF&apos;)"/>
                        <a:da name="RANGE">
                          <a:v>SOAD_COLLECT_LAST_IS_BEST</a:v>
                          <a:v>SOAD_COLLECT_QUEUED</a:v>
                        </a:da>
                      </v:var>
                      <v:lst name="SoAdPduRouteDest" type="MAP">
                        <a:da name="INVALID" type="Multi">
                          <mt:xpath>
                            <mt:tst 
                                    true="A SoAdPduRoute using SoAdTxUpperLayerType = TP and multiple SoAdPduRouteDest is only allowed if the PDU referenced by SoAdTxPduRef has a MetaDataTypeRef configured with MetaDataItem of type SOCKET_CONNECTION_ID_16." 
                                    expr="node:refvalid(../SoAdTxPduRef) and (../SoAdTxUpperLayerType = &apos;TP&apos;) and (count(./*) &gt; 1) and not(node:refvalid(node:ref(../SoAdTxPduRef)/MetaDataTypeRef))"/>
                            <mt:tst 
                                    true="The PDU referenced by SoAdTxPduRef has a MetaDataTypeRef configured but no MetaDataItem of type SOCKET_CONNECTION_ID_16. This is required if SoAdTxUpperLayerType = TP and multiple SoAdPduRouteDest are configured." 
                                    expr="node:refvalid(../SoAdTxPduRef) and (../SoAdTxUpperLayerType = &apos;TP&apos;)                                         and node:refvalid(node:ref(../SoAdTxPduRef)/MetaDataTypeRef)                                         and (count(./*) &gt; 1)                                         and (count(node:ref(node:ref(../SoAdTxPduRef)/MetaDataTypeRef)/MetaDataItem/*[./MetaDataItemType = &apos;SOCKET_CONNECTION_ID_16&apos;]) &lt; 1)"/>
                          </mt:xpath>
                        </a:da>
                        <a:da name="MIN" value="1"/>
                        <v:ctr name="SoAdPduRouteDest" type="IDENTIFIABLE">
                          <a:a name="DESC">
                            <a:v>&lt;html&gt;
                                  Describes the upper layer destination PDU for
                                  a message received on a TcpIp socket.
                                &lt;/html&gt;</a:v>
                          </a:a>
                          <a:a name="POSTBUILDCHANGEABLE" value="true"/>
                          <a:a name="UUID" 
                               value="ECUC:608916f0-a352-48d1-9911-cd629867e984"/>
                          <v:var name="SoAdTxPduHeaderId" type="INTEGER">
                            <a:a name="DESC">
                              <a:v>&lt;html&gt;
                                    ID to be sent on the TCP/IP connection if the PDU
                                    header option is enabled.
                                  &lt;/html&gt;</a:v>
                            </a:a>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                 type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v class="PostBuild">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="OPTIONAL" value="true"/>
                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                            <a:a name="POSTBUILDVARIANTMULTIPLICITY" 
                                 value="true"/>
                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                            <a:a name="SCOPE" value="LOCAL"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID" 
                                 value="ECUC:f9cd4c91-4e8f-404d-a6b9-1fa911afb222"/>
                            <a:da name="DEFAULT" value="0"/>
                            <a:da name="ENABLE" value="false"/>
                            <a:da name="INVALID" type="Multi">
                              <mt:range>
                                <mt:tst expr="&lt;4294967296"/>
                                <mt:tst expr="&gt;=0"/>
                              </mt:range>
                            </a:da>
                          </v:var>
                          <v:ref name="SoAdTxSocketConnOrSocketConnBundleRef" 
                                 type="CHOICE-REFERENCE">
                            <a:a name="DESC">
                              <a:v>&lt;html&gt;
                                    Choice Reference to a SocketConnection or to a 
                                    SocketConnectionGroup on which the PDU is to be sent on. The 
                                    reference to a SocketConnectionGroup shall only be used for 
                                    upper layers with IF API.
                                  &lt;/html&gt;</a:v>
                            </a:a>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                 type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                            <a:a name="SCOPE" value="LOCAL"/>
                            <a:a name="UUID" 
                                 value="ECUC:22ca0917-bfb5-45f5-9765-f859d37d0482"/>
                            <a:da name="INVALID" type="Multi">
                              <mt:xpath>
                                <mt:tst 
                                        false="SoAdTxSocketConnOrSocketConnBundleRef needs to be configured to a valid reference!" 
                                        expr="node:refvalid(.)"/>
                                <mt:tst 
                                        true="A SoAdPduRoute using SoAdTxUpperLayerType = TP and SoAdTxSocketConnOrSocketConnBundleRef referring to a socket connection group is only allowed if the PDU referenced by SoAdTxPduRef has a MetaDataTypeRef configured with MetaDataItem of type SOCKET_CONNECTION_ID_16." 
                                        expr="node:refvalid(../../../SoAdTxPduRef) and (../../../SoAdTxUpperLayerType = &apos;TP&apos;) and node:refvalid(.) and (count(text:split(string(../SoAdTxSocketConnOrSocketConnBundleRef),&apos;/&apos;)) = 5) and not(node:refvalid(node:ref(../../../SoAdTxPduRef)/MetaDataTypeRef))"/>
                                <mt:tst 
                                        true="The PDU referenced by SoAdTxPduRef has a MetaDataTypeRef configured but no MetaDataItem of type SOCKET_CONNECTION_ID_16. This is required if SoAdTxUpperLayerType = TP and a SoAdPduRouteDest has SoAdTxSocketConnOrSocketConnBundleRef configured to a socket connection group." 
                                        expr="node:refvalid(../../../SoAdTxPduRef) and (../../../SoAdTxUpperLayerType = &apos;TP&apos;) and node:refvalid(.) and (count(text:split(string(../SoAdTxSocketConnOrSocketConnBundleRef),&apos;/&apos;)) = 5) and node:refvalid(node:ref(../../../SoAdTxPduRef)/MetaDataTypeRef)                                          and (count(node:ref(node:ref(../../../SoAdTxPduRef)/MetaDataTypeRef)/MetaDataItem/*[./MetaDataItemType = &apos;SOCKET_CONNECTION_ID_16&apos;]) &lt; 1)"/>
                              </mt:xpath>
                            </a:da>
                            <a:da name="REF">
                              <a:v>ASPathDataOfSchema:/AUTOSAR/EcucDefs/SoAd/SoAdConfig/SoAdSocketConnectionGroup/SoAdSocketConnection</a:v>
                              <a:v>ASPathDataOfSchema:/AUTOSAR/EcucDefs/SoAd/SoAdConfig/SoAdSocketConnectionGroup</a:v>
                            </a:da>
                          </v:ref>
                          <v:lst name="SoAdTxRoutingGroupRef">
                            <v:ref name="SoAdTxRoutingGroupRef" 
                                   type="REFERENCE">
                              <a:a name="DESC">
                                <a:v>&lt;html&gt;
                                      Reference to the routing group.
                                    &lt;/html&gt;</a:v>
                              </a:a>
                              <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                   type="IMPLEMENTATIONCONFIGCLASS">
                                <icc:v class="PostBuild">VariantPostBuild</icc:v>
                              </a:a>
                              <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                              <a:a name="POSTBUILDVARIANTMULTIPLICITY" 
                                   value="true"/>
                              <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                              <a:a name="SCOPE" value="LOCAL"/>
                              <a:a name="UUID" 
                                   value="ECUC:1ffd982b-fc0d-4ce9-aab8-4b67e71bd8e7"/>
                              <a:da name="INVALID" type="Multi">
                                <mt:xpath 
                                          false="SoAdTxRoutingGroupRef needs to be configured to a valid reference!" 
                                          expr="node:refvalid(.)"/>
                              </a:da>
                              <a:da name="REF" 
                                    value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/SoAd/SoAdConfig/SoAdRoutingGroup"/>
                            </v:ref>
                          </v:lst>
                          <v:var name="SoAdTxUdpTriggerMode" type="ENUMERATION">
                            <a:a name="DESC">
                              <a:v>&lt;html&gt;
                                    &lt;p&gt;Specifies whether a PDU triggers the transmission of the
                                    nPduUdpTxBuffer.&lt;/p&gt;
                                    &lt;p&gt;If this parameter is set to TRIGGER_NEVER, SoAd shall use an
                                    nPduUdpTxBuffer for the related socket connection.&lt;/p&gt;
                                    &lt;p&gt;nPduUdpTxBuffer can only be used for upper layers with IF
                                    API, i.e. this parameter shall only be set to TRIGGER_NEVER if
                                    all upper layers belonging to the related socket connection have
                                    SoAdTxUpperLayerType set to &quot;IF&quot;.&lt;/p&gt;
                                    &lt;p&gt;&lt;strong&gt;This parameter is only relevant for UDP connections
                                    that have SoAdSocketnPduUdpTxBufferMin enabled.&lt;/strong&gt;&lt;/p&gt;
                                  &lt;/html&gt;</a:v>
                            </a:a>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                 type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                            <a:a name="SCOPE" value="LOCAL"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID" 
                                 value="ECUC:8a5d9e39-36fb-4b39-8c27-859278c3f153"/>
                            <a:da name="DEFAULT" value="TRIGGER_ALWAYS"/>
                            <a:da name="ENABLE" type="XPath" 
                                  expr="node:refvalid(../SoAdTxSocketConnOrSocketConnBundleRef) and                                      (   (((node:ref(../SoAdTxSocketConnOrSocketConnBundleRef)/../../SoAdSocketProtocol = &apos;SoAdSocketUdp&apos;) and node:exists(node:ref(../SoAdTxSocketConnOrSocketConnBundleRef)/../../SoAdSocketProtocol/SoAdSocketnPduUdpTxBufferMin)))                                       or (((node:ref(../SoAdTxSocketConnOrSocketConnBundleRef)/SoAdSocketProtocol = &apos;SoAdSocketUdp&apos;) and node:exists(node:ref(../SoAdTxSocketConnOrSocketConnBundleRef)/SoAdSocketProtocol/SoAdSocketnPduUdpTxBufferMin))) )"/>
                            <a:da name="RANGE">
                              <a:v>TRIGGER_ALWAYS</a:v>
                              <a:v>TRIGGER_NEVER</a:v>
                            </a:da>
                            <a:da name="WARNING" type="XPath">
                              <a:tst 
                                     expr=".=&apos;TRIGGER_NEVER&apos; and node:containsValue(../../../../*/SoAdPduRouteDest/*[SoAdTxSocketConnOrSocketConnBundleRef = node:current()/../SoAdTxSocketConnOrSocketConnBundleRef]/../../SoAdTxUpperLayerType,&apos;TP&apos;)" 
                                     true="The nPduUdpTx feature cannot be enabled on SocketConnections that are also used by upper layers that use the TP-API. There is at least one SoAdPduRoute that is configured with SoAdTxUpperLayerType set to TP that also refers to the same SocketConnection."/>
                              <a:tst 
                                     expr="((node:refvalid(../../../SoAdTxPduRef)) and (as:ref(../SoAdTxSocketConnOrSocketConnBundleRef)/../../SoAdPduHeaderEnable = &apos;true&apos;) and (num:i(as:ref(../../../SoAdTxPduRef)/PduLength + 8) &gt; num:i(as:ref(../SoAdTxSocketConnOrSocketConnBundleRef)/../../SoAdSocketProtocol/SoAdSocketnPduUdpTxBufferMin)))" 
                                     true="The length of the referenced PDU (PduHeader + Payload) is larger than the configured nPduUdpTx buffer. This PDU cannot be stored in the buffer. It will be immediately transmitted independent of the SoAdTxUdpTriggerMode setting."/>
                            </a:da>
                          </v:var>
                          <v:var name="SoAdTxUdpTriggerTimeout" type="FLOAT">
                            <a:a name="DESC">
                              <a:v>&lt;html&gt;
                                    &lt;p&gt;Specifies the timeout in [s] the nPduUdpTxBuffer shall be
                                    transmitted at the latest after this PDU is put into the buffer.
                                    This optional parameter is only relevant if SoAdTxUdpTriggerMode
                                    is TRIGGER_NEVER.&lt;/p&gt;
                                  &lt;/html&gt;</a:v>
                            </a:a>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                 type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v class="PostBuild">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="OPTIONAL" value="true"/>
                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                            <a:a name="POSTBUILDVARIANTMULTIPLICITY" 
                                 value="true"/>
                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                            <a:a name="SCOPE" value="LOCAL"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID" 
                                 value="ECUC:c9ca6016-a93c-4495-8899-231966519c98"/>
                            <a:da name="DEFAULT" value="0.1"/>
                            <a:da name="ENABLE" value="false"/>
                            <a:da name="INVALID" type="Multi">
                              <mt:range>
                                <mt:tst expr="&lt;=1000000000.0"/>
                                <mt:tst expr="&gt;=0.0"/>
                              </mt:range>
                            </a:da>
                            <a:da name="WARNING" type="XPath">
                              <a:tst 
                                     expr="(num:i(. * 1000000) mod num:i(as:modconf(&apos;SoAd&apos;)[1]/SoAdGeneral/SoAdMainFunctionPeriod * 1000000)) = 0" 
                                     false="SoAdTxUdpTriggerTimeout should be a decimal multiple of SoAdMainFunctionPeriod. If this is not the case the timeout value will be rounded down."/>
                              <a:tst expr=". = 0.0" 
                                     true="SoAdTxUdpTriggerTimeout set to 0 will have no effect."/>
                              <a:tst 
                                     expr="((.) &lt; (as:modconf(&apos;SoAd&apos;)[1]/SoAdGeneral/SoAdMainFunctionPeriod))" 
                                     true="SoAdTxUdpTriggerTimeout should be greater than configured SoAdMainFunctionPeriod. If this is not the case, it will be rounded down to 0 which actually means that it&apos;s disabled."/>
                            </a:da>
                          </v:var>
                        </v:ctr>
                      </v:lst>
                      <v:var name="SoAdSkipIfTxConfirmation" type="BOOLEAN">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                                Configuration parameter to disabling TxConfirmation for
                                SoAd_IfTransmit() call.
                                
                                &lt;ul&gt;
                                  &lt;li&gt;&lt;code&gt;TRUE&lt;/code&gt;: Tx confirmation gets skipped for this PDU.&lt;/li&gt;
                                  &lt;li&gt;&lt;code&gt;FALSE&lt;/code&gt;: Tx confirmation API will be called for this PDU if transmission was successful.&lt;/li&gt;
                                &lt;/ul&gt;
                                &lt;p&gt;&lt;strong&gt;Optimization Effect:&lt;/strong&gt;&lt;/p&gt;
                                &lt;ul&gt;
                                  &lt;li class=&quot;optimization.code.exetime.reduction&quot;&gt;
                                    &lt;strong&gt;Runtime Reduction&lt;/strong&gt;
                                    Enabling this parameter will reduces the runtime consumption of the modules tx confirmations.
                                  &lt;/li&gt;
                                &lt;/ul&gt;
                              &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="Elektrobit Automotive GmbH"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:da name="DEFAULT" value="FALSE"/>
                      </v:var>
                    </v:ctr>
                  </v:lst>
                  <v:lst name="SoAdSocketConnectionGroup" type="MAP">
                    <a:da name="INVALID" type="Multi">
                      <mt:xpath>
                        <mt:tst 
                                true="SoAdSocketIds of all SoAdSocketConnections must be zero-based and dense!" 
                                expr="count(./*/SoAdSocketConnection/*/SoAdSocketId) != (num:max(./*/SoAdSocketConnection/*/SoAdSocketId)+1)"/>
                        <mt:tst 
                                true="The number of SoAd TCP Socket Connection exceeds the number of TcpIp TCP Sockets (TcpIp/TcpIpGeneral/TcpIpTcpSocketMax)" 
                                expr="node:exists(as:modconf(&apos;TcpIp&apos;)/TcpIpGeneral/TcpIpTcpSocketMax)                                        and (as:modconf(&apos;TcpIp&apos;)/TcpIpGeneral/TcpIpTcpSocketMax &lt;                                        (count(*[SoAdSocketProtocol=&apos;SoAdSocketTcp&apos;  and (node:value(node:ref(./SoAdSocketLocalAddressRef)/TcpIpDomainType) = &apos;TCPIP_AF_INET&apos;)]/SoAdSocketConnection/*) +                                        count(*[SoAdSocketProtocol=&apos;SoAdSocketTcp&apos; and (node:value(node:ref(./SoAdSocketLocalAddressRef)/TcpIpDomainType) = &apos;TCPIP_AF_INET&apos;)]/*[SoAdSocketTcpInitiate=&apos;false&apos;])) )"/>
                        <mt:tst 
                                true="The number of SoAd TCP Ipv6 Socket Connection exceeds the number of TcpIp TCP Ipv6 Sockets (TcpIp/TcpIpGeneral/TcpIpTcpV6SocketMax)" 
                                expr="node:exists(as:modconf(&apos;TcpIp&apos;)/TcpIpGeneral/TcpIpTcpV6SocketMax)                                        and (as:modconf(&apos;TcpIp&apos;)/TcpIpGeneral/TcpIpTcpV6SocketMax &lt;                                        (count(*[SoAdSocketProtocol=&apos;SoAdSocketTcp&apos; and (node:value(node:ref(./SoAdSocketLocalAddressRef)/TcpIpDomainType) = &apos;TCPIP_AF_INET6&apos;)]/SoAdSocketConnection/*) +                                        count(*[SoAdSocketProtocol=&apos;SoAdSocketTcp&apos; and (node:value(node:ref(./SoAdSocketLocalAddressRef)/TcpIpDomainType) = &apos;TCPIP_AF_INET6&apos;)]/*[SoAdSocketTcpInitiate=&apos;false&apos;])) )"/>
                        <mt:tst 
                                true="The number of SoAd UDP Socket Connection exceeds the number of TcpIp UDP Sockets (TcpIp/TcpIpGeneral/TcpIpUdpSocketMax)" 
                                expr="node:exists(as:modconf(&apos;TcpIp&apos;)/TcpIpGeneral/TcpIpUdpSocketMax)                                        and (as:modconf(&apos;TcpIp&apos;)/TcpIpGeneral/TcpIpUdpSocketMax &lt;                                        count(*[SoAdSocketProtocol=&apos;SoAdSocketUdp&apos; and (node:value(node:ref(./SoAdSocketLocalAddressRef)/TcpIpDomainType) = &apos;TCPIP_AF_INET&apos;)]/SoAdSocketConnection/*))"/>
                        <mt:tst 
                                true="The number of SoAd UDP Ipv6 Socket Connection exceeds the number of TcpIp UDP Ipv6 Sockets (TcpIp/TcpIpGeneral/TcpIpUdpV6SocketMax)" 
                                expr="node:exists(as:modconf(&apos;TcpIp&apos;)/TcpIpGeneral/TcpIpUdpV6SocketMax)                                        and (as:modconf(&apos;TcpIp&apos;)/TcpIpGeneral/TcpIpUdpV6SocketMax &lt;                                        count(*[(SoAdSocketProtocol=&apos;SoAdSocketUdp&apos;) and (node:value(node:ref(./SoAdSocketLocalAddressRef)/TcpIpDomainType) = &apos;TCPIP_AF_INET6&apos;)]/SoAdSocketConnection/*))"/>
                      </mt:xpath>
                    </a:da>
                    <a:da name="MIN" value="1"/>
                    <v:ctr name="SoAdSocketConnectionGroup" type="IDENTIFIABLE">
                      <a:a name="DESC">
                        <a:v>&lt;html&gt;
                              Specifies the configuration of a socket connection group, i.e.
                              specifies the socket connections belonging to the group and the
                              parameters which are common for all socket connections of the
                              group. A socket connection specifies how data can be received
                              and transmited via a TCP or UDP socket.
                            &lt;/html&gt;</a:v>
                      </a:a>
                      <a:a name="POSTBUILDCHANGEABLE" value="true"/>
                      <a:a name="UUID" 
                           value="ECUC:abae0f3b-aa69-47ae-82df-bac364d52b6c"/>
                      <a:da name="INVALID" type="Multi">
                        <mt:xpath 
                                  true="Not more than one SocketConnectionGroup shall be configured for the same LocalAddress, the same LocalPort and the same Protocol!" 
                                  expr="(SoAdSocketLocalPort != 0) and                                         (count(../*[((SoAdSocketLocalPort=node:value(node:current()/SoAdSocketLocalPort)) and                                                  SoAdSocketLocalAddressRef=node:value(node:current()/SoAdSocketLocalAddressRef)) and                                                  SoAdSocketProtocol=node:current()/SoAdSocketProtocol]) &gt; 1)"/>
                      </a:da>
                      <v:var name="SoAdPduHeaderEnable" type="BOOLEAN">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                                &lt;p&gt;Enables the transmission of the PDU header (ID, length)
                                on this socket connection.&lt;/p&gt;
                                &lt;ul&gt;
                                  &lt;li&gt;true: Send PDU header before data.&lt;/li&gt;
                                  &lt;li&gt;false: Send data only.&lt;/li&gt;
                                &lt;/ul&gt;
                              &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                        <a:a name="SCOPE" value="LOCAL"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" 
                             value="ECUC:410309ba-cede-4382-895d-35b0025664f5"/>
                        <a:da name="DEFAULT" value="true"/>
                      </v:var>
                      <v:var name="SoAdResourceManagementEnable" type="BOOLEAN">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                                &lt;p&gt;&lt;em&gt;The functionality related to this parameter is not
                                supported by the current implementation.&lt;/em&gt;&lt;/p&gt;
                                &lt;p&gt;
                                  Enables the resource management option for this socket.
                                  May not be activated for UDP sockets in receive and not
                                  for DoIP sockets.
                                &lt;/p&gt;
                                &lt;ul&gt;
                                  &lt;li&gt;true: resource management option enabled.&lt;/li&gt;
                                  &lt;li&gt;false: resource management option disabled.&lt;/li&gt;
                                &lt;/ul&gt;
                              &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                        <a:a name="SCOPE" value="LOCAL"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" 
                             value="ECUC:9d03a035-be38-49bb-9cb5-6c542059bffc"/>
                        <a:da name="DEFAULT" value="false"/>
                        <a:da name="EDITABLE" value="false"/>
                      </v:var>
                      <v:ref name="SoAdSocketLocalAddressRef" 
                             type="SYMBOLIC-NAME-REFERENCE">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                                Local IP address and interface used for this connection.
                              &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                        <a:a name="SCOPE" value="LOCAL"/>
                        <a:a name="UUID" 
                             value="ECUC:5f8d7d9a-ea26-4a66-b188-8f9a5d39e808"/>
                        <a:da name="INVALID" type="Multi">
                          <mt:xpath 
                                    false="SoAdSocketLocalAddressRef needs to be configured to a valid reference!" 
                                    expr="node:refvalid(.)"/>
                        </a:da>
                        <a:da name="REF" 
                              value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/TcpIp/TcpIpConfig/TcpIpLocalAddr"/>
                      </v:ref>
                      <v:var name="SoAdSocketLocalPort" type="INTEGER">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                                Local UDP or TCP port used for this connection. If this parameter
                                set to 0 SoAd requests TcpIp to select an ephemeral port.
                              &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                        <a:a name="SCOPE" value="LOCAL"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" 
                             value="ECUC:c6e53314-62e5-4f81-8f47-3c5cb6a19260"/>
                        <a:da name="DEFAULT" value="0"/>
                        <a:da name="INVALID" type="Multi">
                          <mt:range>
                            <mt:tst expr="&lt;=65535"/>
                            <mt:tst expr="&gt;=0"/>
                          </mt:range>
                        </a:da>
                      </v:var>
                      <v:var name="SoAdSocketAutomaticSoConSetup" 
                             type="BOOLEAN">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                                &lt;p&gt;
                                  Specifies if the setup of the socket connection shall be
                                  done automatically or manually.
                                &lt;/p&gt;
                                &lt;ul&gt;
                                  &lt;li&gt;true: Setup done automatically.&lt;/li&gt;
                                  &lt;li&gt;false: Setup done manually via SoAd_OpenSoCon() and
                                    SoAd_CloseSoCon().&lt;/li&gt;
                                &lt;/ul&gt;
                              &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                        <a:a name="SCOPE" value="LOCAL"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" 
                             value="ECUC:40c95585-89c4-4ca2-ae2d-5a0c639ea0c4"/>
                        <a:da name="DEFAULT" value="true"/>
                      </v:var>
                      <v:var name="SoAdSocketSoConModeChgNotification" 
                             type="BOOLEAN">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                                Specifies if the SoCon mode change notification callback
                                function of the upper layer shall be called in case of SoCon
                                mode change.
                              &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                        <a:a name="SCOPE" value="LOCAL"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" 
                             value="ECUC:2e4db225-90bc-4a6c-9d2e-b1f84b63318b"/>
                        <a:da name="DEFAULT" value="false"/>
                      </v:var>
                      <v:var name="SoAdSocketIpAddrAssignmentChgNotification" 
                             type="BOOLEAN">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                                Specifies if the local IP address assignment change
                                notification callback function of the upper layer shall
                                be called if the assignment of the local IP address used
                                by this socket connection changes.
                              &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                        <a:a name="SCOPE" value="LOCAL"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" 
                             value="ECUC:4ceb0609-a4e0-4022-bb2c-63f13206cd82"/>
                        <a:da name="DEFAULT" value="false"/>
                      </v:var>
                      <v:var name="SoAdSocketTpRxBufferMin" type="INTEGER">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                                &lt;p&gt;Specifies the amount of data in bytes (PDU data for the upper layer and PDU Header if used) the SoAd shall be able to buffer for data reception via this socket connection and using an upper layer with TP.&lt;/p&gt;
                                &lt;p&gt;Note: in case of a TCP socket where PduHeaderMode is used
                                and an upper layer with IF-API, the required buffer size can
                                be determined automatically.&lt;/p&gt;
                              &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v class="PostBuild">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="OPTIONAL" value="true"/>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="POSTBUILDVARIANTMULTIPLICITY" value="true"/>
                        <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                        <a:a name="SCOPE" value="LOCAL"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" 
                             value="ECUC:f1daede7-ba0e-4dbc-9ee7-6c3e6299c67b"/>
                        <a:da name="DEFAULT" value="0"/>
                        <a:da name="ENABLE" value="false"/>
                        <a:da name="INVALID" type="Multi">
                          <mt:range>
                            <mt:tst expr="&lt;=65535"/>
                            <mt:tst expr="&gt;=0"/>
                          </mt:range>
                        </a:da>
                      </v:var>
                      <v:var name="SoAdSocketMsgAcceptanceFilterEnabled" 
                             type="BOOLEAN">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                                &lt;p&gt;
                                  Specifies if the message acceptance filter is enabled (TRUE) or not (FALSE).
                                &lt;/p&gt;
                                &lt;p&gt;
                                  Note: If a wildcard is used in SoAdSocketRemoteAddress AND SoAdSocketUdpListenOnly is FALSE, this parameter must be enabled.
                                &lt;/p&gt;
                                &lt;ul&gt;
                                  &lt;li&gt;true: The message acceptance filter is enabled.&lt;/li&gt;
                                  &lt;li&gt;false: The message acceptance filter is disabled.&lt;/li&gt;
                                &lt;/ul&gt;
                              &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                        <a:a name="SCOPE" value="LOCAL"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" 
                             value="ECUC:f1edc6f3-aa71-429b-9005-d78fe8ff68d3"/>
                        <a:da name="DEFAULT" value="true"/>
                        <a:da name="INVALID" type="Multi">
                          <mt:xpath>
                            <mt:tst 
                                    true="If a wildcard is used in SoAdSocketRemoteAddress AND SoAdSocketUdpListenOnly is FALSE, SoAdSocketMsgAcceptanceFilterEnabled parameter must be enabled." 
                                    expr="(.=&apos;false&apos;) and (../SoAdSocketProtocol/SoAdSocketUdpListenOnly = &apos;false&apos;)                                       and (node:containsValue(../SoAdSocketConnection/*/SoAdSocketRemoteAddress/SoAdSocketRemoteIpAddress,&apos;ANY&apos;)                                       or node:containsValue(../SoAdSocketConnection/*/SoAdSocketRemoteAddress/SoAdSocketRemoteIpAddress,&apos;0.0.0.0&apos;)                                       or node:containsValue(../SoAdSocketConnection/*/SoAdSocketRemoteAddress/SoAdSocketRemotePort,&apos;0&apos;))"/>
                            <mt:tst 
                                    true="The message acceptance filter must be active for SoAdSocketConnectionGroups with multiple SoAdSocketConnections." 
                                    expr="(.=&apos;false&apos;) and (count(../SoAdSocketConnection/*) &gt; 1)"/>
                          </mt:xpath>
                        </a:da>
                      </v:var>
                      <v:var name="SoAdSocketFramePriority" type="INTEGER">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                                &lt;p&gt;Specifies the priority of the Ethernet frames.&lt;/p&gt;
                              &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v class="PostBuild">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="OPTIONAL" value="true"/>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="POSTBUILDVARIANTMULTIPLICITY" value="true"/>
                        <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                        <a:a name="SCOPE" value="LOCAL"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" 
                             value="ECUC:785737d3-b1bb-46f7-be70-a3a689d38251"/>
                        <a:da name="DEFAULT" value="0"/>
                        <a:da name="ENABLE" value="false"/>
                        <a:da name="INVALID" type="Multi">
                          <mt:range>
                            <mt:tst expr="&lt;=7"/>
                            <mt:tst expr="&gt;=0"/>
                          </mt:range>
                        </a:da>
                      </v:var>
                      <v:lst name="SoAdSocketConnection" type="MAP">
                        <a:da name="MIN" value="1"/>
                        <v:ctr name="SoAdSocketConnection" type="IDENTIFIABLE">
                          <a:a name="DESC">
                            <a:v>&lt;html&gt;
                                  &lt;p&gt;Specifies the socket connection (Id and remote address
                                  information).&lt;/p&gt;
                                  &lt;p&gt;Note: Parameters which are common to all socket
                                  connections of a socket connection group are specified
                                  directly at the group. &lt;/p&gt;
                                &lt;/html&gt;</a:v>
                          </a:a>
                          <a:a name="POSTBUILDCHANGEABLE" value="true"/>
                          <a:a name="UUID" 
                               value="ECUC:e21ff4b2-9c1b-4704-aacb-498a1675c0ca"/>
                          <a:da name="INVALID" type="Multi">
                            <mt:xpath 
                                      false="SoAdSocketConnections with ../SoAdSocketAutomaticSoConSetup set to TRUE require a SoAdSocketRemoteAddress." 
                                      expr="(../../SoAdSocketAutomaticSoConSetup = &apos;false&apos;)                                             or (node:exists(SoAdSocketRemoteAddress))"/>
                          </a:da>
                          <v:var name="SoAdSocketId" type="INTEGER">
                            <a:a name="DESC">
                              <a:v>&lt;html&gt;
                                    Socket connection identifier used as SoConId in the interaction
                                    with upper layers.
                                  &lt;/html&gt;</a:v>
                            </a:a>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                 type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                            <a:a name="SCOPE" value="ECU"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="true"/>
                            <a:a name="UUID" 
                                 value="ECUC:f4c911ee-b2ee-42a9-9193-e80a9e168aca"/>
                            <a:da name="DEFAULT" value="0"/>
                            <a:da name="INVALID" type="Multi">
                              <mt:range>
                                <mt:tst expr="&lt;=65534"/>
                                <mt:tst expr="&gt;=0"/>
                              </mt:range>
                              <mt:xpath>
                                <mt:tst 
                                        false="SoAdSocketIds of SoAdSocketConnectionGroups must be consecutive." 
                                        expr="num:i(.) = ( num:i(../../*[1]/SoAdSocketId) + num:i(../@index))"/>
                                <mt:tst 
                                        false="SoAdSocketIds of all SoAdSocketConnectionGroups must be unique." 
                                        expr="count(../../../../*/SoAdSocketConnection/*[SoAdSocketId=node:current()]) = 1"/>
                              </mt:xpath>
                            </a:da>
                          </v:var>
                          <v:ref name="SoAdTlsConnectionRef" type="REFERENCE">
                            <a:a name="DESC">
                              <a:v>&lt;html&gt;
                                    Reference to the TLS connection.
                                  &lt;/html&gt;</a:v>
                            </a:a>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                 type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v class="PostBuild">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="OPTIONAL" value="true"/>
                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                            <a:a name="SCOPE" value="ECU"/>
                            <a:da name="ENABLE" value="false"/>
                            <a:da name="INVALID" type="Multi">
                              <mt:xpath>
                                <mt:tst 
                                        false="SoAdTlsConnectionRef needs to be configured to a valid reference!" 
                                        expr="node:refvalid(.)"/>
                                <mt:tst 
                                        true="SoAdTlsConnectionRef can&apos;t be configurable if SoAdTlsEnabled is disabled." 
                                        expr="(node:exists(.) and (as:modconf(&apos;SoAd&apos;)[1]/SoAdGeneral/SoAdTlsEnabled = &apos;false&apos;))"/>
                              </mt:xpath>
                            </a:da>
                            <a:da name="REF" 
                                  value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/Tls/TlsConnection"/>
                          </v:ref>
                          <v:ctr name="SoAdSocketRemoteAddress" 
                                 type="IDENTIFIABLE">
                            <a:a name="DESC">
                              <a:v>&lt;html&gt;
                                    Containter to specify the remote address (IP address and port)
                                    for a socket connection.
                                    If SoAdSocketRemoteAddress is not specified the remote address
                                    has to be set by the upper layer via SoAd_SetRemoteAddr().
                                  &lt;/html&gt;</a:v>
                            </a:a>
                            <a:a name="OPTIONAL" value="true"/>
                            <a:a name="UUID" 
                                 value="ECUC:61d6f4d9-7fe5-44a5-9c03-17ad0895a2f9"/>
                            <a:da name="ENABLE" value="true"/>
                            <v:var name="SoAdSocketRemoteIpAddress" 
                                   type="STRING">
                              <a:a name="DESC">
                                <a:v>&lt;html&gt;
                                      IP address of remote node. The configured address must be of
                                      the same TcpIpDomainType (i.e. IPv4 or IPv6) as the
                                      TcpIpLocalAddr referred by SoAdSocketLocalAddressRef.
                                      To accept any remote IP address, set
                                      SoAdSocketRemoteIpAddress to &quot;ANY&quot;.
                                      See message acceptance policy for more details.
                                    &lt;/html&gt;</a:v>
                              </a:a>
                              <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                   type="IMPLEMENTATIONCONFIGCLASS">
                                <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                              </a:a>
                              <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                              <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                              <a:a name="SCOPE" value="LOCAL"/>
                              <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                              <a:a name="UUID" 
                                   value="ECUC:bb1fc885-6e6e-4cf5-9df0-d8ccbdbe059f"/>
                              <a:da name="DEFAULT" value="ANY"/>
                              <a:da name="INVALID" type="Multi">
                                <mt:xpath>
                                  <mt:tst 
                                          true="Please specify a valid IPv4 remote address, e.g. 192.168.0.1." 
                                          expr="(.!=&apos;ANY&apos;) and (node:refvalid(../../../../SoAdSocketLocalAddressRef)) and (node:ref(../../../../SoAdSocketLocalAddressRef)/TcpIpDomainType = &apos;TCPIP_AF_INET&apos;) and not(text:match(.,&apos;^([0-9]{1,3}\.){3}[0-9]{1,3}$&apos;))"/>
                                  <mt:tst 
                                          true="Please specify a valid, unabbreviated IPv6 remote address, e.g. 2001:DB8:0:0:8:800:200C:417A." 
                                          expr="(.!=&apos;ANY&apos;) and (node:refvalid(../../../../SoAdSocketLocalAddressRef)) and                                          (node:ref(../../../../SoAdSocketLocalAddressRef)/TcpIpDomainType = &apos;TCPIP_AF_INET6&apos;) and not(text:match(.,&apos;^([a-fA-F0-9]{1,4}:){7}([a-fA-F0-9]{1,4})$&apos;))"/>
                                </mt:xpath>
                              </a:da>
                              <a:da name="WARNING" type="XPath" expr=".=&apos;&apos;" 
                                    true="Remote Ip Address field must not be empty. Please specify an address or use &apos;ANY&apos; as wild card."/>
                            </v:var>
                            <v:var name="SoAdSocketRemotePort" type="INTEGER">
                              <a:a name="DESC">
                                <a:v>&lt;html&gt;
                                      Remote UDP or TCP port used for this connection.
                                      To accept any remote port, set SoAdSocketRemotePort
                                      to 0. See message acceptance policy for more details.
                                    &lt;/html&gt;</a:v>
                              </a:a>
                              <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                   type="IMPLEMENTATIONCONFIGCLASS">
                                <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                              </a:a>
                              <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                              <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                              <a:a name="SCOPE" value="LOCAL"/>
                              <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                              <a:a name="UUID" 
                                   value="ECUC:d92ba3b3-f3db-4249-877f-97905d97d603"/>
                              <a:da name="DEFAULT" value="0"/>
                              <a:da name="INVALID" type="Multi">
                                <mt:range>
                                  <mt:tst expr="&lt;=65535"/>
                                  <mt:tst expr="&gt;=0"/>
                                </mt:range>
                              </a:da>
                            </v:var>
                          </v:ctr>
                        </v:ctr>
                      </v:lst>
                      <v:chc name="SoAdSocketProtocol" type="IDENTIFIABLE">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                                Specifies the transport protocol and transport protocol specific
                                parameters used for the socket connections of the socket connection
                                group.
                              &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="UUID" 
                             value="ECUC:eb8a2965-1cce-423b-be4e-e617127d909f"/>
                        <v:ctr name="SoAdSocketUdp" type="IDENTIFIABLE">
                          <a:a name="DESC">
                            <a:v>&lt;html&gt;
                                  Specifies that UDP is used as transport protocol for the
                                  socket connection group and parameters only related to UDP
                                  socket connections.
                                &lt;/html&gt;</a:v>
                          </a:a>
                          <a:a name="UUID" 
                               value="ECUC:7365929a-7420-4e93-b00a-6f1576bd11d9"/>
                          <v:var name="SoAdSocketUdpAliveSupervisionTimeout" 
                                 type="FLOAT">
                            <a:a name="DESC">
                              <a:v>&lt;html&gt;
                                    &lt;p&gt;Specifies the time in [s] a UDP socket connection remains
                                    in the mode SOAD_SOCON_ONLINE after the latest reception of a
                                    frame from the remote peer specified by the remote address.
                                    If this optional parameter is not enabled UDP Alive Supervision
                                    is deactivated for the related socket connection group.&lt;/p&gt;
                                  &lt;/html&gt;</a:v>
                            </a:a>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                 type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v class="PostBuild">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="OPTIONAL" value="true"/>
                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                            <a:a name="POSTBUILDVARIANTMULTIPLICITY" 
                                 value="true"/>
                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                            <a:a name="SCOPE" value="LOCAL"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID" 
                                 value="ECUC:f35c1589-030c-40d2-9757-9878b3c6bbd1"/>
                            <a:da name="ENABLE" value="false"/>
                            <a:da name="INVALID" type="Multi">
                              <mt:range>
                                <mt:tst expr="&lt;=1000000000.0"/>
                                <mt:tst expr="&gt;=0.0"/>
                              </mt:range>
                            </a:da>
                            <a:da name="WARNING" type="XPath">
                              <a:tst 
                                     expr="(num:i(. * 1000000) mod num:i(as:modconf(&apos;SoAd&apos;)[1]/SoAdGeneral/SoAdMainFunctionPeriod * 1000000)) = 0" 
                                     false="SoAdSocketUdpAliveSupervisionTimeout should be a decimal multiple of SoAdMainFunctionPeriod. If this is not the case the timeout value will be rounded down."/>
                              <a:tst expr=". = 0.0" 
                                     true="SoAdSocketUdpAliveSupervisionTimeout set to 0 will have no effect."/>
                              <a:tst 
                                     expr="((.) &lt; (as:modconf(&apos;SoAd&apos;)[1]/SoAdGeneral/SoAdMainFunctionPeriod))" 
                                     true="SoAdSocketUdpAliveSupervisionTimeout should be greater than configured SoAdMainFunctionPeriod. If this is not the case, it will be rounded up to SoAdMainFunctionPeriod"/>
                            </a:da>
                          </v:var>
                          <v:var name="SoAdSocketUdpListenOnly" type="BOOLEAN">
                            <a:a name="DESC">
                              <a:v>&lt;html&gt;
                                    &lt;p&gt;
                                      Specifies if the socket connection group is only used for reception or used for both reception and transmission. &lt;/p&gt;
                                    &lt;ul&gt;
                                      &lt;li&gt;true: This UDP port cannot transmit data.&lt;/li&gt;
                                      &lt;li&gt;false: This UDP port can send and receive data.&lt;/li&gt;
                                    &lt;/ul&gt;
                                  &lt;/html&gt;</a:v>
                            </a:a>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                 type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                            <a:a name="SCOPE" value="LOCAL"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID" 
                                 value="ECUC:d290a4ee-dd54-4e7d-b4d2-1df95c069f5b"/>
                            <a:da name="DEFAULT" value="false"/>
                          </v:var>
                          <v:var name="SoAdSocketUdpRetryEnabled" 
                                 type="BOOLEAN">
                            <a:a name="DESC">
                              <a:v>&lt;html&gt;
                                    &lt;p&gt;
                                    Specifies if an UdpTxBuffer shall be used to retry an UDP
                                    transmission in case of TCPIP_E_PHYS_ADDR_MISS or not.
                                    &lt;/p&gt;
                                    &lt;ul&gt;
                                      &lt;li&gt;true: A UdpTxBuffer is used to retry a UDP
                                      transmission&lt;/li&gt;
                                      &lt;li&gt;false: A UDP transmission is not retried, if not
                                      successful.&lt;/li&gt;
                                    &lt;/ul&gt;
                                  &lt;/html&gt;</a:v>
                            </a:a>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                 type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:da name="DEFAULT" value="false"/>
                          </v:var>
                          <v:var 
                                 name="SoAdSocketUdpStrictHeaderLenCheckEnabled" 
                                 type="BOOLEAN">
                            <a:a name="DESC">
                              <a:v>&lt;html&gt;
                                    &lt;p&gt;Specifies if UDP messages shall be dropped (TRUE) if the
                                    length of all contained PDUs does not match the length of the
                                    whole message or not (FALSE). Shall only be set to TRUE if
                                    SoAdPduHeaderEnable is also set to TRUE.&lt;/p&gt;
                                  &lt;/html&gt;</a:v>
                            </a:a>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                 type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                            <a:a name="SCOPE" value="LOCAL"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID" 
                                 value="ECUC:0c09f922-af2c-43c1-a903-fa88c9819ddb"/>
                            <a:da name="DEFAULT" value="false"/>
                            <a:da name="INVALID" type="Multi">
                              <mt:xpath 
                                        true="SoAdSocketUdpStrictHeaderLenCheckEnabled shall only be set to                                            TRUE if SoAdPduHeaderEnable is also set to TRUE" 
                                        expr="(.=&apos;true&apos;) and (../../SoAdPduHeaderEnable=&apos;false&apos;)"/>
                            </a:da>
                          </v:var>
                          <v:var name="SoAdSocketUdpTriggerTimeout" 
                                 type="FLOAT">
                            <a:a name="DESC">
                              <a:v>&lt;html&gt;
                                    &lt;p&gt;Specifies the timeout in [s] a nPduUdpTxBuffer is
                                    waiting for a PDU with TriggerMode = TRIGGER_ALWAYS,
                                    i.e. when the timeout expires the nPduUdpTxBuffer is
                                    transmitted. Timer is reset after each UDP transmission.
                                    This optional parameter is only relevant if a
                                    nPduUdpTxBuffer is used. &lt;/p&gt;
                                  &lt;/html&gt;</a:v>
                            </a:a>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                 type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v class="PostBuild">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="OPTIONAL" value="true"/>
                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                            <a:a name="POSTBUILDVARIANTMULTIPLICITY" 
                                 value="true"/>
                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                            <a:a name="SCOPE" value="LOCAL"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID" 
                                 value="ECUC:ca90e8f9-cc0d-49e9-bebb-c1b79a3c0ef5"/>
                            <a:da name="DEFAULT" value="0.1"/>
                            <a:da name="ENABLE" value="false"/>
                            <a:da name="INVALID" type="Multi">
                              <mt:range>
                                <mt:tst expr="&lt;=1000000000.0"/>
                                <mt:tst expr="&gt;=0.0"/>
                              </mt:range>
                              <mt:xpath 
                                        true="SoAdSocketUdpTriggerTimeout shall not be smaller than SoAdMainFunctionPeriod." 
                                        expr="((.) &lt; (as:modconf(&apos;SoAd&apos;)[1]/SoAdGeneral/SoAdMainFunctionPeriod))"/>
                            </a:da>
                            <a:da name="WARNING" type="XPath" 
                                  expr="(num:i(. * 1000000) mod num:i(as:modconf(&apos;SoAd&apos;)[1]/SoAdGeneral/SoAdMainFunctionPeriod * 1000000)) = 0" 
                                  false="SoAdSocketUdpTriggerTimeout should be a decimal multiple of SoAdMainFunctionPeriod. If this is not the case the timeout value will be rounded down."/>
                          </v:var>
                          <v:var name="SoAdSocketnPduUdpTxBufferMin" 
                                 type="INTEGER">
                            <a:a name="DESC">
                              <a:v>&lt;html&gt;
                                    &lt;p&gt;Specifies the amount of data in bytes (PDU data
                                    provided by the upper layer and PDU Header if used) the
                                    SoAd shall be able to buffer for data transmission via
                                    this socket connection in case the UDP message shall be
                                    buffered for transmission of multiple PDUs per UDP.
                                    Note: in case of a UDP socket and an  upper layer with
                                    TP API or an upper layer with IF API with UDP transmit
                                    retry (for single PDUs) configured, the required buffer
                                    size can be determined automatically. This optional
                                    parameter is only relevant if a nPduUdpTxBuffer is
                                    used.&lt;/p&gt;
                                  &lt;/html&gt;</a:v>
                            </a:a>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                 type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v class="PostBuild">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="OPTIONAL" value="true"/>
                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                            <a:a name="POSTBUILDVARIANTMULTIPLICITY" 
                                 value="true"/>
                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                            <a:a name="SCOPE" value="LOCAL"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID" 
                                 value="ECUC:35773a86-d1bf-4d8b-82a4-5120276e4d33"/>
                            <a:da name="DEFAULT" value="0"/>
                            <a:da name="ENABLE" value="false"/>
                            <a:da name="INVALID" type="Multi">
                              <mt:range>
                                <mt:tst expr="&lt;=65535"/>
                                <mt:tst expr="&gt;=0"/>
                              </mt:range>
                              <mt:xpath 
                                        true="If SoAdSocketnPduUdpTxBufferMin is enabled, SoAdPduHeaderEnable must be enabled too." 
                                        expr="(../../SoAdPduHeaderEnable = &apos;false&apos;)"/>
                            </a:da>
                          </v:var>
                          <v:var name="SoAdSocketnPduUdpCapacity" 
                                 type="INTEGER">
                            <a:a name="DESC">
                              <a:v>&lt;html&gt;
                                    &lt;p&gt;
                                    This parameter defines the transmission threshold in bytes
                                    for SoAdSocketnPduUdpTxBufferMin. As soon as the buffer gets
                                    filled with PDUs and reaches the threshold, the buffer will be
                                    transmitted. PDUs can only be stored if enough buffer is
                                    available. Using the threshold it is possible to send the buffer
                                    before it is full. This way PDUs interrupting the buffer
                                    transmission can be stored in the remaining buffer.
                                    
                                    Feature is not supported.
                                    &lt;/p&gt;
                                  &lt;/html&gt;</a:v>
                            </a:a>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                 type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v class="PostBuild">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="OPTIONAL" value="true"/>
                            <a:a name="POSTBUILDVARIANTMULTIPLICITY" 
                                 value="true"/>
                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                            <a:a name="SCOPE" value="LOCAL"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:da name="DEFAULT" value="0"/>
                            <a:da name="EDITABLE" value="false"/>
                            <a:da name="ENABLE" value="false"/>
                            <a:da name="INVALID" type="Multi">
                              <mt:range>
                                <mt:tst expr="&lt;=65535"/>
                                <mt:tst expr="&gt;=0"/>
                              </mt:range>
                              <mt:xpath 
                                        false="SoAdSocketnPduUdpTxBufferMin needs to be enabled and greater than SoAdSocketnPduUdpCapacity." 
                                        expr="node:exists(./../SoAdSocketnPduUdpTxBufferMin) and (num:i(.) &lt; num:i(./../SoAdSocketnPduUdpTxBufferMin))"/>
                            </a:da>
                          </v:var>
                        </v:ctr>
                        <v:ctr name="SoAdSocketTcp" type="IDENTIFIABLE">
                          <a:a name="DESC">
                            <a:v>&lt;html&gt;
                                  Specifies that TCP is used as transport protocol for the
                                  socket connection group and parameters only related to TCP
                                  socket connections
                                &lt;/html&gt;</a:v>
                          </a:a>
                          <a:a name="UUID" 
                               value="ECUC:2fd748e9-1c58-4419-a869-f1583dab6a51"/>
                          <v:var name="SoAdSocketTcpImmediateTpTxConfirmation" 
                                 type="BOOLEAN">
                            <a:a name="DESC">
                              <a:v>&lt;html&gt;
                                    If set to FALSE, SoAd notifies the TP upper layer via transmit
                                    confirmation after a Tcp Ack has been received. If set to TRUE,
                                    SoAd notifies the TP upper layer via transmit confirmation
                                    immediately after transmit has been accepted by TcpIp.
                                  &lt;/html&gt;</a:v>
                            </a:a>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                 type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                            <a:a name="SCOPE" value="ECU"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID" 
                                 value="ECUC:c6ad7d55-2e5d-49c0-9f7a-e42599a33a50"/>
                            <a:da name="DEFAULT" value="false"/>
                          </v:var>
                          <v:var name="SoAdSocketTcpInitiate" type="BOOLEAN">
                            <a:a name="DESC">
                              <a:v>&lt;html&gt;
                                    &lt;p&gt;
                                      Specifies the initiator for this TCP connection.
                                    &lt;/p&gt;
                                    &lt;ul&gt;
                                      &lt;li&gt;true: This TCP connection is initiated by this module.&lt;/li&gt;
                                      &lt;li&gt;false: This TCP connection is to be initiated in the
                                        listen mode.&lt;/li&gt;
                                    &lt;/ul&gt;
                                  &lt;/html&gt;</a:v>
                            </a:a>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                 type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                            <a:a name="SCOPE" value="LOCAL"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID" 
                                 value="ECUC:544ba1ab-6419-42e8-99dc-635daba8c15f"/>
                            <a:da name="DEFAULT" value="true"/>
                            <a:da name="INVALID" type="Multi">
                              <mt:xpath>
                                <mt:tst 
                                        true="TcpInitiate must not be set to TRUE if SoAdSocketAutomaticSoConSetup is set to TRUE and the remote addresses contains wildcards!" 
                                        expr="(.=&apos;true&apos;) and (../../SoAdSocketAutomaticSoConSetup=&apos;true&apos;) and (node:containsValue(../../SoAdSocketConnection/*/SoAdSocketRemoteAddress/SoAdSocketRemoteIpAddress,&apos;ANY&apos;)                                           or node:containsValue(../../SoAdSocketConnection/*/SoAdSocketRemoteAddress/SoAdSocketRemoteIpAddress,&apos;0.0.0.0&apos;)                                           or (../../SoAdSocketConnection/*/SoAdSocketRemoteAddress/SoAdSocketRemotePort = 0))"/>
                                <mt:tst 
                                        true="TcpInitiate must not be set for SoAdSocketConnectionGrops with multiple SoAdSocketConnections." 
                                        expr="(.=&apos;true&apos;) and (count(../../SoAdSocketConnection/*) &gt; 1)"/>
                              </mt:xpath>
                            </a:da>
                          </v:var>
                          <v:var name="SoAdSocketTcpKeepAlive" type="BOOLEAN">
                            <a:a name="DESC">
                              <a:v>&lt;html&gt;
                                    &lt;p&gt;Specifies to use the keep-alive mechanism for this
                                    connection. It will not be defined for UDP sockets.&lt;/p&gt;
                                  &lt;/html&gt;</a:v>
                            </a:a>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                 type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                            <a:a name="SCOPE" value="LOCAL"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID" 
                                 value="ECUC:225b680b-49a8-4dd9-ad4c-51e9d5fa70af"/>
                            <a:da name="DEFAULT" value="false"/>
                            <a:da name="INVALID" type="Multi">
                              <mt:xpath 
                                        true="SoAdSocketTcpKeepAlive shall only be enabled if                                         TcpIpTcpKeepAliveEnabled is also enabled" 
                                        expr="(.=&apos;true&apos;) and (as:modconf(&apos;TcpIp&apos;)/TcpIpConfig/TcpIpConfig_0/TcpIpTcpConfig/TcpIpTcpConfig_0/TcpIpTcpKeepAliveEnabled)=&apos;false&apos;"/>
                            </a:da>
                          </v:var>
                          <v:var name="SoAdSocketTcpKeepAliveInterval" 
                                 type="FLOAT">
                            <a:a name="DESC">
                              <a:v>&lt;html&gt;
                                      &lt;p&gt;Specifies the interval in seconds between subsequent
                                      keepalive probes&lt;/p&gt;
                                    &lt;/html&gt;</a:v>
                            </a:a>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                 type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v class="PostBuild">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="OPTIONAL" value="true"/>
                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                            <a:a name="POSTBUILDVARIANTMULTIPLICITY" 
                                 value="true"/>
                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                            <a:a name="SCOPE" value="LOCAL"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID" 
                                 value="ECUC:55d0475c-c4a4-4185-b9fc-2d7bbc7d1704"/>
                            <a:da name="ENABLE" value="false"/>
                            <a:da name="INVALID" type="Multi">
                              <mt:range>
                                <mt:tst expr="&lt;=1000000000.0"/>
                                <mt:tst expr="&gt;=0.00001"/>
                              </mt:range>
                              <mt:xpath 
                                        true="SoAdSocketTcpKeepAliveInterval shall only be enabled if                                               SoAdSocketTcpKeepAlive is also enabled" 
                                        expr="(../SoAdSocketTcpKeepAlive=&apos;false&apos;)"/>
                            </a:da>
                          </v:var>
                          <v:var name="SoAdSocketTcpKeepAliveProbesMax" 
                                 type="INTEGER">
                            <a:a name="DESC">
                              <a:v>&lt;html&gt;
                                      &lt;p&gt;Maximum number of times that TCP retransmits an individual
                                      data segment before aborting the connection.&lt;/p&gt;
                                    &lt;/html&gt;</a:v>
                            </a:a>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                 type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v class="PostBuild">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="OPTIONAL" value="true"/>
                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                            <a:a name="POSTBUILDVARIANTMULTIPLICITY" 
                                 value="true"/>
                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                            <a:a name="SCOPE" value="LOCAL"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID" 
                                 value="ECUC:52cde23e-faae-496b-9318-b75317681adf"/>
                            <a:da name="ENABLE" value="false"/>
                            <a:da name="INVALID" type="Multi">
                              <mt:range>
                                <mt:tst expr="&lt;=255"/>
                                <mt:tst expr="&gt;=1"/>
                              </mt:range>
                              <mt:xpath 
                                        true="SoAdSocketTcpKeepAliveProbesMax shall only be enabled if                                               SoAdSocketTcpKeepAlive is also enabled" 
                                        expr="(../SoAdSocketTcpKeepAlive=&apos;false&apos;)"/>
                            </a:da>
                          </v:var>
                          <v:var name="SoAdSocketTcpKeepAliveTime" type="FLOAT">
                            <a:a name="DESC">
                              <a:v>&lt;html&gt;
                                      &lt;p&gt;Specifies the time in seconds between the last data packet
                                      sent and the first keepalive probe.&lt;/p&gt;
                                    &lt;/html&gt;</a:v>
                            </a:a>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                 type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v class="PostBuild">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="OPTIONAL" value="true"/>
                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                            <a:a name="POSTBUILDVARIANTMULTIPLICITY" 
                                 value="true"/>
                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                            <a:a name="SCOPE" value="LOCAL"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID" 
                                 value="ECUC:3a0fbe14-e493-43af-b169-7ee4fa002459"/>
                            <a:da name="DEFAULT" value="7200.0"/>
                            <a:da name="ENABLE" value="false"/>
                            <a:da name="INVALID" type="Multi">
                              <mt:range>
                                <mt:tst expr="&lt;=1000000000.0"/>
                                <mt:tst expr="&gt;=0.00001"/>
                              </mt:range>
                              <mt:xpath 
                                        true="SoAdSocketTcpKeepAliveTime shall only be enabled if                                               SoAdSocketTcpKeepAlive is also enabled" 
                                        expr="(../SoAdSocketTcpKeepAlive=&apos;false&apos;)"/>
                            </a:da>
                          </v:var>
                          <v:var name="SoAdSocketTcpNoDelay" type="BOOLEAN">
                            <a:a name="DESC">
                              <a:v>&lt;html&gt;
                                    &lt;p&gt;
                                      Specifies not to use the congestion control mechanism
                                      for this connection. It will not be defined for UDP sockets.
                                    &lt;/p&gt;
                                    &lt;ul&gt;
                                      &lt;li&gt;true: This TCP connection will NOT use congestion
                                        control.&lt;/li&gt;
                                      &lt;li&gt;false: This TCP connection will use congestion
                                      control.&lt;/li&gt;
                                    &lt;/ul&gt;
                                    &lt;p&gt;
                                      If the optional parameter is not enabled, the default behavior
                                      configured for TcpIp via the parameter TcpIpTcpNagleEnabled is
                                      applied. Note: This parameter must not be set to FALSE if
                                      TcpIpTcpNagleEnabled is set to FALSE.
                                    &lt;/p&gt;
                                  &lt;/html&gt;</a:v>
                            </a:a>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                 type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v class="PostBuild">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="OPTIONAL" value="true"/>
                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                            <a:a name="POSTBUILDVARIANTMULTIPLICITY" 
                                 value="true"/>
                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                            <a:a name="SCOPE" value="LOCAL"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID" 
                                 value="ECUC:babda4bd-27c8-4502-ad52-3b21483c2bb6"/>
                            <a:da name="DEFAULT" value="true"/>
                            <a:da name="ENABLE" value="false"/>
                          </v:var>
                          <v:var name="SoAdSocketTcpTxQuota" type="INTEGER">
                            <a:a name="DESC">
                              <a:v>&lt;html&gt;
                                    &lt;p&gt;Specifies the maximum amount of bytes (PDU data provided by the
                                    upper layer and PDU Header if used) the SoAd may queue for
                                    transmission via TCP at the TcpIp module for each socket connection
                                    of this socket connection group.&lt;/p&gt;
                                    &lt;p&gt;Rationale: prohibits that a socket connection consumes all
                                    available transmit buffers at the TcpIp and blocks transmissions
                                    via other socket connections.&lt;/p&gt;
                                    &lt;p&gt;If the optional parameter is not enabled, the amout of data is
                                    not limited.&lt;/p&gt;
                                  &lt;/html&gt;</a:v>
                            </a:a>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                 type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v class="PostBuild">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="OPTIONAL" value="true"/>
                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                            <a:a name="POSTBUILDVARIANTMULTIPLICITY" 
                                 value="true"/>
                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                            <a:a name="SCOPE" value="LOCAL"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID" 
                                 value="ECUC:a8df0686-4cc6-4284-987d-e8e01d20d77f"/>
                            <a:da name="DEFAULT" value="0"/>
                            <a:da name="ENABLE" value="false"/>
                            <a:da name="INVALID" type="Multi">
                              <mt:range>
                                <mt:tst expr="&lt;=4294967295"/>
                                <mt:tst expr="&gt;=0"/>
                              </mt:range>
                            </a:da>
                          </v:var>
                        </v:ctr>
                      </v:chc>
                    </v:ctr>
                  </v:lst>
                  <v:lst name="SoAdSocketRoute" type="MAP">
                    <a:da name="INVALID" type="Multi">
                      <mt:xpath 
                                false="SoAdRxPduIds of all SoAdSocketRouteDests must be zero-based and dense!" 
                                expr="(count(*/SoAdSocketRouteDest/*/SoAdRxPduId) = 0) or (count(*/SoAdSocketRouteDest/*/SoAdRxPduId) = (num:max(*/SoAdSocketRouteDest/*/SoAdRxPduId)+1))"/>
                    </a:da>
                    <a:da name="MIN" value="0"/>
                    <v:ctr name="SoAdSocketRoute" type="IDENTIFIABLE">
                      <a:a name="DESC">
                        <a:v>&lt;html&gt;
                              Describes the path of a PDU from a socket in the TCP/IP
                              stack to an upper layer of the SoAd after reception in the TCP/IP Stack.
                            &lt;/html&gt;</a:v>
                      </a:a>
                      <a:a name="POSTBUILDCHANGEABLE" value="true"/>
                      <a:a name="UUID" 
                           value="ECUC:6d408df2-0d97-411f-bede-b1b3f2335c97"/>
                      <v:var name="SoAdRxPduHeaderId" type="INTEGER">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                                ID contained in the packet received on the TCP/IP
                                connection if the PDU header option is enabled.
                              &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v class="PostBuild">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="OPTIONAL" value="true"/>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="POSTBUILDVARIANTMULTIPLICITY" value="true"/>
                        <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                        <a:a name="SCOPE" value="LOCAL"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" 
                             value="ECUC:e71a64bd-f0cd-471d-96c8-2b92f5751301"/>
                        <a:da name="DEFAULT" value="0"/>
                        <a:da name="ENABLE" value="true"/>
                        <a:da name="INVALID" type="Multi">
                          <mt:range>
                            <mt:tst expr="&lt;4294967296"/>
                            <mt:tst expr="&gt;=0"/>
                          </mt:range>
                        </a:da>
                      </v:var>
                      <v:ref name="SoAdRxSocketConnOrSocketConnBundleRef" 
                             type="CHOICE-REFERENCE">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                                Choice Reference to a SocketConnection or to a 
                                SocketConnectionGroup on which the PDU was received. The reference 
                                to a SocketConnectionGroup shall only be used for upper layers 
                                with IF API or for both IF and TP in case that Meta data item 
                                SOCKET_CONNECTION_ID_16 exists for the referenced PDU.
                              &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                        <a:a name="SCOPE" value="LOCAL"/>
                        <a:a name="UUID" 
                             value="ECUC:f4f02f3d-d765-4a84-88ec-01a80b047fe0"/>
                        <a:da name="INVALID" type="Multi">
                          <mt:xpath>
                            <mt:tst 
                                    false="SoAdRxSocketConnOrSocketConnBundleRef needs to be configured to a valid reference!" 
                                    expr="node:refvalid(.)"/>
                            <mt:tst 
                                    true="The SoAdSocketRoute has multiple SoAdSocketRouteDestinations. Therefore the SoAdRxSocketConnOrSocketConnBundleRef shall refer to a SoAdSocketConnectionGroup." 
                                    expr="(count(../SoAdSocketRouteDest/*) &gt; 1) and (count(text:split(string(.),&apos;/&apos;))&gt; 5)"/>
                          </mt:xpath>
                        </a:da>
                        <a:da name="REF">
                          <a:v>ASPathDataOfSchema:/AUTOSAR/EcucDefs/SoAd/SoAdConfig/SoAdSocketConnectionGroup/SoAdSocketConnection</a:v>
                          <a:v>ASPathDataOfSchema:/AUTOSAR/EcucDefs/SoAd/SoAdConfig/SoAdSocketConnectionGroup</a:v>
                        </a:da>
                        <a:da name="WARNING" type="Multi">
                          <mt:xpath 
                                    true="Socket route linking to a TCP socket connection group is not supported." 
                                    expr="(count(text:split(string(.),&apos;/&apos;))= 5) and (node:ref(.)/SoAdSocketProtocol = &apos;SoAdSocketTcp&apos;)"/>
                        </a:da>
                      </v:ref>
                      <v:lst name="SoAdSocketRouteDest" type="MAP">
                        <a:da name="MIN" value="1"/>
                        <v:ctr name="SoAdSocketRouteDest" type="IDENTIFIABLE">
                          <a:a name="DESC">
                            <a:v>&lt;html&gt;
                                  Describes the upper layer destination PDU for a message
                                  received on a TcpIp socket.
                                &lt;/html&gt;</a:v>
                          </a:a>
                          <a:a name="UUID" 
                               value="ECUC:c9313315-2428-474e-bf2b-fa9f9491ab64"/>
                          <a:da name="INVALID" type="Multi">
                            <mt:xpath>
                              <mt:tst 
                                      true="A SoAdSocketRoute using SoAdRxUpperLayerType = TP and SoAdRxSocketConnOrSocketConnBundleRef referring to a socket connection group is only allowed if the PDU referenced by SoAdRxPduRef has a MetaDataTypeRef configured with MetaDataItem of type SOCKET_CONNECTION_ID_16." 
                                      expr="node:refvalid(../../SoAdRxSocketConnOrSocketConnBundleRef) and (./SoAdRxUpperLayerType = &apos;TP&apos;) and node:refvalid(./SoAdRxPduRef) and (count(text:split(string(../../SoAdRxSocketConnOrSocketConnBundleRef),&apos;/&apos;)) = 5) and not(node:refvalid(node:ref(./SoAdRxPduRef)/MetaDataTypeRef))"/>
                              <mt:tst 
                                      true="The PDU referenced by SoAdRxPduRef has a MetaDataTypeRef configured but no MetaDataItem of type SOCKET_CONNECTION_ID_16. This is required if SoAdRxUpperLayerType = TP and a SoAdSocketRouteDest has SoAdRxSocketConnOrSocketConnBundleRef configured to a socket connection group." 
                                      expr="node:refvalid(../../SoAdRxSocketConnOrSocketConnBundleRef) and (./SoAdRxUpperLayerType = &apos;TP&apos;) and node:refvalid(./SoAdRxPduRef) and (count(text:split(string(../../SoAdRxSocketConnOrSocketConnBundleRef),&apos;/&apos;)) = 5) and (node:refvalid(node:ref(./SoAdRxPduRef)/MetaDataTypeRef))                                       and (count(node:ref(node:ref(./SoAdRxPduRef)/MetaDataTypeRef)/MetaDataItem/*[./MetaDataItemType = &apos;SOCKET_CONNECTION_ID_16&apos;]) &lt; 1)"/>
                            </mt:xpath>
                          </a:da>
                          <v:ref name="SoAdRxPduRef" type="REFERENCE">
                            <a:a name="DESC">
                              <a:v>&lt;html&gt;
                                    Reference to the global PDU structure.
                                  &lt;/html&gt;</a:v>
                            </a:a>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                 type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                            <a:a name="SCOPE" value="ECU"/>
                            <a:a name="UUID" 
                                 value="ECUC:7f29b096-bb12-495e-9d74-21c032008ada"/>
                            <a:da name="INVALID" type="Multi">
                              <mt:xpath>
                                <mt:tst 
                                        false="SoAdRxPduRef needs to be configured to a valid reference!" 
                                        expr="node:refvalid(.)"/>
                                <mt:tst 
                                        true="Each global PDU must not be referenced by more than one SoAdSocketRouteDest with SoAdRxUpperLayerType = &apos;IF&apos; unless this is specifically allowed by setting SoAdGeneral/VendorSpecific/SoAdSocketRouteDestFanInEnable to TRUE." 
                                        expr="(as:modconf(&apos;SoAd&apos;)[1]/SoAdGeneral/VendorSpecific/SoAdSocketRouteDestFanInEnable = &apos;false&apos;) and count(../../../../*/SoAdSocketRouteDest/*[SoAdRxUpperLayerType = &apos;IF&apos;]/../*[SoAdRxPduRef = node:current()]) &gt; 1"/>
                                <mt:tst 
                                        true="The referenced PDU shall not be used by other SoAdSocketRouteDest if SoAdRxUpperLayerType = &apos;TP&apos; is used." 
                                        expr="((../SoAdRxUpperLayerType = &apos;TP&apos;) and (count(../../../../*/SoAdSocketRouteDest/*/../*[SoAdRxPduRef = node:current()]) &gt; 1))"/>
                              </mt:xpath>
                            </a:da>
                            <a:da name="REF" 
                                  value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/EcuC/EcucPduCollection/Pdu"/>
                          </v:ref>
                          <v:var name="SoAdRxPduId" type="INTEGER">
                            <a:a name="DESC">
                              <a:v>&lt;html&gt;
                                    This unique identifier is used for a receive
                                    cancellation request from an upper layer of the SoAd.
                                  &lt;/html&gt;</a:v>
                            </a:a>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                 type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v class="PostBuild">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="OPTIONAL" value="true"/>
                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                            <a:a name="POSTBUILDVARIANTMULTIPLICITY" 
                                 value="true"/>
                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                            <a:a name="SCOPE" value="ECU"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID" 
                                 value="ECUC:a420c5fd-18c3-4bfb-96e8-9f12c650aef8"/>
                            <a:da name="DEFAULT" value="0"/>
                            <a:da name="ENABLE" value="false"/>
                            <a:da name="INVALID" type="Multi">
                              <mt:range>
                                <mt:tst expr="&lt;=65535"/>
                                <mt:tst expr="&gt;=0"/>
                              </mt:range>
                              <mt:xpath>
                                <mt:tst 
                                        false="PDU receive cancelation is only supported for SoAdRxUpperLayerType = &apos;TP&apos;." 
                                        expr="(../SoAdRxUpperLayerType) = &apos;TP&apos;"/>
                                <mt:tst 
                                        false="SoAdRxPduIds of all SoAdSocketRouteDests must be unique." 
                                        expr="count(../../../../../SoAdSocketRoute/*/SoAdSocketRouteDest/*[SoAdRxPduId=node:current()]) = 1"/>
                              </mt:xpath>
                            </a:da>
                          </v:var>
                          <v:var name="SoAdRxUpperLayerType" type="ENUMERATION">
                            <a:a name="DESC">
                              <a:v>&lt;html&gt;
                                    Specifies the upper layer interface type
                                    (must be &quot;If&quot; in case of multiple RxPdus).
                                  &lt;/html&gt;</a:v>
                            </a:a>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                 type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            </a:a>
                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                            <a:a name="SCOPE" value="ECU"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID" 
                                 value="ECUC:1ad3be2c-e898-4c78-a3e5-b9c54698e2ed"/>
                            <a:da name="DEFAULT" value="IF"/>
                            <a:da name="INVALID" type="Multi">
                              <mt:xpath 
                                        true="For SoAdSocketRoute with multiple SoAdSocketRouteDest all SoAdRxUpperLayerType shall be IF." 
                                        expr="(count(../../*) &gt; 1) and ((.) != &apos;IF&apos;)"/>
                            </a:da>
                            <a:da name="RANGE">
                              <a:v>IF</a:v>
                              <a:v>TP</a:v>
                            </a:da>
                          </v:var>
                          <v:lst name="SoAdRxRoutingGroupRef">
                            <a:da name="INVALID" type="Multi">
                              <mt:xpath 
                                        true="The SoAdSocketRoute has multiple SoAdSocketRouteDest, each SoAdSocketRouteDest shall have at least one SoAdRxRoutingGroupRef entry." 
                                        expr="(count(../../*) &gt; 1) and (count(./*) = 0)"/>
                            </a:da>
                            <v:ref name="SoAdRxRoutingGroupRef" 
                                   type="REFERENCE">
                              <a:a name="DESC">
                                <a:v>&lt;html&gt;
                                      Reference to the routing group.
                                    &lt;/html&gt;</a:v>
                              </a:a>
                              <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                   type="IMPLEMENTATIONCONFIGCLASS">
                                <icc:v class="PostBuild">VariantPostBuild</icc:v>
                              </a:a>
                              <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                              <a:a name="POSTBUILDVARIANTMULTIPLICITY" 
                                   value="true"/>
                              <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                              <a:a name="SCOPE" value="LOCAL"/>
                              <a:a name="UUID" 
                                   value="ECUC:8a4e0666-fa03-4bb5-99fd-10c2bf83e9ed"/>
                              <a:da name="INVALID" type="Multi">
                                <mt:xpath>
                                  <mt:tst 
                                          false="SoAdRxRoutingGroupRef needs to be configured to a valid reference!" 
                                          expr="node:refvalid(.)"/>
                                  <mt:tst 
                                          true="The SoAdSocketRoute has multiple SoAdSocketRouteDest and this SoAdRxRoutingGroupRef is already used by another SoAdSocketRouteDest of this SoAdSocketRoute. The same SoAdRoutingGroup shall not be referenced by multiple SoAdSocketRouteDest of the same SoAdSocketRoute." 
                                          expr="(count(../../../*) &gt; 1) and count(../../../../SoAdSocketRouteDest/*/SoAdRxRoutingGroupRef/*[.=node:current()]) &gt; 1"/>
                                  <mt:tst 
                                          true="SoAdSocketRoute has multiple SoAdSocketRouteDest. SoAdRoutingGroupIsEnabledAtInit of referenced SoAdRoutingGroup shall be disabled." 
                                          expr="(count(../../../*) &gt; 1) and (node:value(node:ref(.)/SoAdRoutingGroupIsEnabledAtInit) = &apos;true&apos;)"/>
                                  <mt:tst 
                                          true="SoAdRoutingGroup refers to SoAdSocketRoute with multiple SoAdSocketRouteDest. SoAdRoutingGroup must be specific which means that the SoAdSocketRoute and SoAdPduRoute belonging to this SoAdRoutingGroup can only reference exactly one SoAdSocketConnectionGroup." 
                                          expr="(count(../../../*) &gt; 1) and (asc_soad:isSpecificRG(node:value(.)) = &apos;false&apos;)"/>
                                </mt:xpath>
                              </a:da>
                              <a:da name="REF" 
                                    value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/SoAd/SoAdConfig/SoAdRoutingGroup"/>
                            </v:ref>
                          </v:lst>
                        </v:ctr>
                      </v:lst>
                    </v:ctr>
                  </v:lst>
                  <v:lst name="SoAdRoutingGroup" type="MAP">
                    <a:da name="INVALID" type="Multi">
                      <mt:xpath 
                                false="SoAdRoutingGroupIds of all SoAdRoutingGroups must be zero-based and dense!" 
                                expr="(count(*) = 0) or (count(*) = (num:max(*/SoAdRoutingGroupId)+1))"/>
                    </a:da>
                    <a:da name="MIN" value="0"/>
                    <v:ctr name="SoAdRoutingGroup" type="IDENTIFIABLE">
                      <a:a name="DESC">
                        <a:v>&lt;html&gt;
                              Specifies a routing group which can be activated or
                              deactivated for PDU routing. A routing group consists
                              of PDUs. Routing of PDUs can either be forwarding of PDUs
                              from the upper layer to a TCP or UDP socket of the TCP/IP
                              stack specified by a SoAdPduRoute or the other way around
                              specified by a SoAdSocketRoute.
                            &lt;/html&gt;</a:v>
                      </a:a>
                      <a:a name="POSTBUILDCHANGEABLE" value="true"/>
                      <a:a name="UUID" 
                           value="ECUC:fc04d0a2-e032-46ec-b1f6-1951663947c7"/>
                      <v:var name="SoAdRoutingGroupId" type="INTEGER">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                                Unique ID of Routing Group.
                              &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="SCOPE" value="ECU"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="true"/>
                        <a:a name="UUID" 
                             value="ECUC:c01186c2-1b38-46d0-a36a-cca2e2dc98e3"/>
                        <a:da name="DEFAULT" value="0"/>
                        <a:da name="INVALID" type="Multi">
                          <mt:range>
                            <mt:tst expr="&lt;=65535"/>
                            <mt:tst expr="&gt;=0"/>
                          </mt:range>
                          <mt:xpath>
                            <mt:tst 
                                    false="SoAdRoutingGroupIds of all SoAdRoutingGroups must be unique." 
                                    expr="count(../../*[SoAdRoutingGroupId=node:current()]) = 1"/>
                            <mt:tst 
                                    true="SoAdRoutingGroups that are triggerable (i.e. configured with SoAdRoutingGroupTxTriggerable = &apos;true&apos;) must be listed before other SoAdRoutingGroups" 
                                    expr="(../SoAdRoutingGroupTxTriggerable = &apos;true&apos;) and count(../../*[SoAdRoutingGroupTxTriggerable = &apos;false&apos; and SoAdRoutingGroupId &lt; node:current()]) &gt; 0"/>
                          </mt:xpath>
                        </a:da>
                      </v:var>
                      <v:var name="SoAdRoutingGroupIsEnabledAtInit" 
                             type="BOOLEAN">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                                If set to true this routing group will be enabled after
                                initializing the SoAd module (i.e. enabled in the SoAd_Init
                                function).
                              &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                        <a:a name="SCOPE" value="LOCAL"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" 
                             value="ECUC:fb184258-ebc6-43ad-b6df-4bac46074bc2"/>
                        <a:da name="DEFAULT" value="true"/>
                      </v:var>
                      <v:var name="SoAdRoutingGroupTxTriggerable" 
                             type="BOOLEAN">
                        <a:a name="DESC">
                          <a:v>&lt;html&gt;
                                Specifies if the If-TxPDUs related to the PduRouteDest containers
                              &lt;/html&gt;</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="SCOPE" value="LOCAL"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" 
                             value="ECUC:aaa3d81d-9664-4c66-b322-d06217a9a89d"/>
                        <a:da name="DEFAULT" value="false"/>
                      </v:var>
                    </v:ctr>
                  </v:lst>
                </v:ctr>
              </v:lst>
            </v:ctr>
          </d:chc>
        </d:lst>
      </d:ctr>
    </d:lst>
  </d:ctr>

</datamodel>
